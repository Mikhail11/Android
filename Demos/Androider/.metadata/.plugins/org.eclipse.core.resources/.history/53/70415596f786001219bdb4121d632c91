public class WidgetAdapter extends ArrayAdapter {
	Context context;
	int layoutResourceId;
	Widget aWidget[];

	public WidgetAdapter(Context context, int textViewResourceId,
			Widget[] aWidget) {
		super(context, textViewResourceId, aWidget);

	}

}
