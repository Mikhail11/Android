package vn.hanoidev.entity;

public class Widget {
	private String name;
	private int icon;

	public Widget() {
		this.name = "";
	}

	public Widget(String sName) {
		this.name = sName;
	}

	public String getName() {
		return name;
	}

	public void setName(String sName) {
		this.name = sName;
	}

	public int getIcon() {
		return icon;
	}

	public void setIcon(int icon) {
		this.icon = icon;
	}
}
