-- --------------------------------------------------------

--
-- Table structure for table `pages`
--

CREATE TABLE IF NOT EXISTS `pages` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `theme_id` int(4) DEFAULT NULL COMMENT 'Mã gói chức năng của page',
  `layout` varchar(50) COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'Layout cua page',
  `name` varchar(45) COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'Ten trang',
  `title` varchar(70) COLLATE utf8_unicode_ci DEFAULT NULL,
  `description` varchar(155) COLLATE utf8_unicode_ci DEFAULT NULL,
  `type` tinyint(1) DEFAULT NULL,
  PRIMARY KEY (`id`),
  KEY `theme_id` (`theme_id`)
) ENGINE=MyISAM  DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci AUTO_INCREMENT=1336452813 ;

-- --------------------------------------------------------

--
-- Table structure for table `payment_merchant`
--

CREATE TABLE IF NOT EXISTS `payment_merchant` (
  `id` int(11) NOT NULL AUTO_INCREMENT COMMENT 'ID tự tăng',
  `cdt_id` int(11) NOT NULL COMMENT ' ID của shop/gian hàng hoặc ID do chợ điện tử quy định với các loại merchant',
  `merchant_id` int(11) NOT NULL COMMENT ' ID merchant tại NL, gọi webservice lần đầu để tạo ra',
  `merchant_pass` varchar(200) COLLATE utf8_unicode_ci NOT NULL COMMENT 'Pass merchant',
  `flat_fee` float NOT NULL COMMENT 'Phí cố định',
  `percentage_fee` float NOT NULL COMMENT 'Phí %',
  `fee_id` int(4) NOT NULL COMMENT ' ID của cái record phí bên NL dùng để đối chiếu & update',
  `time_begin` int(11) NOT NULL COMMENT 'Bắt đầu áp dụng',
  `time_end` int(11) NOT NULL COMMENT 'Kết thúc áp dụng',
  `status` tinyint(1) NOT NULL COMMENT 'Trạng thái duyệt',
  `domain` varchar(100) COLLATE utf8_unicode_ci NOT NULL COMMENT 'Tên gian hàng hoặc dịch vụ',
  PRIMARY KEY (`id`)
) ENGINE=MyISAM  DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci COMMENT='Lưu chính sách giá các Merchant CĐT' AUTO_INCREMENT=538 ;

-- --------------------------------------------------------

--
-- Table structure for table `permission`
--

CREATE TABLE IF NOT EXISTS `permission` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `code` varchar(20) CHARACTER SET utf8 COLLATE utf8_unicode_ci NOT NULL COMMENT 'mã quyền , dùng để check quyền tương ứng , các giá trị : ADMIN,VIEW,ADD,EDIT.DELETE',
  `name` varchar(100) CHARACTER SET utf8 COLLATE utf8_unicode_ci NOT NULL COMMENT 'tên hiển thị của quyền ',
  `description` varchar(200) CHARACTER SET utf8 COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'Mô tả thêm về quyền',
  PRIMARY KEY (`id`)
) ENGINE=MyISAM  DEFAULT CHARSET=latin1 AUTO_INCREMENT=6 ;

-- --------------------------------------------------------

--
-- Table structure for table `privilege_level_function`
--

CREATE TABLE IF NOT EXISTS `privilege_level_function` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `level_code` varchar(100) CHARACTER SET utf8 COLLATE utf8_unicode_ci NOT NULL COMMENT 'mã của level của gian hàng',
  `permission_code` varchar(100) CHARACTER SET utf8 COLLATE utf8_unicode_ci NOT NULL COMMENT 'mã quyền, giá trị code của bảng permission',
  `privilege_code` varchar(100) CHARACTER SET utf8 COLLATE utf8_unicode_ci NOT NULL COMMENT 'giá trị trường code của bảng privilege',
  `function_id` int(11) NOT NULL COMMENT 'id của chức năng (giá trị id của bảng function)',
  `function_name` varchar(100) CHARACTER SET utf8 COLLATE utf8_unicode_ci NOT NULL COMMENT 'tên chức năng từ bảng function',
  `function_description` varchar(200) CHARACTER SET utf8 COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'mô tả chức năng,từ bảng function',
  `function_portal` varchar(100) CHARACTER SET utf8 COLLATE utf8_unicode_ci NOT NULL COMMENT 'portal của chức năng , giá trị portal từ bảng function',
  `function_page` varchar(100) CHARACTER SET utf8 COLLATE utf8_unicode_ci NOT NULL COMMENT 'page của function , giá trị page từ bảng function',
  `status` tinyint(1) NOT NULL DEFAULT '1' COMMENT 'tình trạng hoạt động của privilege : 1 : hoạt động , 0 : không hoạt động',
  PRIMARY KEY (`id`),
  KEY `level_code` (`level_code`)
) ENGINE=MyISAM  DEFAULT CHARSET=latin1 AUTO_INCREMENT=3266 ;

-- --------------------------------------------------------

--
-- Table structure for table `quotations`
--

CREATE TABLE IF NOT EXISTS `quotations` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `service_name` varchar(255) COLLATE utf8_unicode_ci NOT NULL COMMENT 'Noi dung thong tin bao gia dich vu gian hang',
  `service_group_id` tinyint(4) NOT NULL COMMENT 'Id cua group bao gia',
  `seller` varchar(255) COLLATE utf8_unicode_ci NOT NULL,
  `shop_basic` varchar(255) COLLATE utf8_unicode_ci NOT NULL,
  `store_silver` varchar(255) COLLATE utf8_unicode_ci NOT NULL,
  `store_gold` varchar(255) COLLATE utf8_unicode_ci NOT NULL,
  `store_platinum` varchar(255) COLLATE utf8_unicode_ci NOT NULL,
  PRIMARY KEY (`id`)
) ENGINE=MyISAM  DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci AUTO_INCREMENT=81 ;

-- --------------------------------------------------------

--
-- Table structure for table `settings_upgrade`
--

CREATE TABLE IF NOT EXISTS `settings_upgrade` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `module_id` int(11) NOT NULL,
  `status` int(1) NOT NULL,
  `name` varchar(50) COLLATE utf8_unicode_ci NOT NULL COMMENT 'tem cau hinh module',
  `label` varchar(100) COLLATE utf8_unicode_ci NOT NULL COMMENT 'nhan thuoc tinh',
  `option` varchar(255) COLLATE utf8_unicode_ci NOT NULL COMMENT 'luc chon',
  `value` varchar(255) COLLATE utf8_unicode_ci NOT NULL,
  `type` varchar(20) COLLATE utf8_unicode_ci NOT NULL,
  PRIMARY KEY (`id`)
) ENGINE=MyISAM  DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci AUTO_INCREMENT=2 ;

-- --------------------------------------------------------

--
-- Table structure for table `store_adv`
--

CREATE TABLE IF NOT EXISTS `store_adv` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `title` varchar(255) CHARACTER SET utf8 COLLATE utf8_unicode_ci NOT NULL,
  `brief` varchar(255) CHARACTER SET utf8 COLLATE utf8_unicode_ci NOT NULL,
  `store_id` int(11) NOT NULL DEFAULT '0',
  `store_name` varchar(255) CHARACTER SET utf8 COLLATE utf8_unicode_ci NOT NULL,
  `user_create` varchar(255) CHARACTER SET utf8 COLLATE utf8_unicode_ci DEFAULT NULL,
  `active` int(1) NOT NULL DEFAULT '0',
  `time` int(11) NOT NULL DEFAULT '0',
  `link` varchar(255) CHARACTER SET utf8 COLLATE utf8_unicode_ci NOT NULL,
  `src` text CHARACTER SET utf8 COLLATE utf8_unicode_ci,
  `type` int(11) NOT NULL COMMENT '0: ảnh dạng(jpg,png,gif) 1: dang flash; 2 video',
  `position` tinyint(2) NOT NULL,
  `position_order` int(11) NOT NULL DEFAULT '1' COMMENT 'thứ tự hiển thị vì trí quảng cáo',
  `time_display` int(11) DEFAULT NULL,
  `clicks` int(11) NOT NULL,
  `time_update` int(11) NOT NULL DEFAULT '0' COMMENT 'Thoi gian cap nhat lai quang cao',
  PRIMARY KEY (`id`),
  KEY `store_id` (`store_id`,`time_display`,`position`)
) ENGINE=MyISAM  DEFAULT CHARSET=latin1 AUTO_INCREMENT=15873 ;

