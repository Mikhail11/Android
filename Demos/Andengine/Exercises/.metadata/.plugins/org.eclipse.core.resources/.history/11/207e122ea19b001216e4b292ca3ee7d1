--
-- Table structure for table `order_user_info`
--

CREATE TABLE IF NOT EXISTS `order_user_info` (
  `id` bigint(20) NOT NULL AUTO_INCREMENT,
  `order_id` varchar(100) NOT NULL COMMENT ' Mã hóa đơn ',
  `type_user` tinyint(1) NOT NULL DEFAULT '1' COMMENT ' Kiểu người dùng. 1 Có tài khoản, 2 không có tài khoản ',
  `buyer_id` int(10) NOT NULL COMMENT 'Id của user',
  `buyer_username` varchar(200) DEFAULT NULL,
  `buyer_fullname` varchar(255) DEFAULT NULL COMMENT ' Họ và tên người mua ',
  `buyer_phone` varchar(100) DEFAULT NULL COMMENT ' Số điện thoại người mua ',
  `buyer_email` varchar(100) DEFAULT NULL COMMENT ' email người mua ',
  `buyer_address` varchar(255) DEFAULT NULL COMMENT ' Địa chỉ nhận hàng của người mua ',
  `buyer_zone` varchar(255) DEFAULT NULL COMMENT 'Tỉnh thành phố',
  `seller_feedback_score` float DEFAULT NULL COMMENT ' Điểm tích lũy người bán ',
  `seller_positive_feedback_percent` float DEFAULT NULL COMMENT ' Số phản hồi của người mua ',
  `seller_username` varchar(100) DEFAULT 'ebay',
  `seller_location` varchar(255) DEFAULT NULL COMMENT ' Vị trí người bán tại Mỹ',
  `info_receive` text COMMENT 'Thong tin nguoi nhan',
  `vat_infos` text COMMENT 'Thông tin xuat hoa don do',
  `has_vat` tinyint(1) NOT NULL DEFAULT '0' COMMENT '0- không vat; 1- có vat',
  PRIMARY KEY (`id`)
) ENGINE=MyISAM  DEFAULT CHARSET=latin1 AUTO_INCREMENT=28265 ;

