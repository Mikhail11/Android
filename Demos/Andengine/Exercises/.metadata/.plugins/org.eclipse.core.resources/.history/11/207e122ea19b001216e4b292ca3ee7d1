--
-- Table structure for table `blog_news`
--

CREATE TABLE IF NOT EXISTS `blog_news` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `category_id` int(11) NOT NULL COMMENT 'Danh mục tin',
  `alias_id` int(11) DEFAULT NULL COMMENT 'Định danh thuộc danh mục tin',
  `title` varchar(255) COLLATE utf8_unicode_ci NOT NULL COMMENT 'tiêu đề',
  `brief` text COLLATE utf8_unicode_ci COMMENT 'Mô tả',
  `image` varchar(255) COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'tên ảnh',
  `source` varchar(255) COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'nguồn tin/ tác giả',
  `link_source` varchar(255) COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'Link gốc của tin tức',
  `status` int(2) NOT NULL DEFAULT '1' COMMENT '0: không hiển thị; 1: Hiển thị trên trang',
  `content` longtext COLLATE utf8_unicode_ci COMMENT 'nội dung tiin',
  `time_created` int(11) NOT NULL COMMENT 'Thời gian tạo tin ',
  `time_update` int(11) NOT NULL,
  `user` varchar(250) COLLATE utf8_unicode_ci NOT NULL COMMENT 'id người tạo tin',
  `text_search` text COLLATE utf8_unicode_ci COMMENT 'chuỗi  thông tin tìm kiếm',
  `tag` varchar(255) COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'Từ khóa li',
  `total_comment` int(10) NOT NULL DEFAULT '0' COMMENT 'Tong so comment cua tin',
  `news_type` tinyint(1) NOT NULL,
  `view` int(11) NOT NULL,
  `file` varchar(255) COLLATE utf8_unicode_ci NOT NULL,
  PRIMARY KEY (`id`)
) ENGINE=MyISAM  DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci AUTO_INCREMENT=1590 ;

