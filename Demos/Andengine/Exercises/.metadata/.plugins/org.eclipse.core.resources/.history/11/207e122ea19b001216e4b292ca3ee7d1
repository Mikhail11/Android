--
-- Table structure for table `usvn_shipments`
--

CREATE TABLE IF NOT EXISTS `usvn_shipments` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `waybill_number` varchar(200) NOT NULL COMMENT 'Mã kiện hàng',
  `tracking_number` varchar(255) DEFAULT NULL,
  `weight` float DEFAULT NULL,
  `shipment_fee` float NOT NULL COMMENT 'Phí',
  `user_create` varchar(100) DEFAULT NULL,
  `date_create` int(11) DEFAULT NULL,
  `date_depature` int(11) NOT NULL COMMENT 'Ngày gửi',
  `date_arrival_estimated` int(11) NOT NULL COMMENT 'Ngày dự kiến đến',
  `date_arrival_actual` int(11) NOT NULL COMMENT 'Ngày đến thực tế',
  `location_status` tinyint(1) NOT NULL DEFAULT '1' COMMENT 'Vị trí 2: proxy, 3: Hải quan, 4: VP chợ điện tử',
  `activity_status` tinyint(1) NOT NULL DEFAULT '1' COMMENT 'Trạng thái  1: đang nằm ở, 2 : đang di chuyển',
  `carrier` varchar(255) DEFAULT NULL COMMENT 'Hãng vận chuyển: 1-DHL; 2-FedEx; 3-UPS; 4-EMS',
  `warehouse_begin` tinyint(1) DEFAULT NULL COMMENT 'Kho US: 1- Bongo; 2-Viaddress; 3-Vietlink',
  `warehouse_end` tinyint(1) DEFAULT NULL COMMENT 'Kho US: 1- Bongo; 2-Viaddress; 3-Vietlink',
  `location_store` varchar(255) DEFAULT NULL,
  `undertable_fee` float NOT NULL COMMENT 'Phí tiêu cực',
  `regular_fee` float NOT NULL COMMENT 'Lệ phí hải quan',
  `other_fee` float NOT NULL COMMENT 'Phí khác',
  `insurance_fee` float NOT NULL COMMENT 'Phí bảo hiểm',
  `shipping_detination` tinyint(1) NOT NULL DEFAULT '3' COMMENT 'Cừa khẩu 1: Hà nội 2: HCM, 3 không xác đinh',
  `custom_date` int(11) NOT NULL COMMENT 'Ngày làm thủ tục hải quan',
  `custom_exch_rate` float NOT NULL COMMENT 'Tỷ giá ngày làm thủ tục',
  `store_fee` float NOT NULL DEFAULT '0' COMMENT 'phi luu kho',
  `total_vat_hq` float DEFAULT NULL COMMENT 'Tổng thuế VAT HQ áp',
  `total_import_tax` float DEFAULT NULL COMMENT 'Tổng thuế nhập khẩu',
  `shipment_name` varchar(255) DEFAULT NULL COMMENT 'Tên kiện hàng',
  `order_ids` text COMMENT 'Danh sách id hóa đơn thuộc kiện hàng',
  PRIMARY KEY (`id`)
) ENGINE=MyISAM  DEFAULT CHARSET=latin1 AUTO_INCREMENT=212 ;
