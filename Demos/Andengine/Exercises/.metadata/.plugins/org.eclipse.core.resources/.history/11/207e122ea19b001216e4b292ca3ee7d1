  `id` int(11) NOT NULL AUTO_INCREMENT,
  `store_id` int(11) NOT NULL COMMENT 'id của gian hàng',
  `user_id` int(11) NOT NULL DEFAULT '0' COMMENT 'id của user',
  `user_name` varchar(50) COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'ten tai khoan',
  `user_fullname` varchar(200) COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'ten day du cua tai khoan',
  `user_email` varchar(200) COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'email cua tai khoan',
  `action_type` varchar(100) COLLATE utf8_unicode_ci NOT NULL COMMENT 'kiểu action mà user thực hiện',
  `action` varchar(300) COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'Nội dung hoạt động (tham gia cái gì hoặc được gửi email lúc nào)',
  `time` int(20) NOT NULL DEFAULT '0' COMMENT 'thời gian lưu hành động',
  PRIMARY KEY (`id`),
  KEY `store_id` (`store_id`)
) ENGINE=MyISAM  DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci AUTO_INCREMENT=324885 ;

-- --------------------------------------------------------

--
-- Table structure for table `store_customers`
--

CREATE TABLE IF NOT EXISTS `store_customers` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `store_id` int(11) NOT NULL DEFAULT '0' COMMENT 'id của gian hàng mà khách hàng thao tác',
  `user_id` int(11) DEFAULT '0' COMMENT 'id của user',
  `user_name` varchar(200) COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'tên đăng nhập user',
  `user_fullname` varchar(200) COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'Ho ten user  khach hang',
  `user_email` varchar(100) COLLATE utf8_unicode_ci NOT NULL DEFAULT '' COMMENT 'email cua user khach hang',
  `user_phone` varchar(12) COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'so dien thoai cua user',
  `last_action_type` varchar(100) COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'kiểu hành động thực hiện lần cuối',
  `last_action_description` text COLLATE utf8_unicode_ci COMMENT 'mô tả hành động cuối cùng',
  `last_action_time` int(4) DEFAULT '0' COMMENT 'thời gian thực hiện hành động cuối',
  `time_create` int(11) NOT NULL DEFAULT '0' COMMENT 'thời gian khởi tạo của khách hàng',
  `gender` tinyint(4) DEFAULT '0' COMMENT 'giới tính của user : 1 : Nam , 2 : Nữ',
  `id_group_customer` varchar(255) COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'id nhom khach hang  ngan cach "|"',
  `total_buy_item` int(11) DEFAULT '0' COMMENT 'tong so mua hang cua shop',
  `is_receive_mail` tinyint(4) DEFAULT '1' COMMENT 'dong y nhan mail hay khong (1 co; 0: khach hang khong nhan mail; 2 chu shop khong gui mail;)',
  `is_default` int(11) DEFAULT '1' COMMENT '1: nhom mac dinh shop; 0: chu shop tu them;2 da xoa',
  PRIMARY KEY (`id`),
  KEY `store_id` (`store_id`)
) ENGINE=MyISAM  DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci AUTO_INCREMENT=136220 ;

-- --------------------------------------------------------

--
-- Table structure for table `store_email_marketing`
--

CREATE TABLE IF NOT EXISTS `store_email_marketing` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `store_id` int(11) NOT NULL COMMENT 'id của gian hàng',
  `title` varchar(200) COLLATE utf8_unicode_ci NOT NULL COMMENT 'tiêu đề email',
  `content` longtext COLLATE utf8_unicode_ci NOT NULL COMMENT 'nội dung email',
  `last_update` int(20) NOT NULL DEFAULT '0' COMMENT 'lần cuối update',
  `user_gender` int(2) DEFAULT NULL COMMENT 'giới tính của danh sách user : NULL || 0 : không set , 1 : Nam , 2 : Nữ ',
  `last_time_start` int(20) DEFAULT NULL COMMENT 'thời điểm bắt đầu giới hạn lần cuối hoạt động của user',
  `last_time_end` int(20) DEFAULT NULL COMMENT 'thời điểm kết thúc lần cuối hoạt động',
  `time_create` int(11) NOT NULL DEFAULT '0' COMMENT 'thời điểm tạo mail',
  `action_type_in` varchar(100) COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'nhom khach hang nhan mail cach nhau dau ,',
  `list_user_name` varchar(500) COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'danh sách mail _name của khách hàng',
  `send_start_time` int(20) NOT NULL DEFAULT '0' COMMENT 'thời gian bắt đầu gửi',
  `total_email` int(10) NOT NULL DEFAULT '0' COMMENT 'tổng số email sẽ được gửi',
  `total_read` int(11) NOT NULL DEFAULT '0' COMMENT 'tổng số email đã được mở ra đọc',
  `total_click` int(11) NOT NULL DEFAULT '0' COMMENT 'tong so click ',
  `total_refuse` int(11) NOT NULL DEFAULT '0' COMMENT 'tong so tu choi cua mail',
  `status_mail` tinyint(4) NOT NULL DEFAULT '0' COMMENT '0: mới tạo; 1: da len lich; 2: dang gui; 3 da gui;4 khong duoc gui',
  `total_send_success` int(11) NOT NULL DEFAULT '0' COMMENT 'tong so mail gui thanh cong',
  `total_send_fail` int(11) NOT NULL DEFAULT '0' COMMENT 'tong so mail gui khong thanh cong',
  PRIMARY KEY (`id`),
  KEY `store_id` (`store_id`,`send_start_time`)
) ENGINE=MyISAM  DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci AUTO_INCREMENT=171 ;

-- --------------------------------------------------------

--
-- Table structure for table `store_email_queue`
--

CREATE TABLE IF NOT EXISTS `store_email_queue` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `email_marketing_id` int(11) NOT NULL COMMENT 'id của email bên bảng store_email_marketing',
  `user_id` int(11) NOT NULL COMMENT 'id của user',
  `user_email` varchar(200) COLLATE utf8_unicode_ci NOT NULL COMMENT 'email của khách hàng cần gửi',
  `user_fullname` varchar(200) COLLATE utf8_unicode_ci NOT NULL COMMENT 'tên khách hàng',
  `owner_email` varchar(200) COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'email chủ gian hàng',
  `owner_fullname` varchar(200) COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'họ tên chủ gian hàng',
  `send_status` tinyint(1) NOT NULL DEFAULT '0' COMMENT 'tình trạng gửi mail : 1 : đã gửi , 0: chưa gửi; 2: khong thanh cong',
  `open_status` tinyint(11) NOT NULL DEFAULT '0' COMMENT 'đã mở mail hay chưa1; da mo; 0 chua mo',
  `time_open` int(11) NOT NULL DEFAULT '0' COMMENT 'thời gian mở mail',
  `click_status` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'trang thai click cua 1 user voi 1 mail:1 da click; 0 chua',
  `time_click` int(11) NOT NULL DEFAULT '0' COMMENT 'thời gian click vào lick trong mail',
  `send_start_time` int(11) NOT NULL COMMENT 'thoi diem gui mail',
  `no_receive_mail` tinyint(4) NOT NULL DEFAULT '1' COMMENT 'co nhan mail hay khong : 1 co; 0 khong',
  `time_no_receive_mail` int(11) NOT NULL DEFAULT '0' COMMENT 'thời gian người dùng từ chối nhận mail',
  PRIMARY KEY (`id`),
  KEY `email_marketing_id` (`email_marketing_id`)
) ENGINE=MyISAM  DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci AUTO_INCREMENT=1964 ;

