--
-- Table structure for table `stores_themes`
--

CREATE TABLE IF NOT EXISTS `stores_themes` (
  `package_id` int(11) NOT NULL COMMENT 'Ma goi shop',
  `theme_id` int(11) NOT NULL,
  `master_theme_id` int(11) DEFAULT NULL COMMENT 'id theme ke thua',
  `master_theme_name` varchar(200) COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'ten theme ke thua',
  `store_id` int(11) NOT NULL,
  `theme_name` varchar(255) COLLATE utf8_unicode_ci NOT NULL,
  `theme_title` varchar(255) COLLATE utf8_unicode_ci NOT NULL,
  `description` varchar(255) COLLATE utf8_unicode_ci NOT NULL,
  `style_css` varchar(100) COLLATE utf8_unicode_ci NOT NULL DEFAULT 'default',
  `last_active_time` int(11) DEFAULT NULL COMMENT 'Trạng thái hoạt động của themes',
  `last_active_user_id` int(11) DEFAULT NULL COMMENT 'Người thay đổi gần nhất',
  `last_active_user_name` varchar(255) COLLATE utf8_unicode_ci NOT NULL,
  `time_bought` int(11) NOT NULL COMMENT 'Thời gian mua themes',
  `price` double NOT NULL,
  `total_bought` int(4) NOT NULL DEFAULT '0',
  `total_use` int(4) NOT NULL DEFAULT '0',
  `buy_type` int(1) NOT NULL DEFAULT '0' COMMENT 'kiểu mua,0 : miễn phí , 1: mua thường , 2: mua độc quyền',
  `used` tinyint(1) NOT NULL DEFAULT '0' COMMENT 'Trang thai them da duoc su dung hay chua ( 0 or 1). 0 la chua, 1 la da su dung',
  `status` tinyint(1) NOT NULL DEFAULT '0' COMMENT 'Trang thai su dung theme. 0 la khong su dung, 1 la su dung',
  `is_processed` tinyint(4) NOT NULL DEFAULT '-1' COMMENT 'Trạng thái cron đã xử lý dữ liệu chưa : 1 : đã xử lý , 0 : chờ xử lý , -1 : chờ user apply',
  PRIMARY KEY (`theme_id`,`store_id`)
) ENGINE=MyISAM DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci COMMENT='Lưu các themes được sử dụng của gian hàng';

-- --------------------------------------------------------

--
-- Table structure for table `themes`
--

CREATE TABLE IF NOT EXISTS `themes` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `master_theme_id` int(11) NOT NULL COMMENT 'id cua theme ke thua',
  `master_theme_name` varchar(200) COLLATE utf8_unicode_ci NOT NULL COMMENT 'ten cua theme ke thua',
  `name` varchar(128) COLLATE utf8_unicode_ci NOT NULL COMMENT 'Tên của theme, có thể trùng nhau',
  `title` varchar(255) COLLATE utf8_unicode_ci NOT NULL,
  `description` varchar(256) COLLATE utf8_unicode_ci NOT NULL COMMENT 'Mô tả qua về themes',
  `price` float NOT NULL COMMENT 'Giá của themes',
  `sole_price` double DEFAULT NULL COMMENT 'gia doc quyen',
  `style_css` varchar(100) COLLATE utf8_unicode_ci NOT NULL DEFAULT 'default',
  `total_bought` int(4) NOT NULL DEFAULT '0',
  `total_use` int(4) NOT NULL DEFAULT '0' COMMENT 'Bao nhiêu gian hàng đang sử dụng',
  `package_code` varchar(10) COLLATE utf8_unicode_ci NOT NULL DEFAULT '1' COMMENT '= SHOP la co ban, = STORE la chuyen nghiep',
  `store_level_free` varchar(100) COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'Luu danh sach cac muc shop duoc free theme nay',
  `store_level_code` varchar(20) COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'lưu thông tin mức shop cao nhất được phép sử dụng',
  `colors` varchar(200) COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'danh sách mã màu phù hợp với theme , cách nhau bởi dấu ,',
  `store_activity_fields` varchar(200) COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'danh sách các mã ngành hoạt động phù hợp với theme , cách nhau bởi dấu ,',
  `tags` varchar(400) COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'danh sách các tags liên quan đến theme , ngăn cách nhau bởi dấu ,',
  `is_sole` tinyint(1) NOT NULL DEFAULT '0' COMMENT '1 : đã mua độc  quyền , 0 : chưa mua độc quyền',
  `status` tinyint(1) NOT NULL COMMENT 'Trang thai ban (0 chưa ban, 1 dang ban)',
  `is_converted` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'Trang thai convert theme: 0 chua, 1 la da convert',
  PRIMARY KEY (`id`)
) ENGINE=MyISAM  DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci COMMENT='Lưu danh sách themes của hệ thống' AUTO_INCREMENT=211 ;
