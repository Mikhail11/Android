CREATE TABLE IF NOT EXISTS `order_transaction_bk` (
  `id` bigint(20) NOT NULL AUTO_INCREMENT,
  `order_id` varchar(100) NOT NULL COMMENT ' Mã hóa đơn ',
  `item_id` varchar(200) DEFAULT NULL COMMENT ' Mã sản phẩm ',
