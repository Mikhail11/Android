  `item_price` double NOT NULL,
  `item_quantity` int(11) NOT NULL,
  `promotion_value` double NOT NULL,
  `promotion_unit` tinyint(1) NOT NULL DEFAULT '1' COMMENT '1:%, 2:vnd',
