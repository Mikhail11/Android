--
-- Table structure for table `item_search`
--

CREATE TABLE IF NOT EXISTS `item_search` (
  `id` int(10) unsigned NOT NULL,
  `search_text` varchar(400) COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'Truong tim kiem fulltext bao  gom title, hang, thuoc tinh',
  `category_id` int(10) unsigned DEFAULT '0' COMMENT 'Id cua danh muc san pham',
  `e_category_id` int(11) DEFAULT '0' COMMENT 'Danh muc GHCN',
  `cmall_category` int(10) NOT NULL,
  `model_id` int(10) unsigned DEFAULT '0' COMMENT 'Id cua model',
  `is_fake` tinyint(4) NOT NULL COMMENT '1 Hang chinh hang, 0 khong fai chinh hang 2 hang hieu 4 nguoi ban lon',
  `is_smart_phone` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'La smart phone hay khong',
  `manufacturer_id` int(11) DEFAULT '0' COMMENT 'id cua hang san xuat',
  `user_id` int(10) unsigned NOT NULL DEFAULT '0' COMMENT 'User ID cua nguoi dang san pham',
  `is_nbdb` tinyint(1) NOT NULL DEFAULT '0',
  `estore_id` int(10) unsigned DEFAULT '0' COMMENT 'Ma GHCN',
  `shop_level` tinyint(4) DEFAULT '0' COMMENT 'cap do nguoi ban: 0 Thuong, >= 5 la ShopCN, < 5 la shop co ban',
  `price_buynow` double DEFAULT '0' COMMENT 'Gia mua ngay cua SP(Gia mua ngay cua SP mua ngay/Mac ca hoac Mua ngay cua dau gia)',
  `price_order` double NOT NULL DEFAULT '0' COMMENT 'Gia dung de sap xep. Neu la USD = Gia*tygia',
  `currency_id` tinyint(4) DEFAULT '1' COMMENT 'Ma don vi tien te(1- VND hoac 3 - USD)',
  `is_not_price` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'Lam co de day cac SP co gia = 0 xuong duoi cung',
  `time_start` int(10) unsigned DEFAULT '0' COMMENT 'Thoi gian bat dau len san cua 1 san pham',
  `time_end` int(11) DEFAULT '0' COMMENT 'Thoi gian het han/ket thuc cua 1 san pham',
  `status` smallint(4) unsigned NOT NULL DEFAULT '1' COMMENT 'Tinh trang duyet san pham',
  `status_cmall` tinyint(1) unsigned NOT NULL COMMENT 'Trạng thái Cmall: 0: chưa duyệt, 1: đã duyệt',
  `status_map` tinyint(3) unsigned NOT NULL COMMENT 'Trang thai map san pham 1. Map dung, 2 da map lai, 4 yeu cau map lai',
  `is_crawl` tinyint(2) NOT NULL DEFAULT '1' COMMENT '1 san pham crawl, 0 san pham dang tay',
  `item_score` int(5) unsigned NOT NULL COMMENT 'Diem chat luong san pham = SUM( is_nganluong+ free_shiping+return+warrenty+promotion+seller_score+seller_level+user_like)',
  `seller_score` smallint(6) DEFAULT '0' COMMENT 'Dien chi so cua seller, dung de tiinh item_score',
  `type` tinyint(4) DEFAULT '0' COMMENT 'Kieu san pham dau gia / Mua ngay / Gian hang/ mac ca',
  `total_item` mediumint(8) unsigned NOT NULL COMMENT 'Tong so san pham',
  `shipping_type` tinyint(4) DEFAULT '0' COMMENT 'Hinh thuc van chuyển: 1: miễn phí,  2: phí theo CĐT (hoặc shipchung nếu là người bán đảm bảo),  3: phí vận chuyển cố định,  4: phí vận chuyển thỏa thuận',
  `total_comment` smallint(5) unsigned NOT NULL COMMENT 'Tong so comment cua 1 san pham',
  `total_like` smallint(5) unsigned NOT NULL COMMENT 'tong so luot uu thich',
  `time_update` int(10) unsigned DEFAULT '0' COMMENT 'Thoi gian cap nhat san pham',
  `time_post` int(10) unsigned DEFAULT '0' COMMENT 'Thoi gian dang san pham',
  `condition` tinyint(4) DEFAULT '0' COMMENT 'Tinh trang san pham la cu, moi',
  `hit_count` mediumint(5) unsigned NOT NULL COMMENT 'tong so luot xem',
  `payment_method` tinyint(4) DEFAULT '0' COMMENT 'Kieu chap nhan thanh toan( ngan luong/ truoc tiep)',
  `sell_zone` int(11) DEFAULT '0' COMMENT 'Ma khu vuc tinh thanh cua nguoi dang ban',
  `store_config` tinyint(4) NOT NULL COMMENT 'trang thai sp cua GH: 1 trang chu, 2 noi bat, 4 moi cap nhat',
  `report_bad` tinyint(6) unsigned NOT NULL COMMENT 'Bao cao xau: 1 noi dung, 2 sai model, 4 het hang, 8 sai gia',
  `p1` double NOT NULL,
  `p2` double NOT NULL,
  `p3` double NOT NULL,
  `p4` double NOT NULL,
  `p5` double NOT NULL,
  `p6` double NOT NULL,
  `p7` double NOT NULL,
  `p8` double NOT NULL,
  `p9` double NOT NULL,
  `p10` double NOT NULL,
  PRIMARY KEY (`id`,`user_id`),
  KEY `category_id` (`category_id`),
  KEY `status` (`status`),
  KEY `item_score` (`item_score`),
  KEY `user_id` (`user_id`),
  KEY `e_category_id` (`e_category_id`),
  KEY `id` (`id`),
  FULLTEXT KEY `search_text` (`search_text`)
) ENGINE=MyISAM DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci;

