--
-- Table structure for table `item_auction`
--

CREATE TABLE IF NOT EXISTS `item_auction` (
  `id` int(11) NOT NULL AUTO_INCREMENT COMMENT 'Mã đâu giá: AUTO',
  `time_created` int(11) DEFAULT NULL COMMENT 'Thời gian tạo',
  `buyer_user_id` int(11) DEFAULT NULL COMMENT 'Mã người đấu giá',
  `buyer_username` varchar(200) DEFAULT NULL COMMENT 'Username người đấu giá',
  `item_id` bigint(20) DEFAULT NULL COMMENT 'Mã sản phẩm',
  `item_name` varchar(255) DEFAULT NULL COMMENT 'Nhãn sản phẩm',
  `item_size` varchar(255) DEFAULT NULL,
  `item_color` varchar(255) DEFAULT NULL,
  `item_condition` varchar(255) DEFAULT NULL,
  `price_nganluong` int(11) DEFAULT NULL COMMENT 'Giá NL',
  `price_success` float DEFAULT NULL COMMENT 'Giá đấu giá thành công',
  `payment_id` int(11) DEFAULT NULL COMMENT 'Mã thanh toán',
  `type_payment` tinyint(1) NOT NULL DEFAULT '1' COMMENT 'Kiểu người mua thanh toán 1:Thanh toán thông qua LK Nganluong 2:Thanh toán qua giao dịch sang bên NganLuong (Đã sửa đổi: Phải liên kết qua NL)',
  `status` tinyint(1) NOT NULL DEFAULT '0' COMMENT 'Trạng thái thanh toán: 0-Chưa thanh toán NL; 1-Đã thanh toán NL-Chưa mua eBay; 2-Đã thanh toán NL-Đã mua eBay; 3-Đặt giá đã trả lại tiền; 4-Đặt giá chiến thắng',
  `exchage_usd` int(11) DEFAULT NULL COMMENT 'Tỷ giá tại lúc đấu giá',
  `order_id` int(11) DEFAULT NULL COMMENT 'Mã hóa đơn: có hóa đơn có nghĩa là đã chiến thắng',
  `seller_username` varchar(200) DEFAULT NULL COMMENT 'username người bán',
  `seller_feedback_score` varchar(200) DEFAULT NULL COMMENT 'Số điểm uy tín người bán',
  `seller_positive_feedback_percent` varchar(200) DEFAULT NULL COMMENT '% uy tín người bán',
  `seller_location` varchar(50) DEFAULT NULL COMMENT 'địa điểm người bán',
  `buyer_fullname` varchar(200) DEFAULT NULL COMMENT 'Họ và tên người mua',
  `buyer_email` varchar(100) DEFAULT NULL COMMENT 'Email người mua',
  `address` varchar(200) DEFAULT NULL COMMENT 'Địa chỉ người mua cũng là địa chỉ nhận hàng',
  `buyer_phone` varchar(100) DEFAULT NULL COMMENT 'Số điện thoại người mua',
  `buyer_zone` varchar(200) DEFAULT NULL COMMENT 'Khu vực(Tỉnh/TP) người mua',
  `item_image` varchar(200) DEFAULT NULL COMMENT 'Ảnh Sản phẩm',
  `item_num` tinyint(4) NOT NULL DEFAULT '1' COMMENT 'Số lượng sản phẩm(thường = 1?)',
  `item_category` int(11) DEFAULT NULL COMMENT 'Mã danh mục sản phẩm',
  `item_category_name` varchar(200) DEFAULT NULL COMMENT 'Tên Danh mục SP',
  `item_time_end` int(11) DEFAULT NULL COMMENT 'Thời gian kết thúc',
  `price_aution` float NOT NULL DEFAULT '0' COMMENT 'Giá đấu giá',
  `ebay_transaction` varchar(200) DEFAULT NULL COMMENT 'Giao dịch eBay',
  `refund_payment_id` varchar(100) DEFAULT NULL COMMENT 'Mã thanh toán refund',
  `us_statustax` float DEFAULT NULL COMMENT 'Thuế bang',
  `us_statustax_usd` float DEFAULT NULL COMMENT 'Thuế nội địa',
  `ression_refund` text COMMENT 'Lý do refund (hoàn lại tiền nếu đấu giá ko thành công-trả lại tiền thừa)',
  `buyer_tax_paid` float DEFAULT NULL COMMENT 'Thuế nguồi mua phải trả',
  `buyer_fees` float DEFAULT NULL COMMENT 'Phí người mua thanh toán cho NL',
  `shipping_fee_usus` float DEFAULT NULL COMMENT 'phí  vận chuyển nội địa Mỹ',
  `shipping_fee_usvn` float DEFAULT NULL COMMENT 'Phí VC US - VN',
  `aution_fee` float DEFAULT NULL COMMENT 'Phí người mua đấu giá',
  `aution_fee_id` int(11) DEFAULT NULL COMMENT 'Mã phí đấu giá',
  `promotion_type` tinyint(1) DEFAULT NULL COMMENT '1:SundayOff 2: SpecialDay 3:LuckyDay',
  `promotion` tinyint(1) NOT NULL DEFAULT '0' COMMENT '0:không có giảm giá 1:Có giảm giá',
  `type_user` tinyint(4) NOT NULL DEFAULT '1' COMMENT '2: không có tài khoản mua hàng, 1: Có tài khoản mua hàng',
  `waiting_seller` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'Chờ người bán',
  `info_receive` text COMMENT 'thông tin người nhận',
  `vat_infos` text,
  PRIMARY KEY (`id`)
) ENGINE=MyISAM  DEFAULT CHARSET=latin1 AUTO_INCREMENT=15092 ;

