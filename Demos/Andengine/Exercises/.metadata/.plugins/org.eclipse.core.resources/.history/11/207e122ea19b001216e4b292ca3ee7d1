  KEY `item_id` (`item_id`)
) ENGINE=MyISAM  DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci COMMENT='Bảng lưu các sản phẩm được chọn khuyến mãi' AUTO_INCREMENT=233 ;

