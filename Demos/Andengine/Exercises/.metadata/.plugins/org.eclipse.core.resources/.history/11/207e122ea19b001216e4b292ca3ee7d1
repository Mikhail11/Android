-- --------------------------------------------------------

--
-- Table structure for table `dtn_transactions_add`
--

CREATE TABLE IF NOT EXISTS `dtn_transactions_add` (
  `id` int(10) unsigned NOT NULL AUTO_INCREMENT,
  `user_id` int(11) unsigned NOT NULL,
  `creator_id` int(11) unsigned NOT NULL COMMENT 'user id or admin id',
  `approver_id` int(11) unsigned NOT NULL DEFAULT '0',
  `main_amount` int(11) NOT NULL COMMENT 'add to main balance',
  `promotion_amount` int(11) NOT NULL COMMENT 'add to promotion balance',
  `promotion_time` int(11) unsigned NOT NULL COMMENT 'add to promotion expire time',
  `transaction_type` tinyint(2) NOT NULL DEFAULT '0' COMMENT 'view file config',
  `transaction_info` text COLLATE utf8_unicode_ci NOT NULL,
  `transaction_status` tinyint(1) NOT NULL DEFAULT '1' COMMENT 'view file config for detail',
  `NL_token` varchar(40) COLLATE utf8_unicode_ci NOT NULL COMMENT 'token used to update status',
  `created_time` int(11) unsigned NOT NULL,
  `last_update` int(11) unsigned NOT NULL,
  `expire_time` int(11) unsigned NOT NULL,
  `trans_month` tinyint(2) unsigned NOT NULL COMMENT 'for partioning',
  PRIMARY KEY (`id`,`trans_month`),
  KEY `user_id` (`user_id`),
  KEY `trans_month` (`trans_month`)
) ENGINE=MyISAM  DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci COMMENT='Stores top-up transactions' AUTO_INCREMENT=154 ;

