  `receiver_address` varchar(255) CHARACTER SET utf8 COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'dia chi nguoi nhan',
  `receiver_zone` int(11) unsigned DEFAULT '0' COMMENT 'khu vuc cua nguoi nhan',
  `receiver_date` int(11) unsigned DEFAULT '0' COMMENT 'ngay nhan',
  `seller_comment` varchar(512) CHARACTER SET utf8 COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'nguoi ban phan hoi',
  `buyer_comment` varchar(512) CHARACTER SET utf8 COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'nguoi mua phan hoi',
  `shipping_guide` varchar(512) CHARACTER SET utf8 COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'huong dan van chuyen',
  `shipping_code` varchar(100) CHARACTER SET utf8 COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'Mã vận đơn',
  `payment_guide` varchar(512) CHARACTER SET utf8 COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'huong dan thanh toan',
  `campaign_text` text CHARACTER SET utf8 COLLATE utf8_unicode_ci COMMENT 'Thông tin khuyến mãi campaign dành cho hóa đơn',
  `ga` tinyint(4) DEFAULT '0' COMMENT 'check ecommerce tren GA',
  PRIMARY KEY (`id`),
  KEY `id` (`id`)
) ENGINE=MyISAM DEFAULT CHARSET=latin1;

-- --------------------------------------------------------

--
-- Table structure for table `order_item`
--

CREATE TABLE IF NOT EXISTS `order_item` (
  `order_id` int(11) NOT NULL AUTO_INCREMENT COMMENT 'id hoa don',
  `item_id` int(11) NOT NULL COMMENT 'id san pham',
  `title` varchar(255) COLLATE utf8_unicode_ci NOT NULL,
  `item_price` double DEFAULT '0' COMMENT 'giá san pham',
  `shipping_type` tinyint(1) DEFAULT '1',
  `shipping_price` double DEFAULT '0' COMMENT 'phí van chuyen',
  `total_item` int(3) DEFAULT NULL COMMENT 'so luong san pham',
  `campaign_id` int(11) DEFAULT NULL COMMENT 'ma chien dich ap dung cho san pham',
  `campaign_price` double DEFAULT NULL COMMENT 'chi phi ap dung cho khuyen mai',
  `campaign_text` text COLLATE utf8_unicode_ci COMMENT 'noi dung ap dung cho khuyen mai cua chien dich',
  `weight` float DEFAULT '0' COMMENT 'can nang san pham',
  `long_width_height` varchar(30) COLLATE utf8_unicode_ci DEFAULT NULL,
  `shipping_fee` double DEFAULT '0',
  `category_id` int(11) DEFAULT '0' COMMENT 'danh muc san pham thuoc',
  `e_category_id` int(11) NOT NULL DEFAULT '0' COMMENT 'id danh muc gian hang',
  `estore_id` int(11) unsigned NOT NULL DEFAULT '0' COMMENT 'ma gian hang cn',
  `date` int(11) NOT NULL COMMENT 'Truong luu nam phat sinh hoa don dung cho convert',
  `year` smallint(6) NOT NULL COMMENT 'Truong luu nam phat sinh hoa don dung cho convert',
  PRIMARY KEY (`order_id`,`item_id`),
  KEY `order_id` (`order_id`,`item_id`,`category_id`,`e_category_id`),
  KEY `total_item` (`total_item`,`campaign_id`)
) ENGINE=MyISAM  DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci COMMENT='bang luu thong tin cac sp cua hoa don' AUTO_INCREMENT=4948039 ;

-- --------------------------------------------------------

