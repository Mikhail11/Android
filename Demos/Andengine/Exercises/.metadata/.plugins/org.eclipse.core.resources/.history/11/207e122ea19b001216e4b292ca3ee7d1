--
-- Table structure for table `cm_heart_banner`
--

CREATE TABLE IF NOT EXISTS `cm_heart_banner` (
  `id` int(10) unsigned NOT NULL AUTO_INCREMENT,
  `title` varchar(255) COLLATE utf8_unicode_ci NOT NULL COMMENT 'tiêu đề heart banner',
  `link` varchar(255) COLLATE utf8_unicode_ci NOT NULL COMMENT 'link liên kết của heart banner',
  `file_name` varchar(255) COLLATE utf8_unicode_ci NOT NULL COMMENT 'tên ảnh lớn',
  `thumb_name` varchar(255) COLLATE utf8_unicode_ci NOT NULL COMMENT 'tên ảnh nhỏ',
