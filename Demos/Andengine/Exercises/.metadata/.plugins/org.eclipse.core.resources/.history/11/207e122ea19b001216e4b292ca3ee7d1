  `created` int(10) unsigned NOT NULL COMMENT 'thời gian tạo bản ghi',
  `updated` int(10) unsigned NOT NULL COMMENT 'thời gian cập nhật',
  PRIMARY KEY (`id`),
  KEY `start_time` (`start_time`),
  KEY `end_time` (`end_time`),
  KEY `weight` (`position`),
  KEY `status` (`status`),
  KEY `type` (`type`),
  KEY `created` (`created`),
  KEY `updated` (`updated`),
  KEY `admin_id` (`admin_id`)
) ENGINE=MyISAM  DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci AUTO_INCREMENT=19 ;

