USE `cdt20_order_feedback`;

-- --------------------------------------------------------

--
-- Table structure for table `order`
--

CREATE TABLE IF NOT EXISTS `order` (
  `id` int(11) unsigned NOT NULL AUTO_INCREMENT COMMENT 'id duoc khoi tao va lay insert tu bảng order_index',
  `order_type` tinyint(1) DEFAULT NULL COMMENT '1 mua ngay tu gio hang, 2 mua ngay doc lap, 3 mac ca rang buoc, 4 mac ca tu do, 5 dau gia thanh cong, 6 mua ngay dau gia, 7 chao mua, 8 giong 2 nhung do CSKH tao, 9  mua tai cua hang',
  `code` varchar(20) CHARACTER SET utf8 COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'Ma hoa don',
  `seller_id` int(11) unsigned DEFAULT NULL COMMENT 'Id cua nguoi ban',
  `seller_username` varchar(255) CHARACTER SET utf8 COLLATE utf8_unicode_ci DEFAULT NULL,
  `seller_zone_id` int(4) unsigned DEFAULT '0' COMMENT 'Mã khu vực người bán',
  `buyer_id` int(11) unsigned DEFAULT NULL COMMENT 'Id cua nguoi mua',
  `buyer_username` varchar(255) CHARACTER SET utf8 COLLATE utf8_unicode_ci DEFAULT NULL,
  `buyer_zone_id` int(11) DEFAULT '0' COMMENT 'Mã khu vực người mua',
  `receiver_zone_id` int(11) DEFAULT '0',
  `payment_status` tinyint(1) NOT NULL DEFAULT '0',
  `payment_method` tinyint(1) DEFAULT '0',
  `shipping_type` tinyint(4) NOT NULL,
  `shipping_status` tinyint(1) NOT NULL DEFAULT '1' COMMENT 'Trang thai van chuyen',
  `shipping_price` double DEFAULT NULL COMMENT 'Tong giá tri phi van chuyen hóa don',
  `cost` double DEFAULT NULL COMMENT 'Tong giá tri thuan hóa don. chua bao gom cac khoan phi',
  `vnd_total_cost` double DEFAULT NULL COMMENT 'Tong gia tri don hang theo Viet Nam Dong',
  `usd_total_cost` double DEFAULT NULL COMMENT 'Tong gia tri don hang theo Do La My',
  `total_checkout` double DEFAULT NULL COMMENT 'Tong gia tri thanh toan don hang. Sau khi bao gom ca cac khoan phi',
  `total_discount` double DEFAULT NULL COMMENT 'Tong gia tri duoc giam gia cua don hang',
  `exchange_rate` double DEFAULT NULL COMMENT 'Ty gia',
  `time_checkout` int(11) unsigned DEFAULT NULL COMMENT 'Thoi gian thanh toan',
  `time_refund` int(11) NOT NULL COMMENT 'thoi gian refund',
  `time_created` int(11) unsigned DEFAULT NULL COMMENT 'Thoi gian tao hoa don',
  `time_update` int(11) unsigned DEFAULT NULL COMMENT 'Thoi gian cap nhat',
  `time_pay_expire` int(11) unsigned DEFAULT NULL COMMENT 'Han thanh toán tinh theo ngay thang, gio, phut, giay',
  `campaign_id` int(11) unsigned DEFAULT NULL COMMENT 'Ma campaign kieu coupon',
  `campaign_price` double DEFAULT NULL COMMENT 'Gia tri campaign ap dung cho hoa don',
  `nganluong_id` int(11) unsigned DEFAULT NULL COMMENT 'Ma ngan luong ap dung cho hoa don',
  `nganluong_type` tinyint(1) DEFAULT NULL COMMENT 'Kieu thanh toan tai ngan luong',
  `nganluong_transaction_id` int(11) unsigned DEFAULT NULL COMMENT 'ma giao dich voi ngan luong',
  `coupon_id` varchar(100) CHARACTER SET utf8 COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'Ma khuyen mai ap dung cho hoa don',
  `estore_id` int(11) unsigned DEFAULT NULL COMMENT 'id gian hang cua nguoi ban',
  `deleted` tinyint(1) NOT NULL DEFAULT '0' COMMENT 'trang thai xoa. 0 la hien thi. 1 la nguoi mua xoa, 2 la nguoi ban xoa. 3 la ca mua ca ban deu xoa, 4 do admin xoa',
  `token_key` varchar(32) CHARACTER SET utf8 COLLATE utf8_unicode_ci NOT NULL COMMENT 'ma dung cho hoa don nguoi dung khong dang nhap',
  `token_key_NL` varchar(50) NOT NULL,
  `content_support` text CHARACTER SET utf8 COLLATE utf8_unicode_ci COMMENT 'noi dung ho tro giao dich',
  `order_status_cskh` tinyint(1) unsigned DEFAULT NULL COMMENT 'Trạng thái chăm sóc hóa đơn',
  `is_call_ga` tinyint(1) NOT NULL COMMENT 'So lan goi GA',
  `is_thuho` tinyint(1) NOT NULL COMMENT '1 la cho dien tu thu ho',
  `year` smallint(6) NOT NULL COMMENT 'Truong luu nam phat sinh hoa don dung cho convert',
  PRIMARY KEY (`id`,`year`),
  KEY `id` (`id`),
  KEY `order_type` (`order_type`,`seller_id`,`buyer_id`,`estore_id`,`time_created`,`time_pay_expire`),
  KEY `payment_status` (`payment_status`,`payment_method`,`shipping_status`)
) ENGINE=MyISAM  DEFAULT CHARSET=latin1 AUTO_INCREMENT=2794463 ;

-- --------------------------------------------------------

--
-- Table structure for table `order_2006`
--

CREATE TABLE IF NOT EXISTS `order_2006` (
  `id` int(11) unsigned NOT NULL COMMENT 'id duoc khoi tao va lay insert tu bảng order_index',
  `order_type` tinyint(1) DEFAULT NULL COMMENT 'Loai hóa don: mua ngay, dau giá, mac ca, chap nhan chào mua.',
  `code` varchar(20) CHARACTER SET utf8 COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'Ma hoa don',
  `seller_id` int(11) unsigned DEFAULT NULL COMMENT 'Id cua nguoi ban',
  `seller_username` varchar(50) NOT NULL,
  `seller_zone_id` int(4) unsigned DEFAULT '0' COMMENT 'Mã khu vực người bán',
  `buyer_id` int(11) unsigned DEFAULT NULL COMMENT 'Id cua nguoi mua',
  `buyer_username` varchar(50) DEFAULT NULL,
  `buyer_zone_id` int(11) DEFAULT '0' COMMENT 'Mã khu vực người mua',
  `receiver_zone_id` int(11) unsigned DEFAULT '0',
  `payment_status` tinyint(1) DEFAULT NULL COMMENT 'trang thái thanh toán',
  `payment_method` tinyint(1) DEFAULT NULL COMMENT 'Phuong thuc thanh toán: qua ngân luong hay COD.',
  `shipping_status` tinyint(1) DEFAULT NULL COMMENT 'Trang thai van chuyen',
  `shipping_price` double DEFAULT NULL COMMENT 'Tong giá tri phi van chuyen hóa don',
  `cost` double DEFAULT NULL COMMENT 'Tong giá tri thuan hóa don. chua bao gom cac khoan phi',
  `vnd_total_cost` double DEFAULT NULL COMMENT 'Tong gia tri don hang theo Viet Nam Dong',
  `usd_total_cost` double DEFAULT NULL COMMENT 'Tong gia tri don hang theo Do La My',
  `total_checkout` double DEFAULT NULL COMMENT 'Tong gia tri thanh toan don hang. Sau khi bao gom ca cac khoan phi',
  `total_discount` double DEFAULT NULL COMMENT 'Tong gia tri duoc giam gia cua don hang',
  `exchange_rate` double DEFAULT NULL COMMENT 'Ty gia',
  `time_checkout` int(11) unsigned DEFAULT NULL COMMENT 'Thoi gian thanh toan',
  `time_refund` int(11) NOT NULL COMMENT 'thoi gian refund',
  `time_created` int(11) unsigned DEFAULT NULL COMMENT 'Thoi gian tao hoa don',
  `time_update` int(11) unsigned DEFAULT NULL COMMENT 'Thoi gian cap nhat',
  `time_pay_expire` int(11) unsigned DEFAULT NULL COMMENT 'Han thanh toán tinh theo ngay thang, gio, phut, giay',
  `campaign_id` int(11) unsigned DEFAULT NULL COMMENT 'Ma campaign ap dung cho hoa don',
  `campaign_price` double DEFAULT NULL COMMENT 'Gia tri campaign ap dung cho hoa don',
  `nganluong_id` int(11) unsigned DEFAULT NULL COMMENT 'Ma ngan luong ap dung cho hoa don',
  `nganluong_type` tinyint(1) DEFAULT NULL COMMENT 'Kieu thanh toan tai ngan luong',
  `coupon_id` varchar(100) CHARACTER SET utf8 COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'Ma khuyen mai ap dung cho hoa don',
  `estore_id` int(11) unsigned DEFAULT NULL COMMENT 'id gian hang cua nguoi ban',
  `deleted` tinyint(1) NOT NULL DEFAULT '0',
  `token_key` varchar(32) CHARACTER SET utf8 COLLATE utf8_unicode_ci NOT NULL COMMENT 'ma dung cho hoa don nguoi dung khong dang nhap',
  `content_support` varchar(512) CHARACTER SET utf8 COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'noi dung ho tro giao dich',
  `is_call_ga` tinyint(1) NOT NULL COMMENT 'So lan goi GA',
  PRIMARY KEY (`id`),
  KEY `id` (`id`),
  KEY `order_type` (`order_type`,`seller_id`,`buyer_id`,`estore_id`,`time_created`,`time_pay_expire`),
  KEY `payment_status` (`payment_status`,`payment_method`,`shipping_status`)
) ENGINE=MyISAM DEFAULT CHARSET=latin1;

