-- --------------------------------------------------------

--
-- Table structure for table `cm_categories`
--

CREATE TABLE IF NOT EXISTS `cm_categories` (
  `id` int(10) unsigned NOT NULL AUTO_INCREMENT COMMENT 'id danh mục cmall',
  `id_parent` int(10) unsigned NOT NULL COMMENT 'id danh mục cha',
  `cdt_id` varchar(225) COLLATE utf8_unicode_ci NOT NULL COMMENT 'danh sách id chợ điện tử tương ứng',
  `title` varchar(255) COLLATE utf8_unicode_ci NOT NULL COMMENT 'tên danh mục cmall',
  `title_slugified` varchar(255) COLLATE utf8_unicode_ci NOT NULL COMMENT 'tên danh mục sau khi đã slugify',
  `page_description` varchar(255) COLLATE utf8_unicode_ci NOT NULL COMMENT 'mô tả thêm về danh mục',
  `page_title` varchar(225) COLLATE utf8_unicode_ci NOT NULL COMMENT 'title của page',
  `filename` varchar(255) COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'tên ảnh danh mục (có thể có hoặc không)',
  `level` tinyint(1) unsigned NOT NULL COMMENT 'cấp độ danh mục',
  `status` tinyint(1) NOT NULL DEFAULT '1' COMMENT '0-chưa kích hoạt, 1-đã kích hoạt, 3 hiển thị trên navigation',
  `position` int(11) NOT NULL COMMENT 'thứ tự danh mục',
  `admin_id` int(10) unsigned NOT NULL COMMENT 'id người cập nhật',
  `created` int(10) unsigned NOT NULL COMMENT 'thời gian tạo',
  `updated` int(10) unsigned NOT NULL COMMENT 'thời gian cập nhật',
  PRIMARY KEY (`id`),
  UNIQUE KEY `title_slugified` (`title_slugified`),
  KEY `id_parent` (`id_parent`),
  KEY `level` (`level`),
  KEY `status` (`status`),
  KEY `weight` (`position`),
  KEY `created` (`created`),
  KEY `updated` (`updated`),
  KEY `admin_id` (`admin_id`)
) ENGINE=MyISAM  DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci AUTO_INCREMENT=97 ;

