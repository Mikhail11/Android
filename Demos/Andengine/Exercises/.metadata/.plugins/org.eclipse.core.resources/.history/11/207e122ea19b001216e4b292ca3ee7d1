CREATE TABLE IF NOT EXISTS `item_order` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `payment_code` varchar(20) NOT NULL DEFAULT '',
  `payment_method` varchar(10) NOT NULL,
  `content` text NOT NULL,
  `date` int(11) NOT NULL DEFAULT '0',
  `listing_time` int(11) NOT NULL DEFAULT '0',
  `seller_id` int(11) NOT NULL DEFAULT '0',
  `seller_username` varchar(50) NOT NULL DEFAULT '',
  `seller_name` varchar(255) NOT NULL,
  `buyer_id` int(11) NOT NULL DEFAULT '0',
  `buyer_username` varchar(50) NOT NULL DEFAULT '',
  `buyer_name` varchar(100) NOT NULL DEFAULT '',
  `buyer_address` varchar(255) NOT NULL,
  `buyer_home_phone` varchar(10) NOT NULL,
  `buyer_mobile` varchar(16) NOT NULL,
  `buyer_email` varchar(255) NOT NULL,
  `buyer_cmtnd` varchar(16) NOT NULL,
  `receiver_name` varchar(100) NOT NULL DEFAULT '',
  `receive_date` int(11) NOT NULL DEFAULT '0',
  `receiver_address` text NOT NULL,
  `receiver_zone` int(11) NOT NULL DEFAULT '0',
  `receiver_home_phone` varchar(11) NOT NULL DEFAULT '',
  `receiver_mobile` varchar(11) NOT NULL DEFAULT '',
  `receiver_email` varchar(50) NOT NULL DEFAULT '',
  `payment_type` varchar(255) NOT NULL,
  `payment_status` int(2) NOT NULL DEFAULT '0',
  `shipping_cost` float NOT NULL DEFAULT '0',
  `shipping_type` varchar(255) NOT NULL,
  `shipping_status` int(2) NOT NULL DEFAULT '0',
  `sms_status` int(2) NOT NULL DEFAULT '0',
  `seller_comment` text NOT NULL,
  `buyer_comment` text NOT NULL,
  `cdt_comment` text NOT NULL,
  `time_modifyer_seller` varchar(255) NOT NULL,
  `time_modifyer_buyer` varchar(255) NOT NULL,
  `time_modifyer_cdter` varchar(255) NOT NULL,
  `carrier_id` int(11) NOT NULL DEFAULT '0',
  `cost` double NOT NULL,
  `vnd_total_cost` double NOT NULL DEFAULT '0',
  `usd_total_cost` double NOT NULL DEFAULT '0',
  `currency_id` tinyint(3) NOT NULL,
  `exchange_rate` float NOT NULL,
  `estore_name` varchar(100) NOT NULL DEFAULT '',
  `estore_id` int(11) NOT NULL DEFAULT '0',
  `payer_name` varchar(100) NOT NULL DEFAULT '',
  `payer_address` varchar(255) NOT NULL DEFAULT '',
  `payer_email` varchar(50) NOT NULL DEFAULT '',
  `payer_home_phone` varchar(10) NOT NULL DEFAULT '',
  `payer_mobile` varchar(10) NOT NULL DEFAULT '',
  `payer_date` int(11) NOT NULL DEFAULT '0',
  `item_id` int(11) NOT NULL,
  `sub_item_id` int(11) NOT NULL,
  `deleted` int(1) NOT NULL DEFAULT '0',
  `seller_deleted` tinyint(1) NOT NULL DEFAULT '0',
  `payment_guide` text NOT NULL,
  `status_update` tinyint(2) NOT NULL,
  `payment_for` tinyint(2) NOT NULL,
  `ip_info` varchar(255) NOT NULL,
  `weight` int(8) NOT NULL,
  `shipping_guide` varchar(255) NOT NULL,
  `shipping_price` double NOT NULL,
  `amount_item` tinyint(2) NOT NULL,
  `seller_zone` int(11) NOT NULL,
  `category_id` int(6) NOT NULL,
  `structure_id` bigint(20) NOT NULL,
  `ga` tinyint(1) NOT NULL,
  `coupon_id` int(11) NOT NULL,
  `item_name` varchar(250) NOT NULL,
  `nganluong_id` int(11) NOT NULL,
  `nganluong_type` tinyint(1) NOT NULL,
  `total_checkout` double NOT NULL,
  `e_item_id` int(11) NOT NULL,
  `e_number_send` tinyint(2) NOT NULL,
  PRIMARY KEY (`id`),
  KEY `seller_id` (`seller_id`,`buyer_id`,`date`)
) ENGINE=MyISAM DEFAULT CHARSET=latin1 AUTO_INCREMENT=1 ;

