-- --------------------------------------------------------

--
-- Table structure for table `promotion`
--

CREATE TABLE IF NOT EXISTS `promotion` (
  `id` int(10) NOT NULL AUTO_INCREMENT,
  `type` tinyint(4) NOT NULL COMMENT 'Hình thức khuyến mãi( 1 - sản phẩm,  2 - Danh mục, 3 - Hóa đơn)',
  `method` tinyint(4) NOT NULL COMMENT 'Phương thức khuyến mãi( 1 - Miễn phí vận chuyển, 2 - Tặng sp, 3 - Tặng quà, 4 - Chiết khấu %, 5 - Giảm giá)',
  `category_id` int(11) NOT NULL COMMENT 'Danh mục cấp 1 sp',
  `title` varchar(500) DEFAULT NULL COMMENT 'Tiêu đề chiến dịch',
  `value` varchar(250) NOT NULL COMMENT 'Số lượng khuyến mãi',
  `sales` varchar(250) DEFAULT NULL COMMENT 'Phân loại khuyến mãi',
  `description` text COMMENT 'Thông tin chiến dịch',
  `time_begin` int(20) DEFAULT NULL COMMENT 'Thời gian bắt đầu chiến dịch',
  `time_end` int(20) DEFAULT NULL COMMENT 'Thời gian kết thúc chiến dịch',
  `status` smallint(2) DEFAULT '0' COMMENT 'Trạng thái của chiến dịch: 0:Tắt; 1:Bật',
  PRIMARY KEY (`id`)
) ENGINE=MyISAM  DEFAULT CHARSET=latin1 AUTO_INCREMENT=33 ;

