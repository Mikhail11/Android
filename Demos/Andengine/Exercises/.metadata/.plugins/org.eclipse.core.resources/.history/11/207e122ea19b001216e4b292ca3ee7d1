-- Table structure for table `item_vip_v2`
--

CREATE TABLE IF NOT EXISTS `item_vip_v2` (
  `id` int(10) NOT NULL AUTO_INCREMENT,
  `title` varchar(150) NOT NULL DEFAULT '' COMMENT 'Tên sản phẩm',
  `item_id` int(11) DEFAULT NULL,
  `item_link` varchar(255) DEFAULT NULL COMMENT 'Link tới quảng cáo, sử dụng trong trường hợp sản phẩm từ website khác',
  `item_image` varchar(200) DEFAULT NULL,
  `item_price` float NOT NULL COMMENT 'Giá của sản phẩm',
  `user_id` int(11) NOT NULL COMMENT 'Mã người bán',
  `creator` int(11) NOT NULL COMMENT 'Mã người đăng tin quảng cáo',
  `category_id` int(11) NOT NULL COMMENT 'Danh sách danh mục quảng cáo tin bán; các danh mục cách nhau bởi dấu phẩy',
  `path` varchar(200) NOT NULL,
  `type` tinyint(1) NOT NULL COMMENT 'Kiểu item: 1-sản phẩm của chợ; 2-sản phẩm ngoài',
  `ad_price` int(11) NOT NULL COMMENT 'Chi phí quảng cáo',
  `from_date` int(11) NOT NULL COMMENT 'Ngày bắt đầu quảng cáo',
  `to_date` int(11) NOT NULL COMMENT 'Ngày hết hạn quảng cáo',
  `created` int(11) NOT NULL COMMENT 'Thời điểm tạo quảng cáo',
  `updated` int(11) DEFAULT NULL,
  `status` int(1) NOT NULL COMMENT 'Tình trạng của quảng cáo: 1-đang chạy; 4-hủy',
  PRIMARY KEY (`id`),
  KEY `c_index` (`category_id`)
) ENGINE=MyISAM  DEFAULT CHARSET=latin1 ROW_FORMAT=DYNAMIC AUTO_INCREMENT=1223 ;

