--
-- Table structure for table `news`
--

CREATE TABLE IF NOT EXISTS `news` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `category_id` int(10) DEFAULT '0' COMMENT 'Danh mục tin',
  `title` varchar(255) NOT NULL COMMENT 'Tiêu đề hiển thị trên danh mục',
  `title_detail` varchar(255) NOT NULL COMMENT 'Tiêu đề khi vào chi tiết tin',
  `brief` text NOT NULL COMMENT 'Mô tả',
  `image_name` varchar(50) DEFAULT 'default.gif',
  `image_title` varchar(255) DEFAULT NULL COMMENT 'Tiêu đề ảnh',
  `author` int(11) DEFAULT '0' COMMENT 'tài khoản tạo tin',
  `status` int(2) DEFAULT '1' COMMENT 'Trạng Thái: 1 - cho hiển thị; 2 - Không hiển thị',
  `content` longtext COMMENT 'Nội dung',
  `time_created` int(11) DEFAULT '0' COMMENT 'Ngày tạo',
  `time_expire` int(11) DEFAULT '0' COMMENT 'Ngày hết',
  `last_time_update` int(11) NOT NULL COMMENT 'Thời gian sửa',
  `text_search` text NOT NULL COMMENT 'trường thông tin tìm kiếm',
  `lang` varchar(50) NOT NULL DEFAULT 'vietnam',
  `hit_count` int(10) unsigned NOT NULL DEFAULT '0' COMMENT 'số lần xem',
  `note` text NOT NULL COMMENT 'Ghi chú khác nếu có',
  `high_light` tinyint(1) NOT NULL DEFAULT '1' COMMENT 'tin nổi bật đặt cho tin tức',
  `home` tinyint(2) NOT NULL DEFAULT '1' COMMENT 'Đặt lên trang chủ ngân lượng: 1 mặc định, 2 đặt trang chủ',
  PRIMARY KEY (`id`),
  FULLTEXT KEY `text_search` (`text_search`)
) ENGINE=MyISAM  DEFAULT CHARSET=latin1 AUTO_INCREMENT=46 ;

