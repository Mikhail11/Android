CREATE TABLE IF NOT EXISTS `event` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `user_id` int(11) DEFAULT NULL,
  `user_name` varchar(50) CHARACTER SET utf8 COLLATE utf8_unicode_ci DEFAULT NULL,
  `date` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP,
  `name` varchar(50) CHARACTER SET utf8 COLLATE utf8_unicode_ci NOT NULL COMMENT 'Ten event',
  `type` tinyint(1) DEFAULT NULL COMMENT '1: Market, 2: Raovat, 3: eBay',
  `obj_id` varchar(20) DEFAULT NULL COMMENT 'Doi tuong bi tac dong',
  `description` varchar(255) DEFAULT NULL,
  `month` tinyint(1) NOT NULL,
  PRIMARY KEY (`id`,`month`)
) ENGINE=MyISAM  DEFAULT CHARSET=latin1
/*!50100 PARTITION BY HASH ( `month`)
PARTITIONS 12 */ AUTO_INCREMENT=20461 ;

