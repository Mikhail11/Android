CREATE TABLE IF NOT EXISTS `seller_list` (
  `id` bigint(20) NOT NULL AUTO_INCREMENT,
  `user_id` bigint(20) DEFAULT NULL,
  `seller_id` varchar(100) DEFAULT NULL,
  PRIMARY KEY (`id`)
) ENGINE=MyISAM  DEFAULT CHARSET=latin1 AUTO_INCREMENT=562 ;

-- --------------------------------------------------------

--
-- Table structure for table `special_request`
--

CREATE TABLE IF NOT EXISTS `special_request` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `item_color` varchar(200) DEFAULT NULL,
  `item_size` varchar(200) DEFAULT NULL,
  `item_shade` varchar(200) DEFAULT NULL COMMENT 'Độ Ánh của sản phẩm',
  `item_sku` varchar(200) DEFAULT NULL COMMENT 'Mã hàng',
  `payment_code` varchar(100) DEFAULT NULL COMMENT 'Mã hóa đơn nếu có',
  `item_id` bigint(20) DEFAULT NULL COMMENT 'Mã sản phẩm',
  `item_name` varchar(255) DEFAULT NULL COMMENT 'Tên sản phẩm',
  `item_price` float DEFAULT NULL COMMENT 'Giá sản phẩm',
  `item_condition` varchar(255) DEFAULT NULL COMMENT 'tình trạng SP: Cũ mới',
  `shipping_fee` float DEFAULT NULL COMMENT 'Phí vận chuyển',
  `shipping_fee_usvn` float DEFAULT NULL COMMENT 'Phí VC US - VN',
  `category_id` bigint(20) DEFAULT NULL COMMENT 'Danh mục sản phẩm',
  `buy_number` int(11) DEFAULT '1' COMMENT 'Số lượng yêu cầu',
  `comment` text COMMENT 'Comment người mua',
  `admin_comment` text,
  `buyer_id` int(11) DEFAULT NULL COMMENT 'User_id người yêu cầu',
  `buyer_username` varchar(200) DEFAULT NULL COMMENT 'Username người yêu cầu',
  `buyer_fullname` varchar(200) DEFAULT NULL COMMENT 'Họ và tên người yêu cầu',
  `buyer_phone` varchar(200) DEFAULT NULL COMMENT 'Số điện thoại người yêu cầu',
  `buyer_address` varchar(255) DEFAULT NULL COMMENT 'Địa chỉ người yêu cầu',
  `buyer_email` varchar(200) DEFAULT NULL COMMENT 'Email người yêu cầu',
  `buyer_zone` varchar(200) DEFAULT NULL COMMENT 'Tỉnh thành phố người yêu cầu',
  `seller_username` varchar(200) DEFAULT NULL COMMENT 'Username người bán',
  `seller_feedback_score` int(11) DEFAULT NULL COMMENT 'Điểm uy tín của người bán',
  `seller_positive_feedback_percent` varchar(200) DEFAULT NULL COMMENT '% uy tín của người bán',
  `seller_location` varchar(200) DEFAULT NULL COMMENT 'Vị trí người bán',
  `status` tinyint(3) NOT NULL DEFAULT '1' COMMENT '''1''=>''Yêu cầu chờ xử lý'', ''2''=>''Yêu cầu được chấp nhận'', ''3''=>''Yêu cầu bị Từ chối'',4=>Yêu cầu chờ người bán trả lời, 5-Chờ duyệt',
  `result_process` text COMMENT 'Kết quả xử lý',
  `request_date` bigint(20) DEFAULT NULL,
  `us_statustax` float DEFAULT NULL COMMENT '%Thuế bang',
  `item_time_end` int(11) DEFAULT NULL COMMENT 'Thời gian hết hạn của sản phẩm',
  `promotion_type` tinyint(1) DEFAULT NULL COMMENT '1:SundayOff 2: SpecialDay 3:LuckyDay',
  `promotion` varchar(1) NOT NULL DEFAULT '0' COMMENT '0:không có giảm giá 1:Có giảm giá',
  `type_user` tinyint(1) NOT NULL DEFAULT '1' COMMENT '2: không có tài khoản mua hàng, 1: Có tài khoản mua hàng',
  `buyer_tax_import` float DEFAULT NULL COMMENT '%Thue Nhap khau',
  `buyer_tax_vat` float DEFAULT NULL COMMENT 'Thue VAT',
  `buyer_tax_other` float DEFAULT NULL COMMENT 'Thue Khác',
  `buyer_tax_paid` float DEFAULT NULL COMMENT 'Tổng Thuế',
  `buyer_percentage_fee` float DEFAULT NULL COMMENT '% Phi dich vu',
  `buyer_fees` float DEFAULT NULL COMMENT 'Tong phi dich vu',
  `approval_user` varchar(255) DEFAULT NULL,
  `info_receive` text,
  `vat_infos` text,
  `has_vat` tinyint(1) NOT NULL DEFAULT '0' COMMENT '0- không vat; 1- có vat',
  PRIMARY KEY (`id`)
) ENGINE=MyISAM  DEFAULT CHARSET=latin1 AUTO_INCREMENT=41340 ;

