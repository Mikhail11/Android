--
-- Table structure for table `other_request`
--

CREATE TABLE IF NOT EXISTS `other_request` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `buyer_id` int(11) DEFAULT NULL COMMENT 'Id người mua (trong trường hợp có tài khỏan CĐT)',
  `buyer_username` varchar(200) DEFAULT NULL COMMENT ' Username người mua (trong trường hợp có tài khỏan CĐT)',
  `buyer_fullname` varchar(200) DEFAULT NULL COMMENT 'Họ và tên người mua',
  `buyer_phone` varchar(200) DEFAULT NULL COMMENT 'Số điện thoại người mua',
  `buyer_address` varchar(200) DEFAULT NULL COMMENT 'Địa chỉ người mua cũng là địa chỉ nhận hàng',
  `buyer_email` varchar(200) DEFAULT NULL COMMENT 'Email người mua',
  `buyer_zone` varchar(200) DEFAULT NULL COMMENT 'Tỉnh thành người mua',
  `seller_username` varchar(200) DEFAULT NULL COMMENT 'Username người bán ',
  `seller_feedback_score` varchar(100) DEFAULT NULL COMMENT 'Số điểm uy tín người bán',
  `seller_positive_feedback_percent` varchar(100) DEFAULT NULL COMMENT '% uy tín người bán',
  `seller_location` varchar(200) DEFAULT NULL COMMENT 'Địa chỉ người bán',
  `item_id` int(11) DEFAULT NULL,
  `item_title` varchar(250) DEFAULT NULL COMMENT 'Tiêu đề sản phẩm',
  `category_id` int(11) DEFAULT NULL,
  `item_url` text COMMENT 'Link đến sản phẩm',
  `item_num` varchar(50) DEFAULT NULL COMMENT 'Số lượng sản phẩm. Mặc định = 1',
  `item_price` float DEFAULT NULL COMMENT 'Giá sản phẩm. Tính bằng USD',
  `item_color` varchar(100) DEFAULT NULL COMMENT 'Màu sắc sản phẩm',
  `item_size` varchar(200) DEFAULT NULL COMMENT 'Kích cỡ sản phẩm',
  `item_weight` varchar(200) DEFAULT NULL COMMENT 'Cân nặng',
  `item_time_end` int(11) DEFAULT NULL,
  `buyer_comment_text` text COMMENT 'Người dùng comment',
  `admin_comment_text` text COMMENT 'Admin comment',
  `shipping_inland` float DEFAULT '0' COMMENT 'Phí vận chuyển',
  `us_statustax` float DEFAULT '0' COMMENT '% Thuế bang',
  `shipping_fee_usvn` float DEFAULT NULL,
  `import_tax` float DEFAULT NULL,
  `vat_tax` float DEFAULT NULL,
  `other_tax` float DEFAULT NULL,
  `buyer_tax_paid` double DEFAULT NULL,
  `buyer_fee` float DEFAULT NULL,
  `buyer_fees` float DEFAULT NULL,
  `time_created` int(11) NOT NULL COMMENT 'Thời gian tạo',
  `time_process` int(11) DEFAULT NULL COMMENT 'Thời gian xử lý',
  `type_shipping` tinyint(2) NOT NULL DEFAULT '1' COMMENT 'Kiểu vận chuyển. 1: Mua hộ hàng, 2: Ship hộ hàng',
  `tracking_number` varchar(200) DEFAULT NULL COMMENT 'Mã tracking với trường hợp ship hộ hàng',
  `payment_code` varchar(50) DEFAULT NULL COMMENT 'Mã hóa đơn xử lý',
  `proxy` text COMMENT 'Nơi nhận hàng tại Mỹ',
  `status` tinyint(1) NOT NULL DEFAULT '1' COMMENT 'Trạng thái yêu cầu. 1. Đang yêu cầu, 2: Yêu cầu đc chấp nhận. 3: yêu cầu bị từ chối, 4: Chờ người bán',
  `result_process` text COMMENT 'Kết quả xử lý',
  `promotion_type` tinyint(1) DEFAULT NULL COMMENT '1:SundayOff 2: SpecialDay 3:LuckyDay',
  `promotion` int(11) NOT NULL DEFAULT '0' COMMENT '0:không có giảm giá 1:Có giảm giá',
  `type_user` tinyint(4) NOT NULL DEFAULT '1' COMMENT 'Kiểu người dùng. 1: Có tài khỏan, 2: không có tài khỏan',
  `info_receive` text,
  `approval_user` varchar(255) DEFAULT NULL,
  `vat_infos` text COMMENT 'Lưu thông tin thuế VAT',
  `has_vat` tinyint(1) NOT NULL DEFAULT '0' COMMENT '0- không vat; 1- có vat',
  PRIMARY KEY (`id`)
) ENGINE=MyISAM  DEFAULT CHARSET=latin1 AUTO_INCREMENT=9255 ;

