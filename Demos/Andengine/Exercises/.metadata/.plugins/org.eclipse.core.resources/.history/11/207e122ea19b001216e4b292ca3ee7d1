-- --------------------------------------------------------

--
-- Table structure for table `report_contact`
--

CREATE TABLE IF NOT EXISTS `report_contact` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `zone_id` tinyint(2) NOT NULL COMMENT 'Mã vùng (miền bắc miền nam, miền trung)',
  `block_id` tinyint(2) NOT NULL COMMENT 'Má khối',
  `room_id` tinyint(2) NOT NULL COMMENT 'mã phòng',
  `group_id` tinyint(2) NOT NULL COMMENT 'Id của nhóm',
  `user_id` int(11) NOT NULL,
  `user_name` varchar(255) NOT NULL,
  `employee_position` tinyint(1) NOT NULL COMMENT 'Chức vụ của người liên hệ, Nhân viên, trưởng nhóm, trưởng khối',
  `name_business` varchar(255) NOT NULL COMMENT 'Tên doanh nghiêp',
  `address_business` varchar(255) NOT NULL COMMENT 'Địa chỉ doanh nghiệp',
  `career_business` varchar(255) NOT NULL COMMENT 'Lĩnh vực kinh doanh',
  `contacter` varchar(255) NOT NULL COMMENT 'Người liên hệ',
  `position_contacter` varchar(255) DEFAULT NULL COMMENT 'Chức vụ người liên hệ',
  `contact_method` tinyint(2) NOT NULL COMMENT 'Hình thức liên hệ',
  `contact_method_detail` varchar(255) NOT NULL COMMENT 'Thông tin chi tiết liên hệ',
  `service_type` int(11) NOT NULL COMMENT 'Dịch vụ cung cấp',
  `content_contact` text NOT NULL COMMENT 'Nội dung chi tiết liên hệ',
  `contract_info` text COMMENT 'Thông tin them',
  `date_sign_contract` int(11) DEFAULT NULL,
  `website` varchar(255) DEFAULT NULL COMMENT 'Web của khách hàng',
  `money` float NOT NULL DEFAULT '0',
  `contract_expire` varchar(266) DEFAULT NULL COMMENT 'Thoi han họp đong',
  `date_contact` int(11) NOT NULL COMMENT 'Ngày liên hệ',
  `flag_success` tinyint(2) NOT NULL DEFAULT '0' COMMENT 'Trang thai thanh cong hay that bai 0: dang cho, 1 that bai 2 thanh cong',
  `time_created` int(11) NOT NULL COMMENT 'ngày tạo',
  PRIMARY KEY (`id`),
  KEY `service_type` (`service_type`)
) ENGINE=MyISAM  DEFAULT CHARSET=latin1 AUTO_INCREMENT=3 ;

-- --------------------------------------------------------

--
-- Table structure for table `search_keyword`
--

CREATE TABLE IF NOT EXISTS `search_keyword` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `keyword` varchar(125) NOT NULL,
  `keyword_english` varchar(125) NOT NULL COMMENT 'Tu khoa dich tieng anh',
  `telex_keyword` varchar(125) NOT NULL,
  `last_time` int(11) NOT NULL DEFAULT '0',
  `count` int(11) NOT NULL DEFAULT '0' COMMENT 'Tong so luot search',
  `total_item` int(11) NOT NULL COMMENT 'Tong so san pham tim thay o chodientu',
  `total_item_ebay` int(11) NOT NULL COMMENT 'Tong so san pham tim thay o ebay',
  `total_by_category` varchar(255) NOT NULL COMMENT 'Tong so san pham theo danh muc',
  `type` tinyint(4) NOT NULL COMMENT '1 danh muc, 2 thuong hieu, 3 model',
  `type_wrong` tinyint(4) NOT NULL COMMENT '0 tu khoa hop le, 1 la tu khoa ko hop le',
  `sub_id` int(11) NOT NULL COMMENT 'ma danh muc hoac ma thuong hieu hoac ma model',
  PRIMARY KEY (`id`),
  KEY `last_time` (`last_time`),
  FULLTEXT KEY `telex_keyword` (`telex_keyword`)
) ENGINE=MyISAM  DEFAULT CHARSET=latin1 AUTO_INCREMENT=2101226 ;

-- --------------------------------------------------------

--
-- Table structure for table `search_keyword_backup_log`
--

CREATE TABLE IF NOT EXISTS `search_keyword_backup_log` (
  `id` int(10) unsigned NOT NULL AUTO_INCREMENT,
  `file` varchar(23) NOT NULL DEFAULT '',
  `endTime` int(10) unsigned NOT NULL DEFAULT '0' COMMENT '> 0 -> đã hoàn thành 100%',
  `keywords` int(10) unsigned NOT NULL DEFAULT '0',
  `kpr` tinyint(3) unsigned NOT NULL DEFAULT '1' COMMENT '= 0 -> (hủy backuping và xóa file) || (restoring)',
  `kpl` tinyint(3) unsigned NOT NULL DEFAULT '1' COMMENT '= 0 -> hủy restoring',
  PRIMARY KEY (`id`),
  KEY `file` (`file`),
  KEY `endTime` (`endTime`),
  KEY `kpr` (`kpr`),
  KEY `kpl` (`kpl`)
) ENGINE=MyISAM  DEFAULT CHARSET=utf8 AUTO_INCREMENT=31 ;

-- --------------------------------------------------------

--
-- Table structure for table `search_keyword_import_log`
--

CREATE TABLE IF NOT EXISTS `search_keyword_import_log` (
  `id` int(10) unsigned NOT NULL AUTO_INCREMENT,
  `file` varchar(32) NOT NULL DEFAULT '' COMMENT '= file, category, manufacture, model',
  `start_date` mediumint(8) unsigned NOT NULL DEFAULT '0',
  `end_date` mediumint(8) unsigned NOT NULL DEFAULT '0',
  `fieldsearch` varchar(32) NOT NULL DEFAULT '0',
  `fieldoriginal` varchar(32) NOT NULL DEFAULT '0',
  `keywords` int(10) unsigned NOT NULL DEFAULT '0' COMMENT '= 0 chay tu dau',
  `insertformule` varchar(255) NOT NULL DEFAULT '',
  `updateformule` varchar(255) NOT NULL DEFAULT '',
  PRIMARY KEY (`id`),
  KEY `file` (`file`),
  KEY `keywords` (`keywords`)
) ENGINE=MyISAM  DEFAULT CHARSET=utf8 AUTO_INCREMENT=37 ;

