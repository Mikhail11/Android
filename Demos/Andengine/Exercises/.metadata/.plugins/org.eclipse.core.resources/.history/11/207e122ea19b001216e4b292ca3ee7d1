CREATE TABLE IF NOT EXISTS `category` (
  `id` int(4) unsigned NOT NULL AUTO_INCREMENT,
  `name` varchar(100) COLLATE utf8_unicode_ci NOT NULL,
  `parent_id` int(4) unsigned NOT NULL,
  `category_id_ebay` int(11) NOT NULL COMMENT 'Ma DM map voi ebay',
  `keyword_ebay` varchar(255) COLLATE utf8_unicode_ci NOT NULL COMMENT 'Tu khoa dung de map voi ebay',
  `path` varchar(100) COLLATE utf8_unicode_ci NOT NULL,
  `level` tinyint(1) NOT NULL,
  `isleaf` tinyint(1) NOT NULL DEFAULT '0',
  `display_default` tinyint(1) NOT NULL,
  `is_hitech` tinyint(1) NOT NULL,
  `sname` varchar(100) COLLATE utf8_unicode_ci NOT NULL,
  `ordering` tinyint(2) NOT NULL DEFAULT '0' COMMENT 'trường sắp xếp',
  `total_item` mediumint(8) unsigned NOT NULL COMMENT 'Tong so san pham trong danh muc',
  `weight` int(11) NOT NULL DEFAULT '0' COMMENT 'tinh theo gram',
  PRIMARY KEY (`id`),
  KEY `parent_id` (`parent_id`)
) ENGINE=MyISAM  DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci COMMENT='Cây danh mục' AUTO_INCREMENT=4829 ;

