-- --------------------------------------------------------

--
-- Table structure for table `site`
--

CREATE TABLE IF NOT EXISTS `site` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `seller` varchar(255) NOT NULL COMMENT 'tên tài khoản nguoi ban ',
  `seller_id` int(11) NOT NULL,
  `crawler` varchar(100) NOT NULL COMMENT 'Tai khoan nhan vien crawl',
  `type` tinyint(1) NOT NULL COMMENT 'Loai website 1 la A, 2 la B, 3 la C, 4 la D',
  `name_site` varchar(255) NOT NULL COMMENT 'tên site',
  `path_site` varchar(255) NOT NULL COMMENT 'đừng dẫn site',
  `prefix` varchar(255) NOT NULL COMMENT 'tiền tô',
  `time_created` int(11) DEFAULT NULL,
  `status` tinyint(1) DEFAULT NULL COMMENT 'Trang thai crawl 1 cho trien khai, 2 la dang trien khai, 3 la Da hoan thanh',
  `status_map` tinyint(1) NOT NULL COMMENT 'Trang thai map catalog 0 la chua map, 1 la dang map 2 la da map hoan thanh',
  `status_update` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'Trang thai crawl: 0 chay thu cong, 1 chay tu dong',
  `desc` varchar(500) NOT NULL COMMENT 'Mo ta them',
  `expire_time` int(11) NOT NULL COMMENT 'Tthoi gia hoan thanh',
  `last_time_check_price` int(11) NOT NULL COMMENT 'Thoi gian scan thanh cong',
  `last_time_check_new_item` int(11) NOT NULL COMMENT 'Thoi gian kiem tra sp moi gan day nhat',
  `total_check_price` smallint(11) NOT NULL COMMENT 'Tong so lan kiem tra gia',
  `total_check_new_item` smallint(11) NOT NULL COMMENT 'Tong so lan kiem tra sp moi',
  `frequency_update` smallint(7) NOT NULL COMMENT 'Tần xuất update (Bao nhieu ngay chay 1 lan)',
  `frequency_new` smallint(7) NOT NULL COMMENT 'Tan xuat kiem tra sp moi (Bao nhieu ngay chay kiem tra sp moi 1 lan)',
  PRIMARY KEY (`id`)
) ENGINE=MyISAM  DEFAULT CHARSET=latin1 COMMENT='quản trị site' AUTO_INCREMENT=3906 ;

-- --------------------------------------------------------

--
-- Table structure for table `site_fix`
--

CREATE TABLE IF NOT EXISTS `site_fix` (
  `id` int(4) NOT NULL AUTO_INCREMENT,
  `site_id` int(4) NOT NULL,
  `fixImage` varchar(600) COLLATE utf8_unicode_ci NOT NULL,
  `fixDesc` varchar(600) COLLATE utf8_unicode_ci NOT NULL,
  `fixPrice` varchar(600) COLLATE utf8_unicode_ci NOT NULL,
  `fixCurrencyid` varchar(600) COLLATE utf8_unicode_ci NOT NULL,
  `fixTitle` varchar(600) COLLATE utf8_unicode_ci NOT NULL,
  PRIMARY KEY (`id`)
) ENGINE=MyISAM  DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci AUTO_INCREMENT=3246 ;

-- --------------------------------------------------------

--
-- Table structure for table `site_thread`
--

CREATE TABLE IF NOT EXISTS `site_thread` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `user_created` varchar(50) DEFAULT NULL COMMENT 'nguoi tao ra thread',
  `name` varchar(250) NOT NULL COMMENT 'Ten luong',
  `site_id` int(11) NOT NULL,
  `category_id` int(11) NOT NULL,
  `e_category_id` int(11) NOT NULL,
  `time_created` int(11) NOT NULL,
  `status` tinyint(1) NOT NULL COMMENT 'Trang thai 1 cho trien khai, 2 la dang trien khai, 3 la Da hoan thanh',
  `total_day` int(11) NOT NULL,
  `total_item` smallint(11) NOT NULL COMMENT 'Tong so sp ',
  `shipping_type` tinyint(1) DEFAULT NULL COMMENT 'hinh thuc van chuyen, mien phi, co dinh, theo bang gia',
  `shipping_fee` double DEFAULT NULL COMMENT 'phi van chuyen co dinh',
  `long_width_height` varchar(30) DEFAULT NULL COMMENT 'truong luu chieu dai, chieu rong, chieu cao cua san pham theo cau chuc (long;width;height)',
  `weight` double DEFAULT NULL COMMENT 'Trong luong cua sp',
  `is_copy` tinyint(1) NOT NULL DEFAULT '0' COMMENT '1 la da copy tu crawl sang kho, 0 la chua copy',
  PRIMARY KEY (`id`)
) ENGINE=MyISAM  DEFAULT CHARSET=latin1 AUTO_INCREMENT=75067 ;
