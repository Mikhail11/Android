--
-- Table structure for table `log_action`
--

CREATE TABLE IF NOT EXISTS `log_action` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `time` int(11) DEFAULT NULL,
  `action` varchar(200) COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'Register/ActiveEmail/VerifySMS/Bid/Bargain/Buy/Sell/List/PayNL/Refund/Login/   BuyEbay/PayNLEbay/BidEbay/RefundEbay',
  `user_id` varchar(100) COLLATE utf8_unicode_ci NOT NULL,
  `seller_id` int(11) DEFAULT NULL,
  `email` varchar(200) COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'Email người mua',
  `mobile` varchar(20) COLLATE utf8_unicode_ci DEFAULT NULL,
  `category_id` int(11) DEFAULT NULL,
  `item_id` varchar(200) COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'Chuỗi mã sản phẩm',
  `item_name` varchar(200) COLLATE utf8_unicode_ci DEFAULT NULL,
  `order_id` int(11) DEFAULT NULL,
  `payment_method` tinyint(1) NOT NULL COMMENT 'hinh thuc thanh toan qua ngan luong hay ko',
  `item_total_quantity` int(11) DEFAULT NULL COMMENT 'SL mua',
  `item_total_value` int(11) DEFAULT NULL COMMENT 'Tổng giá mua',
  `item_shop_level` int(11) DEFAULT NULL COMMENT 'Level shop lúc mua',
  `item_type` int(11) DEFAULT NULL COMMENT 'đăng theo mặc cả (3), đấu giá (2), mua ngay(1)',
  `item_buying_type` int(11) DEFAULT NULL COMMENT 'mua theo mặc cả (3), đấu giá (2), mua ngay(1)',
  `year` int(4) NOT NULL DEFAULT '2010' COMMENT 'Năm',
  PRIMARY KEY (`id`,`year`)
) ENGINE=MyISAM  DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci
/*!50100 PARTITION BY HASH (`year`)
PARTITIONS 20 */ AUTO_INCREMENT=2428614 ;

