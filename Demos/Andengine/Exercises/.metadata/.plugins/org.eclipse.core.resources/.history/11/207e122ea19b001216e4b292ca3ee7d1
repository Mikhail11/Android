  `verified_sms` tinyint(1) DEFAULT NULL,
  `active_email` tinyint(1) DEFAULT NULL,
  `verified_cmtnd` tinyint(1) DEFAULT NULL,
  `mapping_nganluong` tinyint(1) DEFAULT NULL,
  `user_type` tinyint(1) DEFAULT NULL,
  `openid_status` tinyint(1) DEFAULT NULL,
  `site_id` tinyint(1) DEFAULT NULL,
  `zone_id` int(11) DEFAULT '0',
  `slogal` varchar(200) DEFAULT NULL,
  `avarta` varchar(200) DEFAULT NULL,
  `display_info` tinyint(1) DEFAULT NULL,
  `lock` tinyint(1) DEFAULT '0',
  `is_staff` int(11) DEFAULT NULL,
  `time_register` int(11) DEFAULT NULL,
  `time_mapping_nganluong` int(11) DEFAULT NULL,
  `time_last_login` int(11) DEFAULT '0',
  `time_register_shop` int(11) DEFAULT NULL,
  `star` tinyint(1) DEFAULT NULL,
  `score` int(4) DEFAULT NULL,
  `total_vote` int(4) DEFAULT NULL,
  `good_vote` int(4) DEFAULT NULL,
  `groups` text,
  `password` varchar(100) NOT NULL DEFAULT '',
  `birth_date` date DEFAULT NULL,
  `address` varchar(200) DEFAULT NULL,
  `join_date` int(11) NOT NULL DEFAULT '0',
  `time` int(11) NOT NULL DEFAULT '0',
  `active` tinyint(1) NOT NULL DEFAULT '0',
  `nick_skype` varchar(50) DEFAULT NULL,
  `nick_ym` varchar(50) DEFAULT NULL,
  `lock_reason` text NOT NULL,
  `acc_bank` text NOT NULL,
  `type` int(1) NOT NULL,
  `nganluong_email` varchar(100) NOT NULL,
  `pass_code` varchar(10) NOT NULL,
  PRIMARY KEY (`id`),
  UNIQUE KEY `user_name` (`user_name`),
  KEY `zone_id` (`zone_id`)
) ENGINE=MyISAM  DEFAULT CHARSET=latin1 PACK_KEYS=0 AUTO_INCREMENT=1000122 ;

