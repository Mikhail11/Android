  `id` int(11) NOT NULL AUTO_INCREMENT,
  `title` varchar(255) COLLATE utf8_unicode_ci NOT NULL COMMENT 'tiêu đề alias',
  `description` text COLLATE utf8_unicode_ci COMMENT 'mô tả thông tin Alias',
  `category_id` int(11) NOT NULL COMMENT 'Danh mục chứa alias',
  `status` tinyint(1) NOT NULL DEFAULT '1' COMMENT '0: không hiển thị; 1: Hiển thị',
  `position` int(5) DEFAULT NULL COMMENT 'Sắp xếp',
  `site_type` smallint(1) NOT NULL,
  PRIMARY KEY (`id`)
) ENGINE=MyISAM  DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci AUTO_INCREMENT=53 ;

