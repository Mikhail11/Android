--
-- Table structure for table `cm_featured_modules`
--

CREATE TABLE IF NOT EXISTS `cm_featured_modules` (
  `id` int(11) unsigned NOT NULL AUTO_INCREMENT,
  `id_parent` int(11) unsigned NOT NULL DEFAULT '0',
  `title` varchar(255) COLLATE utf8_unicode_ci NOT NULL,
  `big_image` varchar(255) COLLATE utf8_unicode_ci DEFAULT NULL,
  `small_image` varchar(255) COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'có thể có hoặc không',
  `type` tinyint(1) NOT NULL DEFAULT '1' COMMENT '1-8 item, 2-9 model',
  `status` tinyint(1) NOT NULL DEFAULT '1' COMMENT '1: Hiển thị, 2: Ẩn',
  `url` varchar(255) COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'liên kết module',
  `sort` int(10) NOT NULL DEFAULT '1' COMMENT 'thứ tự module',
  `admin_id` int(10) unsigned NOT NULL DEFAULT '0' COMMENT 'id người cập nhật',
  `created` int(10) unsigned NOT NULL,
  `updated` int(10) unsigned NOT NULL,
  PRIMARY KEY (`id`),
  KEY `id_parent` (`id_parent`),
  KEY `sort` (`sort`),
  KEY `created` (`created`),
  KEY `updated` (`updated`),
  KEY `type` (`type`),
  KEY `admin_id` (`admin_id`)
) ENGINE=MyISAM  DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci AUTO_INCREMENT=49 ;

