  `id` int(11) unsigned NOT NULL AUTO_INCREMENT,
  `order_id` int(11) NOT NULL COMMENT 'id hoa don',
  `user_id` int(11) DEFAULT NULL COMMENT 'id nguoi mua',
  `full_name` varchar(512) COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'ho ten nguoi nhan',
  `mobile_phone` varchar(20) COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'so dien thoai nguoi nhan',
  `address_receiver` text COLLATE utf8_unicode_ci COMMENT 'dia chi nguoi nhan',
  `zone_id` int(11) NOT NULL COMMENT 'khu vuc cua nguoi nhan',
  `total_apply` smallint(3) DEFAULT '1' COMMENT 'tong so lan ap dung',
  PRIMARY KEY (`id`),
  KEY `order_id` (`order_id`,`user_id`,`zone_id`)
) ENGINE=MyISAM  DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci AUTO_INCREMENT=271 ;

-- --------------------------------------------------------

--
-- Table structure for table `order_sc`
--

CREATE TABLE IF NOT EXISTS `order_sc` (
  `id` int(10) unsigned NOT NULL AUTO_INCREMENT COMMENT 'mã vận đơn',
  `approver_id` int(10) unsigned DEFAULT NULL COMMENT 'id người duyệt',
  `order_id` int(10) unsigned NOT NULL COMMENT 'mã hóa đơn',
  `shipchung_code` varchar(55) COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'Mã vận đơn bên ShipChung gửi sang khi tạo vận đơn thành công',
  `carrier_id` int(10) unsigned DEFAULT NULL COMMENT 'Mã hãng vận chuyển',
  `receiver_cmtnd` varchar(11) COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'Số chứng minh thư người nhận',
  `receiver_city_id` int(11) unsigned DEFAULT NULL COMMENT 'Mã thành phố người nhận',
  `receiver_district_id` int(11) unsigned DEFAULT NULL COMMENT 'Mã quận/Huyện người nhận',
  `fee_sc` int(10) unsigned NOT NULL DEFAULT '0' COMMENT 'PVC tính theo bảng giá SC chỉ các SP dùng SC',
  `fee_sc_total` int(11) unsigned DEFAULT NULL COMMENT 'Phí SC của toàn bộ sản phẩm thực tế',
  `fee_nbqd` int(10) unsigned NOT NULL DEFAULT '0' COMMENT 'PVC người bán quy đinh',
  `fee_cod` int(10) unsigned NOT NULL DEFAULT '0' COMMENT 'phí COD',
  `fee_alert` int(11) unsigned NOT NULL DEFAULT '0' COMMENT 'phí báo phát',
  `fee_master` int(11) unsigned NOT NULL DEFAULT '0' COMMENT 'phí đúng ng nhận',
  `fee_gdtc` int(10) unsigned NOT NULL DEFAULT '0' COMMENT 'phí giao dịch thành công',
  `fee_total_nbqd_shipping` int(11) unsigned NOT NULL DEFAULT '0' COMMENT '∑PVC theo bảng giá SC + ∑ PVC cố định NBQD + ∑VAS - phi COD',
  `total_seller_receive` int(10) NOT NULL COMMENT 'tổng tiền NB nhận đc',
  `total_buyer_pay` int(10) unsigned NOT NULL COMMENT 'tổng tiền NM phải trả',
  `total_weight` int(10) unsigned NOT NULL COMMENT 'tổng khối lượng hàng hóa',
  `total_weight_SC` int(11) unsigned NOT NULL DEFAULT '0',
  `shipping_company` varchar(30) COLLATE utf8_unicode_ci NOT NULL COMMENT 'hãng vận chuyển',
  `status` tinyint(2) NOT NULL DEFAULT '1' COMMENT 'Trạng thái vận đơn.',
  `nganluong_transaction_id` int(10) unsigned DEFAULT NULL COMMENT 'Mã giao dịch chuyển tiền cho người bán từ GMV',
  `last_update` int(10) unsigned NOT NULL,
  `partition_month` tinyint(2) unsigned NOT NULL COMMENT 'partioning theo tháng',
  PRIMARY KEY (`id`,`partition_month`),
  KEY `partition_month` (`partition_month`),
  KEY `fee_sc` (`fee_sc`),
  KEY `fee_nbqd` (`fee_nbqd`),
  KEY `fee_alert` (`fee_alert`),
  KEY `fee_master` (`fee_master`),
  KEY `fee_gdtc` (`fee_gdtc`),
  KEY `fee_total_nbqd_shipping` (`fee_total_nbqd_shipping`),
  KEY `total_seller_receive` (`total_seller_receive`),
  KEY `total_buyer_pay` (`total_buyer_pay`),
  KEY `total_weight` (`total_weight`),
  KEY `status` (`status`),
  KEY `last_update` (`last_update`),
  KEY `carrier_id` (`carrier_id`),
  KEY `total_weight_SC` (`total_weight_SC`)
) ENGINE=MyISAM  DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci COMMENT='bảng vận đơn'
/*!50100 PARTITION BY LIST (`partition_month`)
(PARTITION p1 VALUES IN (1) ENGINE = MyISAM,
 PARTITION p2 VALUES IN (2) ENGINE = MyISAM,
 PARTITION p3 VALUES IN (3) ENGINE = MyISAM,
 PARTITION p4 VALUES IN (4) ENGINE = MyISAM,
 PARTITION p5 VALUES IN (5) ENGINE = MyISAM,
 PARTITION p6 VALUES IN (6) ENGINE = MyISAM,
 PARTITION p7 VALUES IN (7) ENGINE = MyISAM,
 PARTITION p8 VALUES IN (8) ENGINE = MyISAM,
 PARTITION p9 VALUES IN (9) ENGINE = MyISAM,
 PARTITION p10 VALUES IN (10) ENGINE = MyISAM,
 PARTITION p11 VALUES IN (11) ENGINE = MyISAM,
 PARTITION p12 VALUES IN (12) ENGINE = MyISAM) */ AUTO_INCREMENT=45 ;

-- --------------------------------------------------------

--
-- Table structure for table `order_shortlink`
--

CREATE TABLE IF NOT EXISTS `order_shortlink` (
  `order_id` int(11) NOT NULL,
  `token_key` varchar(32) COLLATE utf8_unicode_ci DEFAULT '',
  `hash_code` varchar(5) COLLATE utf8_unicode_ci NOT NULL,
  PRIMARY KEY (`order_id`),
  UNIQUE KEY `hash_code` (`hash_code`)
) ENGINE=MyISAM DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci;

-- --------------------------------------------------------

--
-- Table structure for table `user_feedback`
--

CREATE TABLE IF NOT EXISTS `user_feedback` (
  `id` int(11) NOT NULL,
  `user_id` int(11) NOT NULL COMMENT 'id nguoi danh gia',
  `user_name` varchar(50) DEFAULT NULL,
  `receiver_id` int(11) NOT NULL COMMENT 'id nguoi duoc danh gia',
  `receiver_username` varchar(50) DEFAULT NULL,
  `order_id` int(11) DEFAULT NULL COMMENT 'id hoa don duoc danh gia',
  `order_code` varchar(30) NOT NULL COMMENT 'Ma hoa don',
  `time` int(11) DEFAULT NULL COMMENT 'thoi gian tao',
  `time_vote` int(11) DEFAULT NULL COMMENT 'thoi gian danh gia',
  `point` tinyint(4) DEFAULT '0' COMMENT 'danh gia: 1 la tot, 2 la trung binh, 3 la xau',
  `comment` varchar(255) CHARACTER SET utf8 COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'loi nhan xet di kem',
  `status` tinyint(4) DEFAULT NULL COMMENT 'trang thai da danh gia hay chua',
  `seller_or_buyer` tinyint(1) NOT NULL DEFAULT '1' COMMENT '1 la uy tin ban (bang user_feedback_buyer), 2 la uy tin mua (trong bang user_feedback_seller)',
  KEY `user_id` (`user_id`)
) ENGINE=MyISAM DEFAULT CHARSET=latin1
/*!50100 PARTITION BY HASH (user_id)
PARTITIONS 100 */;

-- --------------------------------------------------------

--
-- Table structure for table `user_feedback_buyer`
--

CREATE TABLE IF NOT EXISTS `user_feedback_buyer` (
  `id` int(11) NOT NULL,
  `user_id` int(11) NOT NULL COMMENT 'id nguoi danh gia',
  `user_name` varchar(50) DEFAULT NULL,
  `receiver_id` int(11) NOT NULL COMMENT 'id nguoi duoc danh gia',
  `receiver_username` varchar(50) DEFAULT NULL,
  `order_id` int(11) DEFAULT NULL COMMENT 'id hoa don duoc danh gia',
  `order_code` varchar(20) DEFAULT NULL,
  `time` int(11) DEFAULT NULL COMMENT 'thoi gian tao',
  `time_vote` int(11) DEFAULT NULL COMMENT 'thoi gian danh gia',
  `point` tinyint(4) DEFAULT '0' COMMENT 'danh gia: 1 la tot, 2 la trung binh, 3 la xau',
  `comment` varchar(255) CHARACTER SET utf8 COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'loi nhan xet di kem',
  `status` tinyint(4) DEFAULT NULL COMMENT 'trang thai da danh gia hay chua',
  PRIMARY KEY (`id`,`user_id`,`receiver_id`),
  KEY `time` (`time`,`time_vote`,`point`,`status`),
  KEY `received_time_vote` (`receiver_id`,`time_vote`)
) ENGINE=MyISAM DEFAULT CHARSET=latin1
/*!50100 PARTITION BY HASH (receiver_id)
PARTITIONS 100 */;

-- --------------------------------------------------------

--
-- Table structure for table `user_feedback_index`
--

CREATE TABLE IF NOT EXISTS `user_feedback_index` (
  `id` int(11) unsigned NOT NULL AUTO_INCREMENT COMMENT 'id feedback',
  `index_table` tinyint(1) NOT NULL DEFAULT '0' COMMENT '1 bang user_feedback_buyer, 2 user_feedback_seller',
  `user_id` int(11) unsigned NOT NULL COMMENT 'id nguoi di danh gia',
  `receiver_id` int(11) unsigned NOT NULL COMMENT 'user id cua nguoi bi danh gia',
  PRIMARY KEY (`id`),
  KEY `index_table` (`index_table`,`user_id`,`receiver_id`)
) ENGINE=MyISAM  DEFAULT CHARSET=latin1 AUTO_INCREMENT=185612 ;

-- --------------------------------------------------------

--
-- Table structure for table `user_feedback_log`
--

CREATE TABLE IF NOT EXISTS `user_feedback_log` (
  `id` int(11) unsigned NOT NULL AUTO_INCREMENT,
  `feedback_id` int(11) unsigned NOT NULL COMMENT 'id feedback',
  `user_id` int(11) unsigned NOT NULL COMMENT 'id nguoi thay doi',
  `content` text COLLATE utf8_unicode_ci NOT NULL COMMENT 'noi dung thay doi',
  PRIMARY KEY (`id`)
) ENGINE=MyISAM  DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci AUTO_INCREMENT=6 ;

-- --------------------------------------------------------

--
-- Table structure for table `user_feedback_seller`
--

CREATE TABLE IF NOT EXISTS `user_feedback_seller` (
  `id` int(11) NOT NULL,
  `user_id` int(11) NOT NULL COMMENT 'id nguoi danh gia',
  `user_name` varchar(50) DEFAULT NULL,
  `receiver_id` int(11) NOT NULL COMMENT 'id nguoi duoc danh gia',
  `receiver_username` varchar(50) DEFAULT NULL,
  `order_id` int(11) DEFAULT NULL COMMENT 'id hoa don duoc danh gia',
  `order_code` varchar(20) DEFAULT NULL,
  `time` int(11) NOT NULL DEFAULT '0' COMMENT 'thoi gian tao',
  `time_vote` int(11) NOT NULL DEFAULT '0' COMMENT 'thoi gian danh gia',
  `point` tinyint(4) DEFAULT '0' COMMENT 'danh gia: 1 la tot, 2 la trung binh, 3 la xau',
  `comment` varchar(255) CHARACTER SET utf8 COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'loi nhan xet di kem',
  `status` tinyint(4) DEFAULT NULL COMMENT 'trang thai da danh gia hay chua',
  KEY `receiver_id` (`receiver_id`)
) ENGINE=MyISAM DEFAULT CHARSET=latin1
/*!50100 PARTITION BY HASH (receiver_id)
PARTITIONS 100 */;

-- --------------------------------------------------------

--
-- Table structure for table `user_feedback_static`
--

CREATE TABLE IF NOT EXISTS `user_feedback_static` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `user_id` int(11) NOT NULL,
  `total_seller_feedback_bad` int(4) NOT NULL,
  `total_seller_feedback_good` int(4) NOT NULL,
  `total_seller_feedback_normal` int(4) NOT NULL,
  `total_buyer_feedback_bad` int(4) NOT NULL,
  `total_buyer_feedback_good` int(4) NOT NULL,
  `total_buyer_feedback_normal` int(4) NOT NULL,
  PRIMARY KEY (`id`),
  KEY `user_id` (`user_id`,`total_seller_feedback_bad`,`total_seller_feedback_good`,`total_seller_feedback_normal`,`total_buyer_feedback_good`,`total_buyer_feedback_bad`,`total_buyer_feedback_normal`)
) ENGINE=MyISAM  DEFAULT CHARSET=latin1 AUTO_INCREMENT=310 ;
--
-- Database: `cdt20_partition`
--
CREATE DATABASE `cdt20_partition` DEFAULT CHARACTER SET latin1 COLLATE latin1_swedish_ci;
USE `cdt20_partition`;

-- --------------------------------------------------------

--
-- Table structure for table `item_bargaining`
--

CREATE TABLE IF NOT EXISTS `item_bargaining` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `item_id` int(10) unsigned NOT NULL COMMENT 'ma sp',
  `bargain_user_id` int(11) NOT NULL COMMENT 'id cua nguoi ban',
  `seller_user_id` int(11) NOT NULL COMMENT 'id cua nguoi ban',
  `bargain_price` double NOT NULL COMMENT 'Gia mac ca',
  `amount_item` smallint(6) NOT NULL COMMENT 'So luong sp',
  `bargain_time` int(10) unsigned NOT NULL COMMENT 'Thoi gian mac ca',
  `expire_time` int(4) NOT NULL COMMENT 'Thoi han mac ca',
  `bargain_accept` tinyint(4) NOT NULL COMMENT 'Co trang thai chap nhan mac ca hoac khong, 1 la dong y',
  `del_bargain` tinyint(4) NOT NULL COMMENT 'Co xoa trang thai 1 mac ca, 1 la rut lai mac ca, 2 la xoa trang thai trong dang mua, 4 la xoa trang thai trong mac ca cho duyet cua nguoi ban',
  `type` tinyint(4) DEFAULT NULL COMMENT 'kieu mac ca, 1 la mac ca tu do 0 la mac ca thuong',
  `is_seller` tinyint(4) NOT NULL COMMENT '1 la mac ca do Nguoi Ban gui, 0 la mac ca do Nguoi Mua gui',
  `order_id` int(11) DEFAULT '0' COMMENT 'ma hoa don tuong ung',
  `is_new` tinyint(1) NOT NULL DEFAULT '1' COMMENT 'danh dau dat gia moi nhat cua sp va nguoi mua',
  PRIMARY KEY (`id`),
  KEY `item_id` (`item_id`),
  KEY `bargain_user_id` (`bargain_user_id`),
  KEY `seller_user_id` (`seller_user_id`)
) ENGINE=MyISAM  DEFAULT CHARSET=latin1 COMMENT='Bang luu cac mac' AUTO_INCREMENT=435 ;

-- --------------------------------------------------------

--
-- Table structure for table `item_bargaining_info`
--

CREATE TABLE IF NOT EXISTS `item_bargaining_info` (
  `id` int(11) NOT NULL COMMENT 'Ma id cua mac ca',
  `content` text NOT NULL COMMENT 'noi dung comment',
  `ip_address` varchar(255) NOT NULL COMMENT 'dia chi ip tien hanh dat mac ca',
  PRIMARY KEY (`id`)
) ENGINE=MyISAM DEFAULT CHARSET=latin1 COMMENT='Luu comment, dia chi ip cac mac ca';

-- --------------------------------------------------------

--
-- Table structure for table `item_bargaining_the_end`
--

CREATE TABLE IF NOT EXISTS `item_bargaining_the_end` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `item_id` int(11) NOT NULL COMMENT 'Ma san pham',
  `sender_id` int(11) NOT NULL COMMENT 'Nguoi gui yeu cau ket thuc mac ca',
  `recieve_id` int(11) NOT NULL COMMENT 'Nguoi nhan yeu cau ket thuc mac ca',
  `time_created` int(11) NOT NULL COMMENT 'Thoi gian yeu cau ket thuc mac ca',
  `comment` varchar(1000) DEFAULT NULL COMMENT 'Nhap ly do ko cho phep mac ca',
  PRIMARY KEY (`id`)
) ENGINE=MyISAM  DEFAULT CHARSET=latin1 AUTO_INCREMENT=39 ;

-- --------------------------------------------------------

--
-- Table structure for table `item_bidding_current`
--

CREATE TABLE IF NOT EXISTS `item_bidding_current` (
  `id` int(10) unsigned NOT NULL,
  `item_id` int(10) unsigned NOT NULL,
  `seller_user_id` int(11) NOT NULL COMMENT 'id user cua Nguoi Ban',
  `bid_user_id` int(11) NOT NULL COMMENT 'id cua user dat gia',
  `bid_price` double NOT NULL COMMENT 'Gia cua mot lan dat gia',
  `bid_time` int(10) unsigned NOT NULL COMMENT 'Thoi gian dat gia',
  `bid_allow` tinyint(4) NOT NULL COMMENT 'trang thai duyet dat gia 1 la dang hien thi, 0 la ko cho phep hien thi',
  `del_bid` tinyint(4) NOT NULL COMMENT 'co xoa trang thai cua 1 bid',
  `bid_type` tinyint(4) NOT NULL COMMENT '1 autobid, 0 la dat thuong',
  `ip_address` varchar(255) NOT NULL,
  KEY `item_id` (`item_id`),
  KEY `bid_allow` (`bid_allow`)
) ENGINE=MyISAM DEFAULT CHARSET=latin1 COMMENT='luu cac dat gia 3 thang gan day duoc partion 4 theo item_id '
/*!50100 PARTITION BY HASH (item_id)
PARTITIONS 6 */;

