--
-- Table structure for table `blog_categories`
--

CREATE TABLE IF NOT EXISTS `blog_categories` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `title` varchar(255) COLLATE utf8_unicode_ci NOT NULL,
  `status` int(1) NOT NULL DEFAULT '1' COMMENT '0: không hiển thị; 1: Hiển thị',
  `site_type` smallint(1) NOT NULL,
  `time_created` int(11) NOT NULL COMMENT 'thời gian tạo danh mục.',
  `position` int(2) DEFAULT NULL COMMENT 'vị trí sắp xếp',
  `type` int(1) NOT NULL DEFAULT '1' COMMENT '1: danh mục tin; 2: danh mục trợ giúp',
  `brief` text COLLATE utf8_unicode_ci NOT NULL COMMENT 'Mô tả thông tin danh mục',
  `description` text COLLATE utf8_unicode_ci COMMENT 'Mô tả',
  `parent_id` int(4) NOT NULL,
  `show_home` tinyint(1) NOT NULL,
  `level` int(1) NOT NULL COMMENT 'cấp danh mục',
  PRIMARY KEY (`id`)
) ENGINE=MyISAM  DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci AUTO_INCREMENT=146 ;

