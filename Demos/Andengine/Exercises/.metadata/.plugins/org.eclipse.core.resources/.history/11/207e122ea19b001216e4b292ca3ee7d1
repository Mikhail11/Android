--
-- Table structure for table `item`
--

CREATE TABLE IF NOT EXISTS `item` (
  `id` int(10) unsigned NOT NULL,
  `category_id` int(10) unsigned DEFAULT '0' COMMENT 'Id cua danh muc san pham',
  `is_smart_phone` tinyint(4) NOT NULL COMMENT '1 la smart phone; 0 bt',
  `search_text` varchar(400) COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'Truong tim kiem fulltext bao  gom title, hang, thuoc tinh',
  `e_category_id_bk` int(11) DEFAULT '0' COMMENT 'Danh muc GHCN',
  `price_order` double NOT NULL DEFAULT '0' COMMENT 'Gia dung de sap xep. Neu la USD = Gia*tygia',
  `is_not_price` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'Lam co de day cac SP co gia = 0 xuong duoi cung',
  `is_promotional` tinyint(4) DEFAULT '0' COMMENT 'Co khuyen mai',
  `estore_id` int(10) unsigned DEFAULT '0' COMMENT 'Ma GHCN',
  `time_for_return` tinyint(3) unsigned DEFAULT '0' COMMENT 'Cho phep tra lai hang trong x ngay',
  `warrenty` smallint(5) unsigned DEFAULT '0' COMMENT 'Thong tin bao hanh, nhap theo (theo so ngay bao hanh)',
  `shipping_type` tinyint(4) DEFAULT '0' COMMENT 'Hinh thuc van chuyen: mien phi, co dinh, hang  3: phí cố định, 2: phi shipchung, 1: miễn phí, 4: thỏa thuận',
  `status` smallint(4) unsigned NOT NULL DEFAULT '1' COMMENT 'Tinh trang duyet san pham. 1: đã được duyệt, 7: chờ duyệt, còn lại là chưa được duyệt.',
  `seller_score` smallint(6) DEFAULT '0' COMMENT 'Dien chi so cua seller, dung de tiinh item_score',
  `hit_count` smallint(5) unsigned NOT NULL COMMENT 'tong so luot xem',
  `total_like` smallint(5) unsigned NOT NULL COMMENT 'tong so luot uu thich',
  `total_track` smallint(5) unsigned NOT NULL COMMENT 'Tong so luot theo doi',
  `total_order` smallint(5) unsigned NOT NULL COMMENT 'Tong so hoa don phat sinh',
  `total_comment` smallint(5) unsigned NOT NULL COMMENT 'Tong so comment cua 1 san pham',
  `site_product_id` varchar(255) COLLATE utf8_unicode_ci NOT NULL,
  `total_item` mediumint(8) unsigned NOT NULL COMMENT 'Tong so san pham',
  `good_mark` smallint(5) unsigned DEFAULT '0' COMMENT 'Gioi han diem uy tin doi voi nguoi muon mua SP nay',
  `include_tax` tinyint(4) DEFAULT '0' COMMENT 'Trang thai quy dinh gia thanh sp da bao gom thue hay chua',
  `time_for_payment` tinyint(4) DEFAULT '0' COMMENT 'Thoi han duoc phep thanh toan cho san pham nay',
  `shipping_fee` double DEFAULT '0' COMMENT 'Gia van chuyen co dinh',
  `long_width_height` varchar(30) COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'truong luu chieu dai, chieu rong, chieu cao cua san pham theo cau chuc (long;width;height)',
  `weight` double DEFAULT '0' COMMENT 'Trong luong cua san pham',
  `price_auto_accept_bargain` double NOT NULL,
  `price_increment` double DEFAULT '0' COMMENT 'Buoc gia toi thieu',
  `price_increment_max` double DEFAULT '0' COMMENT 'Buoc gia toi da',
  `user_id_auto_bid` double DEFAULT '0' COMMENT 'Ma id Nguoi dat gia autobid',
  `price_auto_bid` double DEFAULT '0' COMMENT 'Gia dat autobid hien tai',
  `current_bid_user_id` int(11) DEFAULT '0' COMMENT 'Ma id nguoi dat bid hien tai',
  `time_the_end` int(11) DEFAULT '0' COMMENT 'Thoi gia ket thuc cua SP da duoc nguoi ban chon luc dang SP',
  `verify_image` tinyint(1) DEFAULT '0' COMMENT 'có yêu cầu nhập mã bảo mật không',
  `price_sms` double DEFAULT '0',
  `estore_name` varchar(64) COLLATE utf8_unicode_ci DEFAULT '' COMMENT 'Ten GHCN             ',
  `e_category_name` varchar(255) COLLATE utf8_unicode_ci DEFAULT '' COMMENT 'Ten cua DM SP tai GHCN',
  `is_in_tab_at_home` tinyint(4) DEFAULT '0' COMMENT 'Trang thai SP duoc hien thi o tab trang chu GH',
  `sku` varchar(10) COLLATE utf8_unicode_ci DEFAULT '' COMMENT 'Theme hien thi SP tai GH',
  `skin` varchar(200) COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'Luu thong tin skin hien thi trang tri cua SP\r\n            ',
  `time_post` int(10) unsigned DEFAULT '0' COMMENT 'Thoi gian dang san pham',
  `is_autobid` tinyint(4) NOT NULL DEFAULT '1' COMMENT '1 cho phep dat autobid, 0 la ko cho phep dat autobid',
  `p1` varchar(255) COLLATE utf8_unicode_ci DEFAULT NULL,
  `p2` varchar(255) COLLATE utf8_unicode_ci DEFAULT NULL,
  `p3` varchar(255) COLLATE utf8_unicode_ci DEFAULT NULL,
  `p4` varchar(255) COLLATE utf8_unicode_ci DEFAULT NULL,
  `p5` varchar(255) COLLATE utf8_unicode_ci DEFAULT NULL,
  `p6` varchar(255) COLLATE utf8_unicode_ci DEFAULT NULL,
  `p7` varchar(255) COLLATE utf8_unicode_ci DEFAULT NULL,
