-- --------------------------------------------------------

--
-- Table structure for table `store_infos`
--

CREATE TABLE IF NOT EXISTS `store_infos` (
  `id` int(4) NOT NULL AUTO_INCREMENT,
  `store_id` int(4) NOT NULL DEFAULT '0' COMMENT 'Mã gian hàng',
  `url` varchar(100) COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'Url của gian hàng',
  `owner_name` varchar(255) COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'Tên chủ gian hàng',
  `display_store_name` varchar(255) COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'Tên gian hàng',
  `user_id` int(4) NOT NULL DEFAULT '0' COMMENT 'Mã tài khoản',
  `user_name` varchar(50) COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'Tên tài khoản',
  `user_fullname` varchar(255) COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'Tên chủ tài khoản',
  `user_email` varchar(50) COLLATE utf8_unicode_ci DEFAULT NULL,
  `user_address` varchar(255) COLLATE utf8_unicode_ci DEFAULT NULL,
  `user_mobile` varchar(20) COLLATE utf8_unicode_ci DEFAULT NULL,
  `user_phone` varchar(20) COLLATE utf8_unicode_ci DEFAULT NULL,
  `email` varchar(50) COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'Địa chỉ Email của gian hàng',
  `address` varchar(255) COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'Địa chỉ của gian hàng',
  `phone` varchar(50) COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'Điện thoại của gian hàng',
  `field_ids` varchar(255) COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'Danh sách ID của lĩnh vực',
  `zone_id` int(11) DEFAULT '0' COMMENT 'Mã khu vực của gian hàng Mã khu vực của gian hàng',
  `zone_name` varchar(50) COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'Tên khu vực của gian hàng',
  `visitor` int(6) NOT NULL COMMENT 'Số lượng truy cập vào gian hàng',
  `likes` int(11) NOT NULL,
  `blast` varchar(255) COLLATE utf8_unicode_ci NOT NULL,
  `total_item` int(8) NOT NULL,
  `total_vote` int(11) NOT NULL DEFAULT '0' COMMENT 'tổng số người đánh giá',
  `score` int(11) NOT NULL DEFAULT '0' COMMENT 'Điểm uy tín của user',
  `type_group` tinyint(4) NOT NULL DEFAULT '3' COMMENT '1; nhóm trên 80%; 2: 50-80%;3:dưới 50%',
  `good_vote` int(11) NOT NULL DEFAULT '0' COMMENT 'tống số đánh giá tốt',
  `store_header` varchar(255) COLLATE utf8_unicode_ci NOT NULL,
  `title_home_page` varchar(255) COLLATE utf8_unicode_ci NOT NULL COMMENT 'tieu de trang chu shop',
  `description_home_page` text COLLATE utf8_unicode_ci NOT NULL COMMENT 'mo ta trang chu shop',
  PRIMARY KEY (`id`),
  KEY `user_id` (`user_id`),
  KEY `store_id` (`store_id`,`user_id`)
) ENGINE=MyISAM  DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci COMMENT='Bảng lưu thông tin của gian hàng' AUTO_INCREMENT=13793 ;

-- --------------------------------------------------------

--
-- Table structure for table `store_invoices`
--

CREATE TABLE IF NOT EXISTS `store_invoices` (
  `id` int(11) NOT NULL AUTO_INCREMENT COMMENT 'Chỉ số hợp đồng',
  `user_id` int(11) NOT NULL DEFAULT '0' COMMENT 'Mã tài khoản',
  `user_name` varchar(255) COLLATE utf8_unicode_ci NOT NULL COMMENT 'Tài khoản',
  `user_fullname` varchar(255) COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'Tên đầy đủ người yêu cầu',
  `user_phone` varchar(45) COLLATE utf8_unicode_ci NOT NULL COMMENT 'Số điện thoại của tài khoản',
  `store_id` int(11) NOT NULL DEFAULT '0' COMMENT 'Mã gian hàng',
  `url` varchar(255) COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'Tên gian hàng',
  `contract_code` varchar(100) COLLATE utf8_unicode_ci NOT NULL COMMENT 'Mã hợp đồng',
  `contract_financial` varchar(100) COLLATE utf8_unicode_ci NOT NULL COMMENT 'Mã hợp đồng tài chính',
  `total_money` double NOT NULL DEFAULT '0' COMMENT 'Tổng số tiền',
  `payment_money` double NOT NULL DEFAULT '0' COMMENT 'Số tiền thanh toán',
  `debt_money` double NOT NULL DEFAULT '0' COMMENT 'Số tiền còn nợ',
  `payment_status` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'Trạng thái thanh toán',
  `payment_type` tinyint(1) NOT NULL DEFAULT '1' COMMENT 'Kiểu TT: 1 TT, 2 NL, 3 CK',
  `payment_time` int(11) NOT NULL COMMENT 'Thời gian thanh toán',
  `service_type` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'Kiểu dịch vụ',
  `time_created` int(11) NOT NULL DEFAULT '0' COMMENT 'Thời gian khởi tạo',
  `time_modified` int(11) NOT NULL DEFAULT '0' COMMENT 'Thời gian sửa đổi',
  `time_signing` int(11) NOT NULL DEFAULT '0' COMMENT 'Thời gian ký HĐ',
  `refer_code` varchar(255) COLLATE utf8_unicode_ci NOT NULL COMMENT 'Mã NL hoặc HĐ tài chính',
  `sale_username` varchar(255) COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'Tài khoản người phụ trách',
  `office_id` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'Mã văn phòng của nhân viên',
  `note` varchar(255) COLLATE utf8_unicode_ci NOT NULL COMMENT 'Ghi chú',
  PRIMARY KEY (`id`,`store_id`,`user_id`)
) ENGINE=MyISAM DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci COMMENT='Bảng lưu thông tin hợp đồng của gian hàng' AUTO_INCREMENT=1 ;

