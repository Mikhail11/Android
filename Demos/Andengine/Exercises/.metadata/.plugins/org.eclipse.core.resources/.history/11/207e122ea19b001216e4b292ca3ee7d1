--
-- Table structure for table `category`
--

CREATE TABLE IF NOT EXISTS `category` (
  `id` int(11) NOT NULL,
  `parent_id` int(11) NOT NULL,
  `name` varchar(200) NOT NULL,
  `level` int(11) NOT NULL,
  `name_en` varchar(255) NOT NULL,
  `status` int(1) NOT NULL DEFAULT '1',
  `cdt_status` tinyint(1) NOT NULL,
  `flat_fee` float NOT NULL DEFAULT '0',
  `percentage_fee` float NOT NULL DEFAULT '0',
  `hs_code` varchar(10) NOT NULL,
  `tax_mnf` double NOT NULL DEFAULT '0',
  `tax_cept` double NOT NULL DEFAULT '0',
  `tax_vat` double NOT NULL,
  `description` text,
  `tax_other` double NOT NULL,
  `sname` varchar(255) NOT NULL,
  `time_update` int(11) NOT NULL,
  `location` varchar(10) NOT NULL DEFAULT 'us',
  `position` int(11) NOT NULL,
  `has_child` tinyint(1) NOT NULL DEFAULT '1',
  `weight` decimal(10,0) NOT NULL,
  `fee_min` float DEFAULT NULL,
  `easy_break` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'De vo',
  `cdt_category_id` int(11) DEFAULT NULL COMMENT 'Map danh muc CDT',
  `cdt_keyword` varchar(200) DEFAULT NULL COMMENT 'Map tu khoa CDT',
  PRIMARY KEY (`id`)
) ENGINE=MyISAM DEFAULT CHARSET=latin1 PACK_KEYS=1;

