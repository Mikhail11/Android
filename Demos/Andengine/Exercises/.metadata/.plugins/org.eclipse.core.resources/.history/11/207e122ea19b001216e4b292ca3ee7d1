  `admin_comment` text NOT NULL,
  `content_answer` text NOT NULL COMMENT 'Nội dung trả lời',
  `phone` varchar(255) NOT NULL DEFAULT '',
  `address` varchar(255) NOT NULL DEFAULT '',
  `user_id` int(11) DEFAULT '0',
  `user_name` varchar(255) DEFAULT NULL,
  `time_created` int(11) DEFAULT NULL,
  PRIMARY KEY (`id`)
) ENGINE=MyISAM  DEFAULT CHARSET=latin1 AUTO_INCREMENT=489 ;

-- --------------------------------------------------------

--
-- Table structure for table `contract`
--

CREATE TABLE IF NOT EXISTS `contract` (
  `id` int(11) NOT NULL AUTO_INCREMENT COMMENT 'Chỉ số hợp đồng',
  `user_id` int(11) NOT NULL DEFAULT '0' COMMENT 'Mã khách hàng',
  `user_name` varchar(255) COLLATE utf8_unicode_ci NOT NULL COMMENT 'Tài khoản khách hàng',
  `user_fullname` varchar(255) COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'Tên đầy đủ khách hàng',
  `user_phone` varchar(45) COLLATE utf8_unicode_ci NOT NULL COMMENT 'Số điện thoại khách hàng',
  `store_id` int(11) NOT NULL DEFAULT '0' COMMENT 'Mã gian hàng',
  `store_name` varchar(255) COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'Tên gian hàng',
  `contract_code` varchar(100) COLLATE utf8_unicode_ci NOT NULL COMMENT 'Mã hợp đồng',
  `service_code` varchar(100) COLLATE utf8_unicode_ci NOT NULL COMMENT 'Mã gói dịch vụ',
  `service_name` varchar(100) COLLATE utf8_unicode_ci NOT NULL COMMENT 'Tên gói dịch vụ',
  `total_money` double NOT NULL DEFAULT '0' COMMENT 'Tổng số tiền',
  `payment_money` double NOT NULL DEFAULT '0' COMMENT 'Số tiền thanh toán',
  `debt_money` double NOT NULL DEFAULT '0' COMMENT 'Số tiền còn nợ',
  `payment_status` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'Trạng thái thanh toán',
  `payment_type` tinyint(1) NOT NULL DEFAULT '1' COMMENT 'Kiểu TT: 1 TT, 2 NL, 3 CK',
  `payment_time` int(11) NOT NULL COMMENT 'Thời gian thanh toán',
  `service_type` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'Kiểu dịch vụ',
  `time_created` int(11) NOT NULL DEFAULT '0' COMMENT 'Thời gian khởi tạo',
  `time_update` int(11) NOT NULL DEFAULT '0' COMMENT 'Thời gian cập nhật',
  `time_signing` int(11) NOT NULL DEFAULT '0' COMMENT 'Thời gian ký HĐ',
  `refer_code` varchar(255) COLLATE utf8_unicode_ci NOT NULL COMMENT 'Mã NL hoặc HĐ tài chính',
  `accunt_username` varchar(255) COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'Tài khoản NVKD',
  `office_id` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'Mã văn phòng NVKD',
  `office_name` varchar(255) COLLATE utf8_unicode_ci NOT NULL COMMENT 'Tên khu vực văn phòng',
  `note` text COLLATE utf8_unicode_ci NOT NULL COMMENT 'Ghi chú',
  PRIMARY KEY (`id`,`store_id`,`user_id`)
) ENGINE=MyISAM  DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci COMMENT='Bảng lưu thông tin hợp đồng' AUTO_INCREMENT=283 ;

-- --------------------------------------------------------

--
-- Table structure for table `deals`
--

CREATE TABLE IF NOT EXISTS `deals` (
  `id` int(10) NOT NULL AUTO_INCREMENT,
  `deal_name` varchar(255) NOT NULL,
  `deal_type` int(2) NOT NULL COMMENT '''Kiểu deal 1: thời trang, 2: công nghệ, 3: tiêu dùng'',',
  `fixed` tinyint(1) NOT NULL COMMENT '1: là fix, 2: là insert bình thường',
  `create_time` int(11) NOT NULL,
  `update_time` int(11) NOT NULL COMMENT 'Thời gian cập nhật thông tin hotdeal',
  `begin_time` int(11) NOT NULL,
  `end_time` int(11) NOT NULL,
  `ordering` int(10) DEFAULT NULL,
  `logo_img` varchar(255) DEFAULT NULL,
  `status` tinyint(1) DEFAULT '1' COMMENT '1: hiện, 2 là ẩn',
  PRIMARY KEY (`id`),
  KEY `create_time` (`create_time`),
  KEY `begin_time` (`begin_time`),
  KEY `end_time` (`end_time`),
  KEY `status` (`status`)
) ENGINE=MyISAM  DEFAULT CHARSET=latin1 AUTO_INCREMENT=16 ;

-- --------------------------------------------------------

--
-- Table structure for table `deals_config`
--

CREATE TABLE IF NOT EXISTS `deals_config` (
  `id` int(10) NOT NULL AUTO_INCREMENT,
  `slider_name` varchar(255) NOT NULL COMMENT 'Tên slider',
  `slider_link` varchar(255) DEFAULT NULL COMMENT 'Link khi click vào slider',
  `img_link` varchar(255) DEFAULT NULL COMMENT 'Link ảnh',
  `img_code` text COMMENT 'Code html thay cho ảnh',
  `ordering` int(10) DEFAULT NULL COMMENT 'Thứ tự slider',
  `status` tinyint(1) DEFAULT '1' COMMENT '1 là hiện, 2 là ẩn',
  PRIMARY KEY (`id`),
  KEY `status` (`status`)
) ENGINE=MyISAM  DEFAULT CHARSET=latin1 AUTO_INCREMENT=11 ;

-- --------------------------------------------------------

--
-- Table structure for table `deals_product`
--

CREATE TABLE IF NOT EXISTS `deals_product` (
  `id` int(10) NOT NULL AUTO_INCREMENT,
  `deal_id` int(10) NOT NULL,
  `cat_id` int(10) NOT NULL,
  `product_id` int(10) NOT NULL,
  `product_name` text NOT NULL,
  `promotion_id` int(10) NOT NULL COMMENT 'Mã khuyến mại',
  `promotion_type` int(1) NOT NULL COMMENT 'Kiểu km: 1-Giảm giá sản phẩm; 2-Giảm giá hóa đơn; 3-quà tặng',
  `price_promotion` double NOT NULL COMMENT 'Giá trị khuyến mại',
  `price` double NOT NULL COMMENT 'Giá đã được đẩy lên so với giá gốc bên hệ thống item của sản phẩm',
  `unit` tinyint(1) NOT NULL COMMENT '1: là %, 2: là VNĐ',
  `ordering` int(10) DEFAULT NULL,
  `status` tinyint(1) NOT NULL COMMENT '1: active, 2: disable_time, 3: disable do sp bị hủy bên promotion',
  PRIMARY KEY (`id`),
  KEY `deal_id` (`deal_id`),
  KEY `product_id` (`product_id`),
  KEY `promotion_id` (`promotion_id`),
  KEY `cat_id` (`cat_id`),
  FULLTEXT KEY `product_name` (`product_name`)
) ENGINE=MyISAM  DEFAULT CHARSET=latin1 AUTO_INCREMENT=302 ;

