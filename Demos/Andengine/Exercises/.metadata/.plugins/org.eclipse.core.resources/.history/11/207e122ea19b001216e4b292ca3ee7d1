  `position` int(10) NOT NULL COMMENT 'thứ tự banner',
  `admin_id` int(10) unsigned NOT NULL COMMENT 'id người cập nhật',
  `start_time` int(10) unsigned NOT NULL COMMENT 'thời gian bắt đầu chạy banner',
