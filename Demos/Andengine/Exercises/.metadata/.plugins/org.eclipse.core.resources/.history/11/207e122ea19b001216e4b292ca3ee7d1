CREATE TABLE IF NOT EXISTS `promotion_item_input` (
  `id` int(11) NOT NULL COMMENT 'Mã hình thức khuyến mại = promotion_id',
  `item_id` int(11) NOT NULL COMMENT 'Mã sản phẩm',
  PRIMARY KEY (`id`,`item_id`)
) ENGINE=MyISAM DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci COMMENT='Bảng lưu danh sách sản phẩm áp dụng khuyến mại (Quan hệ 1-N ';

-- --------------------------------------------------------

--
-- Table structure for table `promotion_item_output`
--

CREATE TABLE IF NOT EXISTS `promotion_item_output` (
  `id` int(11) NOT NULL COMMENT 'Mã hình thức khuyến mại áp dụng = promotion_id',
  `item_id` int(11) NOT NULL COMMENT 'Mã sản phẩm đầu ra cho hình thức khuyến mại',
  PRIMARY KEY (`id`,`item_id`)
) ENGINE=MyISAM DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci COMMENT='b?ng luu danh sách s?n ph?m d?u ra c?a hình th?c khuy?n m?i ';

-- --------------------------------------------------------

--
-- Table structure for table `promotion_time_buyer`
--

CREATE TABLE IF NOT EXISTS `promotion_time_buyer` (
  `id` int(11) NOT NULL DEFAULT '0' COMMENT 'Mã hình thức khuyến mại sẽ áp dụng = promotion_id',
  `times` int(11) NOT NULL COMMENT 'Số lần mã người mua đã giao dịch thành công trong khoảng thời gian begin_date, end_date',
  `buy_begin_date` int(11) NOT NULL COMMENT 'Thời gian bắt đầu tính số lần giao dịch thành công của người mua',
  `buy_end_date` int(11) NOT NULL COMMENT 'Thời gian kết thúc tính số lần giao dịch thành công của người mua',
  PRIMARY KEY (`id`)
) ENGINE=MyISAM DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci COMMENT='Bảng lưu điều kiện người mua được phép áp dụng hình thức khu';

-- --------------------------------------------------------

--
-- Table structure for table `promotion_user`
--

CREATE TABLE IF NOT EXISTS `promotion_user` (
  `id` int(11) NOT NULL COMMENT 'mã khuyến mãi = promotion_id',
  `user_name` varchar(70) COLLATE utf8_unicode_ci NOT NULL,
  PRIMARY KEY (`id`,`user_name`)
) ENGINE=MyISAM DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci COMMENT='Bảng lưu danh sách người bán được phép áp dụng khuyen mai';

-- --------------------------------------------------------

--
-- Table structure for table `template`
--

CREATE TABLE IF NOT EXISTS `template` (
  `id` int(11) NOT NULL AUTO_INCREMENT COMMENT 'mã template',
  `template_category_id` int(11) NOT NULL COMMENT 'mã chủ đề',
  `name` varchar(100) COLLATE utf8_unicode_ci NOT NULL,
  `thumb_image` varchar(100) COLLATE utf8_unicode_ci NOT NULL,
  `price` double NOT NULL,
  `is_free` tinyint(1) NOT NULL,
  `description` varchar(100) COLLATE utf8_unicode_ci NOT NULL,
  `html_code` text COLLATE utf8_unicode_ci NOT NULL,
  `user_id` int(11) NOT NULL,
  `create_timed` int(10) unsigned NOT NULL,
  `status` tinyint(1) NOT NULL,
  `view` int(11) NOT NULL,
  PRIMARY KEY (`id`)
) ENGINE=MyISAM  DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci AUTO_INCREMENT=34 ;

-- --------------------------------------------------------

