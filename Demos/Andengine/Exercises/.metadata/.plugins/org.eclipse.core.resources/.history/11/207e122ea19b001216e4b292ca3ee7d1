  `end_date` int(11) NOT NULL DEFAULT '0' COMMENT 'Ngày kết thúc quá trình sử dụng chức năng',
  `quota_type` tinyint(4) NOT NULL COMMENT 'Loại quota: 1-Số lượng, 2-Thời gian, 3-Slượng/Tgian, 4-Không có quota',
  PRIMARY KEY (`store_id`,`function_id`)
