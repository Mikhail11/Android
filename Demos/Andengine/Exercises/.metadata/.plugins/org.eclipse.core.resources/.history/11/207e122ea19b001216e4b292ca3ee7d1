--
-- Table structure for table `search_keyword_model`
--

CREATE TABLE IF NOT EXISTS `search_keyword_model` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `keyword` varchar(255) NOT NULL DEFAULT '',
  `telex_keyword` varchar(255) NOT NULL,
  `last_time` int(11) NOT NULL DEFAULT '0',
  `count` int(11) NOT NULL DEFAULT '0',
  `total_model` int(11) NOT NULL DEFAULT '0',
  PRIMARY KEY (`id`),
  FULLTEXT KEY `telex_keyword` (`telex_keyword`)
) ENGINE=MyISAM DEFAULT CHARSET=latin1 AUTO_INCREMENT=1 ;

-- --------------------------------------------------------

--
-- Table structure for table `search_keyword_new`
--

CREATE TABLE IF NOT EXISTS `search_keyword_new` (
  `id` int(10) unsigned NOT NULL AUTO_INCREMENT,
  `firstChar` tinyint(3) unsigned NOT NULL DEFAULT '0' COMMENT 'Mã ACSII của ký tự đầu tiên trong searchText, bằng 0 là các ký tự khác',
  `searchText` varchar(64) NOT NULL DEFAULT '' COMMENT 'Từ dùng để so sanh',
  `originalText` varchar(150) NOT NULL DEFAULT '' COMMENT 'Từ để hiển thị',
  `id_text` varchar(22) NOT NULL DEFAULT '' COMMENT 'id: - id của danh muc, id - id của model, id:id - id của danh mục và id của thương hiệu',
  `point` int(11) NOT NULL DEFAULT '0' COMMENT 'dùng để sắp xếp kết quả',
  PRIMARY KEY (`id`,`firstChar`),
  UNIQUE KEY `firstChar` (`firstChar`,`originalText`,`searchText`,`id_text`) USING BTREE,
  KEY `searchText` (`searchText`),
  KEY `id_text` (`id_text`),
  KEY `point` (`point`)
) ENGINE=MyISAM  DEFAULT CHARSET=utf8
