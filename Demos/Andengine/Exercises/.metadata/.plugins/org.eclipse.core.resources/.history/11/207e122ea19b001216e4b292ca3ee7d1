-- --------------------------------------------------------

--
-- Table structure for table `order_cache`
--

CREATE TABLE IF NOT EXISTS `order_cache` (
  `id` int(11) unsigned NOT NULL AUTO_INCREMENT,
  `order_id` int(11) unsigned NOT NULL,
  `item_id` int(11) unsigned NOT NULL,
  `title` varchar(255) COLLATE utf8_unicode_ci DEFAULT NULL,
  `content` text COLLATE utf8_unicode_ci,
  `discount_policy` text COLLATE utf8_unicode_ci COMMENT 'chính sách giam giá',
  `time_created` int(11) unsigned NOT NULL COMMENT 'thoi gian sinh cache',
  `time_expire` int(11) unsigned DEFAULT NULL COMMENT 'luu den ngay',
  PRIMARY KEY (`id`)
) ENGINE=MyISAM  DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci AUTO_INCREMENT=1276 ;

-- --------------------------------------------------------

--
-- Table structure for table `order_complain`
--

CREATE TABLE IF NOT EXISTS `order_complain` (
  `id` int(11) unsigned NOT NULL COMMENT 'id cua khieu nai, ung voi mot id hoa don',
  `title` varchar(512) COLLATE utf8_unicode_ci DEFAULT NULL,
  `user_id_current` int(11) unsigned NOT NULL,
  `seller_id` int(11) unsigned NOT NULL COMMENT 'id nguoi khieu nai',
  `seller_username` varchar(50) COLLATE utf8_unicode_ci NOT NULL,
  `buyer_id` int(11) NOT NULL,
  `buyer_username` varchar(50) COLLATE utf8_unicode_ci NOT NULL,
  `admin_id` int(11) unsigned DEFAULT '0',
  `admin_username` varchar(50) COLLATE utf8_unicode_ci DEFAULT NULL,
  `time_created` int(11) DEFAULT NULL,
  `time_reply` int(11) unsigned DEFAULT NULL COMMENT 'thoi gian dua ra khieu nai',
  `status_end` tinyint(1) NOT NULL DEFAULT '1' COMMENT 'trang thai ket thuc cua khieu nai. 1 la mo, 0 la ket thuc',
  `status_delete` tinyint(1) NOT NULL DEFAULT '0' COMMENT '1 la nguoi ban xoa, 2 la nguoi mua xoa, 3 la ca 2 deu xoa nhung admin CDT van con thay',
  `total_reply` smallint(3) unsigned NOT NULL DEFAULT '1' COMMENT 'tong so reply trong mot khieu nai',
  `new_reply` smallint(2) NOT NULL DEFAULT '1' COMMENT 'so luong khieu nai moi',
  PRIMARY KEY (`id`),
  KEY `user_id_current` (`user_id_current`,`seller_id`,`buyer_id`,`admin_id`,`time_reply`,`status_end`,`status_delete`),
  KEY `total_reply` (`total_reply`)
) ENGINE=MyISAM DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci COMMENT='bang luu phan hoi, khieu nai';

-- --------------------------------------------------------

--
-- Table structure for table `order_complain_info`
--

CREATE TABLE IF NOT EXISTS `order_complain_info` (
  `id` int(11) unsigned NOT NULL AUTO_INCREMENT,
  `order_id` int(11) unsigned NOT NULL COMMENT 'ma hoa don',
  `title` varchar(512) COLLATE utf8_unicode_ci DEFAULT NULL,
  `content` text COLLATE utf8_unicode_ci COMMENT 'noi dung khieu nai',
  `seller_id` int(11) unsigned NOT NULL COMMENT 'id nguoi khieu nai',
  `seller_username` varchar(127) COLLATE utf8_unicode_ci NOT NULL COMMENT 'username nguoi khieu nai',
  `buyer_id` int(11) unsigned NOT NULL COMMENT 'id nguoi mua',
  `buyer_username` varchar(50) COLLATE utf8_unicode_ci NOT NULL COMMENT 'user_name nguoi mua',
  `admin_id` int(11) unsigned DEFAULT '0' COMMENT 'id nguoi quan tri tham gia tra loi khieu nai',
  `admin_username` varchar(50) COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'user_name nguoi quan tri tra loi khieu nai',
  `time_created` int(11) unsigned DEFAULT NULL,
  `time_reply` int(11) unsigned DEFAULT '0' COMMENT 'thoi gian dua ra khieu nai',
  `user_id_current` int(11) unsigned NOT NULL COMMENT 'id nguoi da reply khieu nai nay',
  `status_view` tinyint(1) NOT NULL DEFAULT '1' COMMENT 'trang thai thu gon hay khong. 1 la khong, 0 la thu gon',
  PRIMARY KEY (`id`),
  KEY `fk_reference_3` (`order_id`),
  KEY `seller_id` (`seller_id`,`buyer_id`,`admin_id`,`time_reply`)
) ENGINE=MyISAM  DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci COMMENT='bang luu phan hoi, khieu nai' AUTO_INCREMENT=48 ;

-- --------------------------------------------------------

--
-- Table structure for table `order_history`
--

CREATE TABLE IF NOT EXISTS `order_history` (
  `id` int(10) unsigned NOT NULL AUTO_INCREMENT,
  `order_id` int(10) unsigned NOT NULL COMMENT 'Mã hóa đơn',
  `user_id` int(10) unsigned NOT NULL COMMENT 'Mã user xử lý',
  `content` text CHARACTER SET utf8 COMMENT 'Nội dung xử lý',
  `contact_status` tinyint(1) unsigned DEFAULT NULL COMMENT 'Trạng thái liên hệ',
  `process_status` tinyint(1) unsigned DEFAULT NULL COMMENT 'Trạng thái xử lý',
  `reason` tinyint(1) unsigned DEFAULT NULL COMMENT 'Lý do khi trạng thái xử lý thất bại',
  `time_created` int(10) unsigned NOT NULL,
  PRIMARY KEY (`id`)
) ENGINE=MyISAM  DEFAULT CHARSET=latin1 COMMENT='Lịch sử liên hệ của hóa đơn' AUTO_INCREMENT=196 ;

-- --------------------------------------------------------

--
-- Table structure for table `order_index`
--

CREATE TABLE IF NOT EXISTS `order_index` (
  `id` int(11) NOT NULL AUTO_INCREMENT COMMENT 'id hoa don',
  `index_table` smallint(6) NOT NULL DEFAULT '0' COMMENT '=0 là order hiện tại, != 0 là order của năm tương ứng, ví dụ = 2010 là order_2010 và order_info_2010',
  `seller_id` int(11) NOT NULL COMMENT 'id nguoi ban',
  `buyer_id` int(11) DEFAULT '0' COMMENT 'id nguoi mua',
  `time_checkout` int(11) DEFAULT '0' COMMENT 'thoi gian thanh toan hoa don',
  `date` int(11) NOT NULL COMMENT 'Truong luu tam date de convert',
  `year` smallint(6) NOT NULL COMMENT 'Truong luu nam phat sinh hoa don dung cho convert',
  PRIMARY KEY (`id`)
) ENGINE=MyISAM  DEFAULT CHARSET=latin1 AUTO_INCREMENT=1030 ;

-- --------------------------------------------------------

--
-- Table structure for table `order_info`
--

CREATE TABLE IF NOT EXISTS `order_info` (
  `id` int(11) unsigned NOT NULL AUTO_INCREMENT,
  `seller_name` varchar(512) CHARACTER SET utf8 COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'Ten nguoi ban',
  `seller_zone` smallint(6) DEFAULT '0' COMMENT 'id quan huyen',
  `buyer_name` varchar(512) CHARACTER SET utf8 COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'ten nguoi mua',
  `buyer_mobile` varchar(20) CHARACTER SET utf8 COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'So di dong nguoi mua',
  `buyer_home_phone` varchar(20) CHARACTER SET utf8 COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'So dien thoai co dinh nguoi mua',
  `buyer_address` varchar(255) CHARACTER SET utf8 COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'Dia chi nguoi mua',
  `buyer_email` varchar(127) CHARACTER SET utf8 COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'E-mail nguoi mua',
  `buyer_cmtnd` varchar(11) CHARACTER SET utf8 COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'so chung minh thu nhan dan nguoi mua',
  `receiver_name` varchar(255) CHARACTER SET utf8 COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'ho ten nguoi nhan',
  `receiver_mobile` varchar(20) CHARACTER SET utf8 COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'so di dong nguoi nhan',
  `receiver_home_phone` varchar(20) CHARACTER SET utf8 COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'so co dinh nguoi nhan',
  `receiver_email` varchar(127) CHARACTER SET utf8 COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'dia chi e-mail nguoi nhan',
  `receiver_address` varchar(255) CHARACTER SET utf8 COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'dia chi nguoi nhan',
  `receiver_zone` int(11) DEFAULT '0' COMMENT 'khu vuc cua nguoi nhan',
  `receiver_date` int(11) unsigned DEFAULT '0' COMMENT 'ngay nhan',
  `seller_comment` varchar(512) CHARACTER SET utf8 COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'nguoi ban phan hoi',
  `buyer_comment` varchar(512) CHARACTER SET utf8 COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'nguoi mua phan hoi',
  `shipping_guide` varchar(512) CHARACTER SET utf8 COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'huong dan van chuyen',
  `shipping_code` varchar(100) CHARACTER SET utf8 COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'Mã vận đơn',
  `payment_guide` varchar(512) CHARACTER SET utf8 COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'huong dan thanh toan',
  `campaign_text` text CHARACTER SET utf8 COLLATE utf8_unicode_ci COMMENT 'Thông tin khuyến mãi campaign dành cho hóa đơn',
  `ga` tinyint(4) DEFAULT '0' COMMENT 'check ecommerce tren GA',
  `date` int(11) NOT NULL COMMENT 'Truong luu tam date de convert',
  `year` smallint(6) NOT NULL COMMENT 'Truong luu nam phat sinh hoa don dung cho convert',
  PRIMARY KEY (`id`),
  KEY `id` (`id`)
) ENGINE=MyISAM  DEFAULT CHARSET=latin1 AUTO_INCREMENT=4294967296 ;

