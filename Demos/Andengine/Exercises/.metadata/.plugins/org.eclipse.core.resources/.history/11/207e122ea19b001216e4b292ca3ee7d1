  `last_ActiveEmail` int(11) DEFAULT NULL,
  `last_VerifySMSs` int(11) DEFAULT NULL,
  `last_Bid` int(11) DEFAULT NULL,
  `last_Bargain` int(11) DEFAULT NULL,
  `last_Buy` int(11) DEFAULT NULL,
  `last_Order` int(11) DEFAULT NULL COMMENT 'Lần cuối sinh hóa đơn',
  `last_Direct` int(11) DEFAULT NULL COMMENT 'Mua hàng theo hình thức thanh toán trực tiếp lần cuối',
  `last_PayNL` int(11) DEFAULT NULL,
  `last_Refund` int(11) DEFAULT NULL,
  `last_List` int(11) DEFAULT NULL,
  `last_Sell` int(11) DEFAULT NULL,
  `first_list` int(11) DEFAULT NULL,
  `first_sell` int(11) DEFAULT NULL,
  `first_bid` int(11) DEFAULT NULL,
  `first_bargain` int(11) DEFAULT NULL,
  `first_pay` int(11) DEFAULT NULL,
  `first_buy` int(11) DEFAULT NULL,
  `first_order` int(11) DEFAULT NULL COMMENT 'Lần đầu sinh hóa đơn',
  `first_refund` int(11) DEFAULT NULL,
  PRIMARY KEY (`id`)
) ENGINE=MyISAM DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci;

