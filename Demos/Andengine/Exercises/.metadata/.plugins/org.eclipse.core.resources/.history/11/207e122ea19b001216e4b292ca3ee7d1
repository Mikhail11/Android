--
-- Table structure for table `item_craw`
--

CREATE TABLE IF NOT EXISTS `item_craw` (
  `id` int(10) unsigned NOT NULL AUTO_INCREMENT,
  `title` varchar(255) COLLATE utf8_unicode_ci DEFAULT NULL,
  `content` text COLLATE utf8_unicode_ci COMMENT 'N?i dung dã crawl v?, luu du?i d?ng xml',
  `site_product_id` varchar(100) COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'Mã id c?a SP du?c craw v?',
  `user_id` int(11) DEFAULT NULL COMMENT 'Mã Ngu?i dùng',
  `is_new` tinyint(4) DEFAULT NULL COMMENT 'Tr?ng thái m?i hay cu (1 là m?i t?c chua dua lên h? th?ng item, 0 là dã dua lên)',
  `error_code` tinyint(4) DEFAULT NULL COMMENT 'Mã l?i',
  `create_time` int(11) DEFAULT NULL COMMENT 'Th?i gian t?o SP',
  `modify_time` int(11) DEFAULT NULL COMMENT 'Th?i gian craw l?i (s?a d?i)',
  PRIMARY KEY (`id`),
  KEY `is_new` (`is_new`)
) ENGINE=MyISAM DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci COMMENT='Bang san pham crawl' AUTO_INCREMENT=1 ;

