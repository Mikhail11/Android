--
-- Table structure for table `model`
--

CREATE TABLE IF NOT EXISTS `model` (
  `id` int(4) unsigned NOT NULL AUTO_INCREMENT,
  `name` varchar(255) COLLATE utf8_unicode_ci NOT NULL,
  `name_english` varchar(255) COLLATE utf8_unicode_ci NOT NULL COMMENT 'Truong ten tieng anh cua model',
  `is_update_ebay` int(11) NOT NULL COMMENT 'Co kiem tra da cap nhat tu khoa name_english cho ebay chua',
  `sname` varchar(255) COLLATE utf8_unicode_ci NOT NULL,
  `description` text COLLATE utf8_unicode_ci NOT NULL COMMENT 'Truong mo ta cac thong so ky thuoc cua model, khong bao gom ten',
  `category_id` int(4) unsigned NOT NULL,
  `manufacture_id` int(10) unsigned NOT NULL,
  `approval_status` tinyint(1) NOT NULL,
  `search_text` text COLLATE utf8_unicode_ci NOT NULL,
  `price_min_new` double NOT NULL COMMENT 'Gia thap nhat cua san pham moi',
  `price_min_old` double NOT NULL COMMENT 'Gia thap nhat cua san pham cu',
  `price_max` double NOT NULL COMMENT 'Gia lon nhat cua san pham da dang vao model do',
  `total_like` mediumint(8) unsigned NOT NULL DEFAULT '0' COMMENT 'Tong so luot yeu thich Model',
  `total_item_ebay` int(11) NOT NULL COMMENT 'Tong so san pham tim thay ben ebay',
  `total_item_ebay_old` int(11) NOT NULL,
  `total_item` mediumint(8) unsigned NOT NULL,
  `hit_count` mediumint(8) unsigned NOT NULL DEFAULT '0' COMMENT 'Tong so luot view model',
  `total_order` mediumint(8) unsigned NOT NULL COMMENT 'Tong so hoa don phat sinh cua model',
  `time` int(11) unsigned NOT NULL,
  `time_update` int(10) unsigned NOT NULL COMMENT 'Thoi gian cap nhat model',
  `user_id` int(4) NOT NULL,
  `main_image` tinyint(1) NOT NULL DEFAULT '1',
  `url_image` varchar(512) COLLATE utf8_unicode_ci DEFAULT NULL,
  `is_smart_phone` tinyint(4) NOT NULL COMMENT 'La smart phone hay khong',
  `level` tinyint(4) NOT NULL DEFAULT '1' COMMENT '3 hot; 2 trung binh; 1 binh thuong; 0  khong con ban',
  `p1` varchar(255) COLLATE utf8_unicode_ci DEFAULT NULL,
  `p2` varchar(255) COLLATE utf8_unicode_ci DEFAULT NULL,
  `p3` varchar(255) COLLATE utf8_unicode_ci DEFAULT NULL,
  `p4` varchar(255) COLLATE utf8_unicode_ci DEFAULT NULL,
  `p5` varchar(255) COLLATE utf8_unicode_ci DEFAULT NULL,
  `p6` varchar(255) COLLATE utf8_unicode_ci DEFAULT NULL,
  `p7` varchar(255) COLLATE utf8_unicode_ci DEFAULT NULL,
  `p8` varchar(255) COLLATE utf8_unicode_ci DEFAULT NULL,
  `p9` varchar(255) COLLATE utf8_unicode_ci DEFAULT NULL,
  `p10` varchar(255) COLLATE utf8_unicode_ci DEFAULT NULL,
  `p11` varchar(255) COLLATE utf8_unicode_ci DEFAULT NULL,
  `p12` varchar(255) COLLATE utf8_unicode_ci DEFAULT NULL,
  `p13` varchar(255) COLLATE utf8_unicode_ci DEFAULT NULL,
  `p14` varchar(255) COLLATE utf8_unicode_ci DEFAULT NULL,
  `p15` varchar(255) COLLATE utf8_unicode_ci DEFAULT NULL,
  `p16` varchar(255) COLLATE utf8_unicode_ci DEFAULT NULL,
  `p17` varchar(255) COLLATE utf8_unicode_ci DEFAULT NULL,
  `p18` varchar(255) COLLATE utf8_unicode_ci DEFAULT NULL,
  `p19` varchar(255) COLLATE utf8_unicode_ci DEFAULT NULL,
  `p20` varchar(255) COLLATE utf8_unicode_ci DEFAULT NULL,
  `p21` varchar(255) COLLATE utf8_unicode_ci DEFAULT NULL,
  `p22` varchar(255) COLLATE utf8_unicode_ci DEFAULT NULL,
  `p23` varchar(255) COLLATE utf8_unicode_ci DEFAULT NULL,
  `p24` varchar(255) COLLATE utf8_unicode_ci DEFAULT NULL,
  `p25` varchar(255) COLLATE utf8_unicode_ci DEFAULT NULL,
  `p26` varchar(255) COLLATE utf8_unicode_ci DEFAULT NULL,
  `p27` varchar(255) COLLATE utf8_unicode_ci DEFAULT NULL,
  `p28` varchar(255) COLLATE utf8_unicode_ci DEFAULT NULL,
  `p29` varchar(255) COLLATE utf8_unicode_ci DEFAULT NULL,
  `p30` varchar(255) COLLATE utf8_unicode_ci DEFAULT NULL,
  `total_image` tinyint(1) DEFAULT NULL,
  PRIMARY KEY (`id`),
  KEY `category_id` (`category_id`),
  KEY `manufacture_id` (`manufacture_id`),
  KEY `price_min_new` (`price_min_new`),
  KEY `price_min_old` (`price_min_old`),
  FULLTEXT KEY `search_text` (`search_text`),
  FULLTEXT KEY `sname` (`sname`)
) ENGINE=MyISAM  DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci COMMENT='Model sản phẩm' AUTO_INCREMENT=367599 ;

