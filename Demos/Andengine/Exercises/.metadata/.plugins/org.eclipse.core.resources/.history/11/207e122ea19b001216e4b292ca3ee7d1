-- --------------------------------------------------------

--
-- Table structure for table `payment`
--

CREATE TABLE IF NOT EXISTS `payment` (
  `id` int(11) NOT NULL AUTO_INCREMENT COMMENT 'Mã hóa đơn',
  `user_id` int(11) NOT NULL DEFAULT '0' COMMENT 'Mã người yêu cầu',
  `user_name` varchar(255) COLLATE utf8_unicode_ci NOT NULL COMMENT 'Tài khoản người yêu cầu',
  `user_fullname` varchar(255) COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'Tên đầy đủ người yêu cầu',
  `user_phone` varchar(45) COLLATE utf8_unicode_ci NOT NULL COMMENT 'Số điện thoại người yêu cầu',
  `user_zone_id` int(4) NOT NULL COMMENT 'Mã địa phương của KH sử dụng dịch vụ',
  `store_id` int(11) NOT NULL DEFAULT '0' COMMENT 'Mã gian hàng',
  `contract_code` varchar(100) COLLATE utf8_unicode_ci NOT NULL COMMENT 'Mã hợp đồng',
  `service_code` varchar(100) COLLATE utf8_unicode_ci NOT NULL COMMENT 'Mã gói dịch vụ',
  `service_name` varchar(100) COLLATE utf8_unicode_ci NOT NULL COMMENT 'Tên dịch vụ',
  `money` double NOT NULL DEFAULT '0' COMMENT 'Tổng số tiền',
  `money_promotional` double NOT NULL COMMENT 'Tiền khuyến mãi',
  `money_checkout` double NOT NULL COMMENT 'Tiền phải thanh toán ',
  `payment_status` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'Trạng thái thanh toán:0 chờ thanh toán, 1 đã thanh toán, 2 Refund lại tiền',
  `payment_type` tinyint(1) NOT NULL DEFAULT '1' COMMENT 'Kiểu TT: 3 TTTT, 4 CK, 1 NL Ngay, 2 NL Tạm giữ',
  `payment_time` int(11) NOT NULL COMMENT 'Thời gian thanh toán',
  `nganluong_receive_email` varchar(100) COLLATE utf8_unicode_ci NOT NULL COMMENT 'Email tài khoản nhận tiền qua NL',
  `nganluong_pay_email` varchar(100) COLLATE utf8_unicode_ci NOT NULL,
  `service_type` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'Kiểu dịch vụ',
  `time_created` int(11) NOT NULL DEFAULT '0' COMMENT 'Thời gian khởi tạo',
  `time_update` int(11) NOT NULL DEFAULT '0' COMMENT 'Thời gian cập nhật',
  `time_start_service` int(11) DEFAULT '0' COMMENT 'Thời gian chính thức kích hoạt dịch vụ',
  `time_approve` int(11) NOT NULL COMMENT 'Thời gian duyệt bởi KTT',
  `approve_status` tinyint(1) NOT NULL COMMENT 'Trạng thái duyệt bởi KTT khi TTTT /CK:0: Ko xác định, 1 chờ duyệt, 2 đã duyệt ',
  `refer_code` varchar(255) COLLATE utf8_unicode_ci NOT NULL COMMENT 'Mã NL hoặc HĐ tài chính',
  `sale_username` varchar(255) COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'Tài khoản người phụ trách',
  `account_username` varchar(30) COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'Mã kế toán viên',
  `office_id` tinyint(1) DEFAULT '0' COMMENT 'Mã văn phòng: 1 Hà Nội, 2 TP. HCM',
  `note` text COLLATE utf8_unicode_ci COMMENT 'Ghi chú',
  `ip_info` varchar(255) COLLATE utf8_unicode_ci NOT NULL COMMENT 'Lưu thông tin IP của người thanh toán',
  PRIMARY KEY (`id`,`store_id`,`user_id`),
  KEY `store_id` (`store_id`)
) ENGINE=MyISAM  DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci COMMENT='Bảng lưu thông tin phiếu yêu cầu thanh toán' AUTO_INCREMENT=3437 ;

