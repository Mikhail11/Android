--
-- Table structure for table `campain_coupon`
--

CREATE TABLE IF NOT EXISTS `campain_coupon` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `name` varchar(255) DEFAULT NULL COMMENT 'Tên chiến dịch',
  `category_id` varchar(255) DEFAULT NULL COMMENT 'Danh sách category áp dụng (cách nhau bởi dấu phẩy " , ")',
  `time_created` int(11) NOT NULL COMMENT 'Thời gian tạo chiến dịch',
  `money_limit` int(11) NOT NULL DEFAULT '0' COMMENT 'Ngân sách cho phép của chiến dịch',
  `money_delimit` int(11) NOT NULL COMMENT 'Hạn mức còn lại của chiến dịch',
  `percent_discount` int(3) NOT NULL DEFAULT '0' COMMENT 'Phần trăm giảm giá tối đa',
  `money_discount` int(11) NOT NULL DEFAULT '0' COMMENT 'Giá trị tiền mặt giảm tối đa.',
  `time_begin` int(11) NOT NULL DEFAULT '0' COMMENT 'Thời gian bắt đầu chiến dịch',
  `time_end` int(11) NOT NULL DEFAULT '0' COMMENT 'thời gian kết thúc chiến dịch',
  `check_ip_buyer` tinyint(2) DEFAULT '0' COMMENT '0: không kiểm tra trùng ip của người mua;  1: kiểm tra trùng id người chơi',
  `check_ip_internal` tinyint(2) DEFAULT '0' COMMENT '0:kiểm tra ip nội bộ Ps; 1: kiểm tra ip nội bộ ps;',
  `status` tinyint(4) NOT NULL COMMENT '1: hoạt động; 0: trạng thái bị tạm ngừng',
  PRIMARY KEY (`id`)
) ENGINE=MyISAM  DEFAULT CHARSET=latin1 AUTO_INCREMENT=6 ;

-- --------------------------------------------------------

--
-- Table structure for table `campain_coupon_transactions`
--

CREATE TABLE IF NOT EXISTS `campain_coupon_transactions` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `order_code` varchar(25) NOT NULL COMMENT 'Mã hoá đơn bên chợ',
  `order_id` int(11) DEFAULT '0' COMMENT 'mã hoá đơn giao dịch bên chợ.',
  `coupon_code` varchar(50) NOT NULL COMMENT 'Mã phiếu giảm giá',
  `campain_coupon_id` int(11) NOT NULL COMMENT 'Mã chiến dịch giảm giá',
  `seller_id` int(11) NOT NULL COMMENT 'user_id người bán',
  `buyer_id` int(11) NOT NULL COMMENT 'user_id người mua',
  `item_id` int(11) NOT NULL COMMENT 'Mã sản phẩm giao dịch',
  `price_item` bigint(15) NOT NULL COMMENT 'Giá trị thực của sản phẩm',
  `price_coupon` int(11) NOT NULL DEFAULT '0' COMMENT 'giá trị phiếu giảm giá',
  `price_order` int(11) NOT NULL DEFAULT '0' COMMENT 'Giá trị hoá đơn thanh toán',
  `item_url` varchar(255) DEFAULT NULL COMMENT 'Đường dẫn chi tiết sản phẩm',
  `NL_transaction_id` int(11) NOT NULL DEFAULT '0' COMMENT 'mã giao dịch ngân lượng trả về',
  `payment_status` tinyint(4) NOT NULL DEFAULT '1' COMMENT '1: chờ thanh toán; 4: đã thanh toán; 5: trả lại tiền; 6: người mua ko thanh toán; ',
  `payment_type` tinyint(4) NOT NULL DEFAULT '1' COMMENT '1: thanh toán ngân lượng ngay; 2: thanh toán ngân lượng tạm giữ',
  `token_key` varchar(32) DEFAULT NULL COMMENT 'mã view hoá đơn cho người dùng không đăng nhập',
  `time_created` int(11) NOT NULL DEFAULT '0' COMMENT 'Thời gian tạo ',
  `time_update` int(11) NOT NULL DEFAULT '0' COMMENT 'Thời gian cập nhật trạng thái mới nhất của giao dịch',
  `status_payment_seller` tinyint(4) NOT NULL DEFAULT '0' COMMENT '0: chưa hoàn trả tiền; 1: Đã hoàn trả tiền còn thiếu cho người bán',
  `nl_id_pay_seller` int(11) DEFAULT NULL COMMENT 'Mã giao dịch ngân lượng hoàn tiền cho người bán',
  `time_checkout` int(11) NOT NULL DEFAULT '0' COMMENT 'Thời điểm đã thanh toán hoá đơn thành công',
  PRIMARY KEY (`id`)
) ENGINE=MyISAM  DEFAULT CHARSET=latin1 AUTO_INCREMENT=50 ;

-- --------------------------------------------------------

--
-- Table structure for table `care_customer`
--

CREATE TABLE IF NOT EXISTS `care_customer` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `name_employee` varchar(255) NOT NULL COMMENT 'Tên nhân viên chăm sóc khách hành',
  `user_id` int(11) NOT NULL COMMENT 'Mã Id của khách hàng',
  `user_name` varchar(255) NOT NULL COMMENT 'Tên khách hàng được chăm soc',
  `time_start` varchar(255) NOT NULL COMMENT 'Luc chăm soc khách hàng',
  `duration` varchar(255) NOT NULL COMMENT 'Thoi gian cham soc khach hang',
  `content` text NOT NULL COMMENT 'Nội dung chăm sóc khách hàng',
  `time_created` int(11) NOT NULL,
  PRIMARY KEY (`id`)
) ENGINE=MyISAM  DEFAULT CHARSET=latin1 AUTO_INCREMENT=610 ;

-- --------------------------------------------------------

--
-- Table structure for table `categories`
--

CREATE TABLE IF NOT EXISTS `categories` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `title` varchar(255) COLLATE utf8_unicode_ci NOT NULL,
  `status` tinyint(3) NOT NULL,
  `parent_id` int(11) NOT NULL,
  `description` text COLLATE utf8_unicode_ci NOT NULL,
  PRIMARY KEY (`id`)
) ENGINE=MyISAM  DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci AUTO_INCREMENT=101 ;

