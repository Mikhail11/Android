--
-- Table structure for table `cm_featured_items`
--

CREATE TABLE IF NOT EXISTS `cm_featured_items` (
  `id` int(10) unsigned NOT NULL COMMENT 'id sản phẩm (trùng với bên CDT)',
  `id_module` int(10) unsigned NOT NULL COMMENT 'id module bên bảng cm_featured_modules',
  `title` varchar(255) COLLATE utf8_unicode_ci NOT NULL COMMENT 'có thể khác tên sp bên chợ',
  `price` float unsigned NOT NULL DEFAULT '0' COMMENT 'Giá trước khi khuyến mại',
  `price_promotion` float NOT NULL DEFAULT '0' COMMENT 'Giá sau khi khuyến mại',
  `status` tinyint(1) unsigned NOT NULL COMMENT '1: Hiển thị, 2: Ẩn',
  `image` varchar(255) COLLATE utf8_unicode_ci NOT NULL COMMENT 'có thể khác ảnh sp bên chợ',
  `admin_id` int(10) unsigned NOT NULL COMMENT 'id người cập nhật',
  `created` int(10) unsigned NOT NULL,
  `updated` int(10) unsigned NOT NULL,
  PRIMARY KEY (`id`,`id_module`),
  KEY `id_module` (`id_module`),
  KEY `created` (`created`),
  KEY `updated` (`updated`),
  KEY `price` (`price`),
  KEY `price_promotion` (`price_promotion`),
  KEY `id` (`id`)
) ENGINE=MyISAM  DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci;

