--
-- Table structure for table `stores`
--

CREATE TABLE IF NOT EXISTS `stores` (
  `id` int(11) NOT NULL AUTO_INCREMENT COMMENT 'Id của gian hàng',
  `name` varchar(100) COLLATE utf8_unicode_ci NOT NULL,
  `user_id` int(4) NOT NULL,
  `store_package_id` tinyint(4) NOT NULL DEFAULT '0',
  `level` varchar(50) COLLATE utf8_unicode_ci DEFAULT NULL,
  `free_theme_amount` int(2) NOT NULL DEFAULT '0' COMMENT 'số theme còn lại được sử dụng miễn phí',
  `theme_id` int(11) NOT NULL COMMENT 'Ma theme shop dang su dung',
  `time_create` int(11) NOT NULL COMMENT 'Thời gian khởi tạo gian hàng',
  `start_date_store_package` int(11) NOT NULL,
  `end_date_store_package` int(11) NOT NULL,
  `active` tinyint(1) NOT NULL DEFAULT '0' COMMENT 'Trang thái kích hoạt SHOP: 0- chưa kích hoạt, 1- đã kích hoạt',
  `time_active` int(11) NOT NULL COMMENT 'Thời gian kích hoạt',
  `status` tinyint(1) NOT NULL DEFAULT '1' COMMENT 'Trạng thái của gian hàng: 0 bị khóa, 1 hoạt động, 2 tạm dừng, 3 dung thu',
  `allow_ext_payment` tinyint(1) NOT NULL DEFAULT '0' COMMENT '0 la khong chap nhan, 1 chap nhan thanh toan vcoin',
  `signature` longtext COLLATE utf8_unicode_ci NOT NULL COMMENT 'chữ ký của chủ shop',
  `time_update` int(11) NOT NULL DEFAULT '0' COMMENT 'update các action(đăng bán, đăng tin , cập nhật cofig...) ',
  PRIMARY KEY (`id`)
) ENGINE=MyISAM  DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci COMMENT='Lưu danh sách các gian hàng	' AUTO_INCREMENT=13794 ;

