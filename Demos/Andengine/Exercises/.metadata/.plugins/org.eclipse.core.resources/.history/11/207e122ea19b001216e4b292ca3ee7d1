  `cronjob` bit(1) NOT NULL DEFAULT b'1' COMMENT '0: chua chay cronjob; 1: da chay cronjob',
  `cronjob1` tinyint(4) NOT NULL DEFAULT '1',
  `last_updated` datetime NOT NULL,
  `description` text,
  `checked` varchar(255) DEFAULT '[]',
  PRIMARY KEY (`user_id`)
) ENGINE=MyISAM DEFAULT CHARSET=latin1;

