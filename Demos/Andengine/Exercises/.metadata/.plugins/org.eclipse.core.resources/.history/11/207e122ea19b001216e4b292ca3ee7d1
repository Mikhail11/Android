  `privilege_id` int(4) NOT NULL,
  PRIMARY KEY (`id`),
  UNIQUE KEY `user_privilege` (`user_id`,`privilege_id`)
) ENGINE=MyISAM  DEFAULT CHARSET=latin1 AUTO_INCREMENT=11749 ;

