  `is_lock` tinyint(1) DEFAULT '0' COMMENT 'Trạng thái bật tắt',
  `type` tinyint(1) NOT NULL DEFAULT '0' COMMENT 'type:0 lập schedule thủ công, 1: auto',
  PRIMARY KEY (`id`),
