--
-- Table structure for table `point_activity`
--

CREATE TABLE IF NOT EXISTS `point_activity` (
  `id` int(4) NOT NULL AUTO_INCREMENT COMMENT 'id',
  `code` varchar(100) COLLATE utf8_unicode_ci NOT NULL COMMENT 'Mã hành vi',
  `name` varchar(255) COLLATE utf8_unicode_ci NOT NULL COMMENT 'Tên hành vi',
  `group_id` int(4) NOT NULL COMMENT 'Nhóm hành vi',
  `type` tinyint(1) NOT NULL COMMENT 'Loại hành vi',
  `description` varchar(255) COLLATE utf8_unicode_ci NOT NULL COMMENT 'Mô tả chính sách',
  `status` tinyint(1) NOT NULL COMMENT 'Trạng thái áp dụng',
  `bonus_point` double NOT NULL COMMENT 'Điểm thưởng',
  `bonus_point_first` double NOT NULL COMMENT 'Điểm thưởng lần đầu',
  `bonus_gold` double NOT NULL COMMENT 'Điểm thưởng vàng',
  `bonus_gold_first` double NOT NULL COMMENT 'Tiền thưởng lần đầu',
  `limit_point` varchar(100) COLLATE utf8_unicode_ci NOT NULL COMMENT 'Giới hạn điểm',
  `limit_gold` varchar(100) COLLATE utf8_unicode_ci NOT NULL COMMENT 'Giới hạn điểm vàng',
  PRIMARY KEY (`id`)
) ENGINE=MyISAM  DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci COMMENT='Các hành vi thưởng điểm' AUTO_INCREMENT=11 ;

-- --------------------------------------------------------

--
-- Table structure for table `point_activity_log`
--

CREATE TABLE IF NOT EXISTS `point_activity_log` (
  `id` int(4) NOT NULL AUTO_INCREMENT,
  `activity_code` varchar(45) COLLATE utf8_unicode_ci NOT NULL COMMENT 'Mã hành vi',
  `group_id` int(4) NOT NULL DEFAULT '0',
  `user_id` int(4) NOT NULL COMMENT 'Mã người dùng',
  `object_table` varchar(45) COLLATE utf8_unicode_ci DEFAULT NULL,
  `object_id` int(4) NOT NULL COMMENT 'Mã đối tượng',
  `object_title` varchar(255) COLLATE utf8_unicode_ci NOT NULL COMMENT 'Title đối tượng',
  `time` int(11) NOT NULL COMMENT 'Thời gian',
  `status` tinyint(1) NOT NULL COMMENT 'Trạng thái',
  `point` double NOT NULL COMMENT 'Điểm thưởng',
  `gold` double NOT NULL COMMENT 'Điểm vàng',
  `note` varchar(255) COLLATE utf8_unicode_ci NOT NULL COMMENT 'Ghi chú',
  PRIMARY KEY (`id`),
  KEY `search` (`activity_code`,`user_id`)
) ENGINE=MyISAM  DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci COMMENT='Lock history activity của người dùng' AUTO_INCREMENT=42 ;

-- --------------------------------------------------------

--
-- Table structure for table `point_group_activity`
--

CREATE TABLE IF NOT EXISTS `point_group_activity` (
  `id` int(4) NOT NULL AUTO_INCREMENT COMMENT 'id',
  `name` varchar(255) COLLATE utf8_unicode_ci NOT NULL COMMENT 'Tên nhóm',
  `code` varchar(25) COLLATE utf8_unicode_ci DEFAULT NULL,
  `time_created` int(11) DEFAULT NULL,
  PRIMARY KEY (`id`)
) ENGINE=MyISAM  DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci COMMENT='Danh sách nhóm hành vi' AUTO_INCREMENT=4 ;

-- --------------------------------------------------------

--
-- Table structure for table `point_static_user`
--

CREATE TABLE IF NOT EXISTS `point_static_user` (
  `id` int(4) NOT NULL AUTO_INCREMENT COMMENT 'id',
  `activity_id` int(4) NOT NULL COMMENT 'Mã hành vi',
  `user_id` int(4) NOT NULL COMMENT 'Mã người dùng',
  `point` double NOT NULL COMMENT 'Tổng số điểm',
  `gold` double NOT NULL COMMENT 'Tổng số Gold',
  PRIMARY KEY (`id`),
  KEY `activity_id` (`activity_id`),
  KEY `user_id` (`user_id`)
) ENGINE=MyISAM  DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci COMMENT='Lưu điểm kiếm được của user theo activity' AUTO_INCREMENT=5 ;

