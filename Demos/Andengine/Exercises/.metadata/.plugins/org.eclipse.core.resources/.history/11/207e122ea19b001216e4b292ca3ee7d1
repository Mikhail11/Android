-- --------------------------------------------------------

--
-- Table structure for table `user`
--

CREATE TABLE IF NOT EXISTS `user` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `user_name` varchar(60) COLLATE latin1_general_ci NOT NULL COMMENT 'Tài khoản đăng nhập',
  `full_name` varchar(100) CHARACTER SET latin1 NOT NULL COMMENT 'Tên đầy đủ',
  `email` varchar(50) COLLATE latin1_general_ci NOT NULL COMMENT 'Email',
  `gender` tinyint(1) DEFAULT '1' COMMENT 'Giới tính',
  `phone` varchar(20) CHARACTER SET latin1 DEFAULT NULL COMMENT 'Điện thoại',
  `mobile_phone` varchar(20) CHARACTER SET latin1 DEFAULT NULL COMMENT 'Di động',
  `verified_sms` tinyint(1) DEFAULT NULL COMMENT 'Trạng thái Verify SMS',
  `address_error` varchar(200) CHARACTER SET latin1 DEFAULT NULL,
  `active_email` tinyint(1) NOT NULL COMMENT 'Trạng thái Acitve Email',
  `user_type` tinyint(1) DEFAULT NULL COMMENT 'Kiểu user, Seller & Buyer',
  `user_suggest` tinyint(4) NOT NULL DEFAULT '0',
  `openid_status` tinyint(1) DEFAULT NULL COMMENT 'Trạng thái liên kết Yahoo & Gmail',
  `site_id` tinyint(1) DEFAULT NULL COMMENT 'Site đăng ký',
  `zone_id_bk` int(4) NOT NULL,
  `zone_id` int(11) DEFAULT '0' COMMENT 'Địa phương',
  `avatar` varchar(200) CHARACTER SET latin1 DEFAULT NULL COMMENT 'Ảnh đại diện',
  `display_info` tinyint(1) DEFAULT NULL COMMENT 'Public thông tin hay ko',
  `lock` tinyint(1) DEFAULT '0' COMMENT 'Có đang bị Lock hay ko',
  `is_staff` int(11) DEFAULT NULL COMMENT 'Có phải là Staff CĐT hay ko',
  `time_register` int(11) DEFAULT NULL COMMENT 'Thời gian đăng ký',
  `time_last_login` int(11) DEFAULT '0' COMMENT 'Đăng nhập lần cuối',
  `time_register_shop` int(11) DEFAULT NULL COMMENT 'Số sao',
  `star` tinyint(1) DEFAULT NULL COMMENT 'Điểm',
  `score` int(4) DEFAULT '0' COMMENT 'Tổng vote',
  `total_vote` int(4) DEFAULT '0' COMMENT 'Số đánh giá tốt',
  `good_vote` int(4) DEFAULT '0' COMMENT 'Số đánh giá tốt',
  `time_integrated_nl` int(11) NOT NULL COMMENT 'Thời điểm tích hợp NL',
  `store_id` int(11) DEFAULT NULL COMMENT 'Mã gian hàng',
  `slogan` varchar(255) CHARACTER SET latin1 NOT NULL COMMENT 'Status hiển thị của cá nhân',
  `check_code` varchar(255) CHARACTER SET latin1 DEFAULT NULL,
  PRIMARY KEY (`id`),
  UNIQUE KEY `user_name` (`user_name`),
  KEY `zone_id` (`zone_id`)
) ENGINE=MyISAM  DEFAULT CHARSET=latin1 COLLATE=latin1_general_ci PACK_KEYS=0 AUTO_INCREMENT=603999 ;

