  `description` varchar(100) COLLATE utf8_unicode_ci NOT NULL COMMENT 'mô tả chủ đề',
  `status` tinyint(1) NOT NULL COMMENT 'trạng thái chủ đề',
  `create_timed` int(10) unsigned NOT NULL COMMENT 'thời gian tạo chủ đề',
  `user_id` int(11) NOT NULL,
  PRIMARY KEY (`id`)
) ENGINE=MyISAM  DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci AUTO_INCREMENT=40 ;

