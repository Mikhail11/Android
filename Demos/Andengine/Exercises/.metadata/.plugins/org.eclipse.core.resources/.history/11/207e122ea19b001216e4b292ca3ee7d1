--
-- Table structure for table `log_list`
--

CREATE TABLE IF NOT EXISTS `log_list` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `time` int(11) DEFAULT NULL,
  `seller_id` int(11) DEFAULT NULL,
  `seller_name` varchar(200) COLLATE utf8_unicode_ci DEFAULT NULL,
  `category_id` int(11) DEFAULT NULL,
  `category_name` varchar(200) COLLATE utf8_unicode_ci DEFAULT NULL,
  `item_id` int(11) DEFAULT NULL,
  `item_name` varchar(200) COLLATE utf8_unicode_ci DEFAULT NULL,
  `item_total_quantity` int(11) DEFAULT NULL COMMENT 'SL mua',
  `item_total_value` int(11) DEFAULT NULL COMMENT 'Tổng giá mua',
  `item_shop_level` int(11) DEFAULT NULL COMMENT 'Level shop lúc mua',
  `item_type` int(11) DEFAULT NULL COMMENT 'đăng theo mặc cả (3), đấu giá (2), mua ngay(1)',
  PRIMARY KEY (`id`)
) ENGINE=MyISAM  DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci AUTO_INCREMENT=5 ;

-- --------------------------------------------------------

--
-- Table structure for table `log_login`
--

CREATE TABLE IF NOT EXISTS `log_login` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `user_id` varchar(200) DEFAULT NULL,
  `time` int(11) DEFAULT NULL,
  PRIMARY KEY (`id`)
) ENGINE=MyISAM DEFAULT CHARSET=latin1 AUTO_INCREMENT=1 ;

-- --------------------------------------------------------

--
-- Table structure for table `log_order`
--

CREATE TABLE IF NOT EXISTS `log_order` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `time` int(11) DEFAULT NULL,
  `user_id` varchar(100) COLLATE utf8_unicode_ci NOT NULL,
  `seller_id` int(11) DEFAULT NULL,
  `order_id` int(11) DEFAULT NULL,
  `payment_method` tinyint(1) NOT NULL COMMENT 'hinh thuc thanh toan qua ngan luong hay ko',
  `item_total_quantity` int(11) DEFAULT NULL COMMENT 'SL mua',
  `item_total_value` int(11) DEFAULT NULL COMMENT 'Tổng giá mua',
  `item_shop_level` int(11) DEFAULT NULL COMMENT 'Level shop lúc mua',
  `item_buying_type` int(11) DEFAULT NULL COMMENT 'mua theo mặc cả (3), đấu giá (2), mua ngay(1)',
  PRIMARY KEY (`id`)
) ENGINE=MyISAM  DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci AUTO_INCREMENT=1135616 ;

-- --------------------------------------------------------

--
-- Table structure for table `log_order_detail`
--

CREATE TABLE IF NOT EXISTS `log_order_detail` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `item_id` int(11) DEFAULT NULL,
  `seller_id` int(11) NOT NULL,
  `user_id` varchar(100) NOT NULL,
  `time` int(11) NOT NULL,
  `item_name` varchar(200) DEFAULT NULL,
  `category_id` int(11) DEFAULT NULL,
  `category_name` varchar(200) DEFAULT NULL,
  `order_id` int(11) DEFAULT NULL,
  `item_total_value` int(11) DEFAULT NULL COMMENT 'Tổng giá cho từng sản phẩm bao gồm cả phí vận chuyển',
  `item_num` int(11) NOT NULL DEFAULT '1' COMMENT 'Số lượng mua',
  `item_type` tinyint(4) NOT NULL DEFAULT '1' COMMENT 'Kiểu đăng bán sản phẩm',
  `item_shop_level` tinyint(1) NOT NULL DEFAULT '0',
  `order_type` tinyint(4) NOT NULL DEFAULT '1' COMMENT 'Kiểu hóa đơn',
  `payment_method` tinyint(1) NOT NULL COMMENT 'hinh thuc thanh toan qua ngan luong hay ko',
  PRIMARY KEY (`id`)
) ENGINE=MyISAM  DEFAULT CHARSET=latin1 AUTO_INCREMENT=1144152 ;

-- --------------------------------------------------------

--
-- Table structure for table `log_payNL`
--

CREATE TABLE IF NOT EXISTS `log_payNL` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `time` int(11) DEFAULT NULL,
  `payment_method` tinyint(1) NOT NULL COMMENT '0 thanh toan qua ngan luong, 1 la thanh toan qua pay@chodientu',
  `user_id` varchar(200) COLLATE utf8_unicode_ci NOT NULL,
  `seller_id` int(11) DEFAULT NULL,
  `order_id` int(11) DEFAULT NULL,
  `item_total_quantity` int(11) DEFAULT NULL COMMENT 'SL mua',
  `item_total_value` bigint(20) DEFAULT NULL COMMENT 'Tổng giá mua',
  `item_shop_level` tinyint(2) NOT NULL DEFAULT '0' COMMENT 'Level shop lúc mua',
  `item_buying_type` int(11) DEFAULT NULL COMMENT 'mua theo mặc cả (3), đấu giá (2), mua ngay(1)',
  PRIMARY KEY (`id`)
) ENGINE=MyISAM  DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci AUTO_INCREMENT=980 ;

-- --------------------------------------------------------

--
-- Table structure for table `log_payment_detail`
--

CREATE TABLE IF NOT EXISTS `log_payment_detail` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `item_id` int(11) DEFAULT NULL,
  `seller_id` int(4) NOT NULL,
  `user_id` varchar(200) NOT NULL COMMENT 'user_id hoac email cua nguoi mua',
  `item_name` varchar(200) DEFAULT NULL,
  `category_id` int(11) DEFAULT NULL,
  `order_id` int(11) DEFAULT NULL,
  `item_total_value` int(11) DEFAULT NULL COMMENT 'Tổng giá cho từng sản phẩm bao gồm cả phí vận chuyển',
  `item_num` int(11) NOT NULL DEFAULT '1' COMMENT 'Số lượng mua',
  `item_type` tinyint(1) NOT NULL DEFAULT '1' COMMENT 'Kiểu đăng sản phẩm',
  `order_type` tinyint(1) NOT NULL DEFAULT '1' COMMENT 'Kiểu hóa đơn',
  `is_refund` tinyint(1) NOT NULL,
  `item_shop_level` tinyint(1) NOT NULL,
  `time` int(11) DEFAULT NULL,
  `payment_method` tinyint(1) NOT NULL COMMENT '0 thanh toan qua ngan luong, 1 la thanh toan qua pay@chodientu',
  PRIMARY KEY (`id`)
) ENGINE=MyISAM  DEFAULT CHARSET=latin1 AUTO_INCREMENT=4716 ;

