--
-- Table structure for table `store_upgrade_request_themes`
--

CREATE TABLE IF NOT EXISTS `store_upgrade_request_themes` (
  `id` int(4) NOT NULL AUTO_INCREMENT,
  `store_upgrade_request_id` int(4) NOT NULL COMMENT 'Mã phiếu đăng ký',
  `package_code` varchar(11) COLLATE utf8_unicode_ci NOT NULL COMMENT 'ma cap shop',
  `store_id` int(4) NOT NULL COMMENT 'Mã gian hàng',
  `theme_id` int(4) NOT NULL COMMENT 'Mã chức năng or theme',
  `master_theme_id` int(11) DEFAULT NULL COMMENT 'id theme mau',
  `master_theme_name` varchar(200) COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'ten theme mau',
  `name` varchar(200) COLLATE utf8_unicode_ci NOT NULL COMMENT 'Tên theme',
  `title` varchar(255) COLLATE utf8_unicode_ci NOT NULL COMMENT 'Tên gói',
  `description` varchar(500) COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'Mô tả theme',
  `style_css` varchar(100) COLLATE utf8_unicode_ci NOT NULL DEFAULT 'default' COMMENT 'file style của theme',
  `price` varchar(50) COLLATE utf8_unicode_ci NOT NULL COMMENT 'giá của theme',
  `is_sole` tinyint(1) NOT NULL DEFAULT '0' COMMENT 'request mua theme độc quyền hay mua thường ,1 : độc quyền , 0: thường',
  PRIMARY KEY (`id`),
  KEY `store_upgrade_request_id` (`store_upgrade_request_id`),
  KEY `store_id` (`store_id`)
) ENGINE=MyISAM  DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci COMMENT='Lưu các chức năng yêu cầu Upgrade' AUTO_INCREMENT=472 ;

-- --------------------------------------------------------

--
-- Table structure for table `store_upgrade_requests`
--

CREATE TABLE IF NOT EXISTS `store_upgrade_requests` (
  `id` int(11) NOT NULL AUTO_INCREMENT COMMENT 'Mã hóa đơn',
  `payment_id` int(11) NOT NULL COMMENT 'Mã phiếu thanh toán',
  `user_id` int(11) NOT NULL DEFAULT '0' COMMENT 'Mã tài khoản',
  `user_name` varchar(255) COLLATE utf8_unicode_ci NOT NULL COMMENT 'Tài khoản',
  `user_fullname` varchar(255) COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'Tên đầy đủ người yêu cầu',
  `user_phone` varchar(45) COLLATE utf8_unicode_ci NOT NULL COMMENT 'Số điện thoại của tài khoản',
  `user_zone_id` int(4) NOT NULL COMMENT 'Mã khu vực của User',
  `owner_user_id` int(11) NOT NULL,
  `owner_user_name` varchar(255) COLLATE utf8_unicode_ci NOT NULL,
  `owner_user_fullname` varchar(255) COLLATE utf8_unicode_ci NOT NULL,
  `owner_user_phone` varchar(45) COLLATE utf8_unicode_ci NOT NULL,
  `store_id` int(11) NOT NULL DEFAULT '0' COMMENT 'Mã gian hàng',
  `store_package_id` int(6) NOT NULL COMMENT 'ma goi shop',
  `store_level` varchar(50) COLLATE utf8_unicode_ci NOT NULL COMMENT 'Ma muc shop dang ky',
  `url` varchar(255) COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'Tên gian hàng',
  `owner_name` varchar(255) COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'Tên của chủ gian hàng',
  `store_name` varchar(255) COLLATE utf8_unicode_ci NOT NULL COMMENT 'Tên của gian hàng',
  `store_phone` varchar(45) COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'Số điện thoại của gian hàng',
  `store_address` varchar(255) COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'Địa chỉ của gian hàng',
  `store_zone_id` int(4) DEFAULT NULL COMMENT 'Mã khu vực của gian hàng',
  `store_zone_name` varchar(255) COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'Tên khu vực của gian hàng',
  `contract_code` varchar(100) COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'Mã hợp đồng',
  `money` double NOT NULL DEFAULT '0' COMMENT 'Tổng số tiền',
  `money_promotional` double NOT NULL COMMENT 'So tien khuyen mai',
  `money_checkout` float NOT NULL,
  `payment_status` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'Trạng thái thanh toán',
  `payment_type` tinyint(1) NOT NULL DEFAULT '1' COMMENT 'Kiểu TT: 1 TT, 2 NL, 3 CK',
  `payment_time` int(11) NOT NULL COMMENT 'Thời gian thanh toán',
  `active_status` tinyint(1) NOT NULL DEFAULT '0' COMMENT 'Trạng thái kích hoạt',
  `active_user_name` varchar(30) COLLATE utf8_unicode_ci NOT NULL COMMENT 'Người active',
  `service_type` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'Kiểu nâng cấp: 1 gói, 2 plugin',
  `total_month` int(11) DEFAULT '0' COMMENT 'Số tháng của hợp đồng',
  `time_created` int(11) NOT NULL DEFAULT '0' COMMENT 'Thời gian khởi tạo',
  `time_active` int(11) NOT NULL DEFAULT '0' COMMENT 'Thời gian kích hoạt',
  `ip_address` varchar(20) COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'Địa chỉ Ip thực hiệu yêu cầu',
  `refer_code` varchar(255) COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'Mã NL hoặc HĐ tài chính',
  `account_username` varchar(255) COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'Tài khoản phụ trách',
  `sale_username` varchar(255) COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'Tài khoản người phụ trách',
  `office_id` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'Mã văn phòng của nhân viên',
  `note` tinytext COLLATE utf8_unicode_ci COMMENT 'Ghi chú',
  PRIMARY KEY (`id`,`store_id`,`user_id`)
) ENGINE=MyISAM  DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci COMMENT='Bảng lưu thông tin yêu cầu nâng cấp gian hàng' AUTO_INCREMENT=1264 ;

