-- --------------------------------------------------------

--
-- Table structure for table `promotion_campaign`
--

CREATE TABLE IF NOT EXISTS `promotion_campaign` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `campaign_code` varchar(50) COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'Mã sự kiện',
  `user_id` int(11) NOT NULL COMMENT 'User id của ng bán',
  `user_name` varchar(50) COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'username người bán',
  `admin_id` int(11) DEFAULT NULL COMMENT 'id của người duyệt',
  `campaign_name` varchar(200) COLLATE utf8_unicode_ci DEFAULT NULL,
  `campaign_desc` text COLLATE utf8_unicode_ci,
  `campaign_type` tinyint(1) NOT NULL DEFAULT '0' COMMENT '1:giam gia sp, 2: giam gia tren hd, 3:khac',
  `campaign_status` tinyint(2) NOT NULL DEFAULT '1' COMMENT '1:chưa bắt đầu, 2:đang diễn ra, 3:đã hoàn thành, 4:đã hủy',
  `min_order_value` double NOT NULL DEFAULT '0' COMMENT 'giá trị hóa đơn nhỏ nhất đc khuyến mãi (nếu có)',
  `promotion_value` double NOT NULL DEFAULT '0' COMMENT 'giá trị khuyến mãi, chỉ dùng cho KM Coupon',
  `promotion_unit` tinyint(1) NOT NULL DEFAULT '1' COMMENT '1:%, 2:vnd',
  `promotion_type` tinyint(1) NOT NULL DEFAULT '0' COMMENT '1:danh muc sp, 2:theo tung sp, 3:mien phi van chuyen, 4:toan bo sp//hiện tại chỉ dùng 1 và 2',
  `begin_time` int(11) NOT NULL DEFAULT '0',
  `end_time` int(11) NOT NULL DEFAULT '0',
  `created_time` int(11) NOT NULL DEFAULT '0',
  `changed_time` int(11) NOT NULL DEFAULT '0',
  `number_turn` int(5) NOT NULL DEFAULT '0' COMMENT 'Số lần sử dụng tối đa của coupon',
  `coupon_code` varchar(10) COLLATE utf8_unicode_ci DEFAULT NULL,
  `pmonth` tinyint(2) NOT NULL,
  PRIMARY KEY (`id`,`pmonth`),
  KEY `pmonth` (`pmonth`),
  KEY `campaign_type` (`campaign_type`),
  KEY `min_order_value` (`min_order_value`),
  KEY `promotion_type` (`promotion_type`),
  KEY `begin_time` (`begin_time`),
  KEY `end_time` (`end_time`),
  KEY `created_time` (`created_time`),
  KEY `changed_time` (`changed_time`),
  KEY `use_number` (`number_turn`),
  KEY `user_id` (`user_id`),
  KEY `campaign_status` (`campaign_status`)
) ENGINE=MyISAM  DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci COMMENT='Bảng lưu chiến dịch khuyến mãi của người bán'
/*!50100 PARTITION BY LIST (`pmonth`)
(PARTITION p1 VALUES IN (1) ENGINE = MyISAM,
 PARTITION p2 VALUES IN (2) ENGINE = MyISAM,
 PARTITION p3 VALUES IN (3) ENGINE = MyISAM,
 PARTITION p4 VALUES IN (4) ENGINE = MyISAM,
 PARTITION p5 VALUES IN (5) ENGINE = MyISAM,
 PARTITION p6 VALUES IN (6) ENGINE = MyISAM,
 PARTITION p7 VALUES IN (7) ENGINE = MyISAM,
 PARTITION p8 VALUES IN (8) ENGINE = MyISAM,
 PARTITION p9 VALUES IN (9) ENGINE = MyISAM,
 PARTITION p10 VALUES IN (10) ENGINE = MyISAM,
 PARTITION p11 VALUES IN (11) ENGINE = MyISAM,
 PARTITION p12 VALUES IN (12) ENGINE = MyISAM) */ AUTO_INCREMENT=83 ;

-- --------------------------------------------------------

--
-- Table structure for table `promotion_category`
--

CREATE TABLE IF NOT EXISTS `promotion_category` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `campaign_id` int(11) NOT NULL,
  `category_id` int(11) NOT NULL,
  `category_type` tinyint(1) NOT NULL COMMENT '1:danh muc cho, 2:danh muc shop',
  `category_id_parent` int(11) NOT NULL,
  `category_level` int(11) NOT NULL,
  `category_name` varchar(100) COLLATE utf8_unicode_ci NOT NULL,
  `category_url` varchar(150) COLLATE utf8_unicode_ci NOT NULL,
  `category_contain` int(11) NOT NULL COMMENT 'số item trong danh mục',
  `promotion_value` double NOT NULL,
  `promotion_unit` tinyint(1) NOT NULL DEFAULT '1' COMMENT '1:%, 2:vnd',
  `created_time` int(11) NOT NULL,
  `changed_time` int(11) NOT NULL,
  PRIMARY KEY (`id`,`campaign_id`),
  UNIQUE KEY `campaign_id_2` (`campaign_id`,`category_id`,`category_type`),
  KEY `campaign_id` (`campaign_id`),
  KEY `category_id` (`category_id`),
  KEY `category_type` (`category_type`),
  KEY `category_id_parent` (`category_id_parent`),
  KEY `category_level` (`category_level`),
  KEY `created_time` (`created_time`),
  KEY `changed_time` (`changed_time`)
) ENGINE=MyISAM  DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci COMMENT='Bảng lưu các danh mục được chọn khuyến mãi'
/*!50100 PARTITION BY HASH (`campaign_id`)
PARTITIONS 10 */ AUTO_INCREMENT=82 ;

