--
-- Table structure for table `user_privarcy`
--

CREATE TABLE IF NOT EXISTS `user_privarcy` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `user_id` int(11) NOT NULL COMMENT 'Mã người dùng',
  `privarcy_name` varchar(100) COLLATE utf8_unicode_ci NOT NULL COMMENT 'Tên chính sách',
  `privarcy_value` varchar(100) COLLATE utf8_unicode_ci NOT NULL COMMENT 'Giá trị',
  PRIMARY KEY (`id`)
) ENGINE=MyISAM DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci COMMENT='Cấu hình các chính sách riêng tư' AUTO_INCREMENT=1 ;

-- --------------------------------------------------------

--
-- Table structure for table `user_privilege`
