  `create_date` int(10) unsigned DEFAULT NULL COMMENT 'ngày khởi tạo',
  `status` tinyint(1) DEFAULT NULL COMMENT 'tình trạng hoạt động của user  : 1 : hoạt  động , 0 : không hoạt động',
  PRIMARY KEY (`id`),
  KEY `store_id` (`store_id`,`user_id`)
