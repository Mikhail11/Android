--
-- Table structure for table `item_like_track`
--

CREATE TABLE IF NOT EXISTS `item_like_track` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `item_id` int(10) unsigned DEFAULT NULL,
  `user_id` int(11) NOT NULL,
  `seller_id` int(11) NOT NULL,
  `action` tinyint(4) DEFAULT NULL COMMENT 'Kieu action la Theo doi hoac yeu thich (1 theo doi, 2 yeu thich, 3 ca yeu thich va theo doi )',
  `time_track` int(4) DEFAULT NULL,
  `time_like` int(10) unsigned NOT NULL,
  `delete` tinyint(4) DEFAULT NULL,
  PRIMARY KEY (`id`),
  KEY `item_id` (`item_id`,`action`),
  KEY `delete` (`delete`)
) ENGINE=MyISAM  DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci COMMENT='Bang lu sp yeu thich hoac luu theo doi cua nguoi nguoi dung' AUTO_INCREMENT=13953 ;

