-- --------------------------------------------------------

--
-- Table structure for table `log_shop_traffic`
--

CREATE TABLE IF NOT EXISTS `log_shop_traffic` (
  `estore_id` int(11) NOT NULL,
  `time` int(11) NOT NULL,
  `level` int(11) NOT NULL,
  `store_name` varchar(50) NOT NULL,
  `traffic` int(11) NOT NULL,
  `id` int(11) NOT NULL AUTO_INCREMENT,
  PRIMARY KEY (`id`),
  UNIQUE KEY `estore_id` (`estore_id`,`time`),
  KEY `time` (`time`)
) ENGINE=MyISAM DEFAULT CHARSET=latin1 AUTO_INCREMENT=1 ;

-- --------------------------------------------------------

--
-- Table structure for table `log_shop_traffic_bak`
--

CREATE TABLE IF NOT EXISTS `log_shop_traffic_bak` (
  `estore_id` int(11) NOT NULL,
  `date` date NOT NULL,
  `time` int(11) NOT NULL,
  `level` int(11) NOT NULL,
  `store_name` varchar(50) NOT NULL,
  `traffic` int(11) NOT NULL,
  `id` int(11) NOT NULL AUTO_INCREMENT,
  PRIMARY KEY (`id`),
  UNIQUE KEY `estore_id` (`estore_id`,`time`)
) ENGINE=MyISAM DEFAULT CHARSET=latin1 AUTO_INCREMENT=1 ;

-- --------------------------------------------------------

--
-- Table structure for table `log_shop_traffic_daily`
--

CREATE TABLE IF NOT EXISTS `log_shop_traffic_daily` (
  `estore_id` int(11) NOT NULL,
  `time` int(11) NOT NULL,
  `level` int(11) NOT NULL,
  `store_name` varchar(50) NOT NULL,
  `traffic` int(11) NOT NULL,
  `flag_update` tinyint(1) NOT NULL DEFAULT '0',
  `id` int(11) NOT NULL AUTO_INCREMENT,
  PRIMARY KEY (`id`),
  KEY `time` (`time`),
  KEY `estore_id` (`estore_id`)
) ENGINE=MyISAM DEFAULT CHARSET=latin1 AUTO_INCREMENT=1 ;

