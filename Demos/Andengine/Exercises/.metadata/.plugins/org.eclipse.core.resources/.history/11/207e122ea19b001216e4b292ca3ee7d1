  `price_order` double NOT NULL DEFAULT '0' COMMENT 'Gia dung de sap xep. Neu la USD = Gia*tygia',
  `price_buynow` double DEFAULT '0' COMMENT 'Gia mua ngay cua SP(Gia mua ngay cua SP mua ngay/Mac ca hoac Mua ngay cua dau gia)',
  `is_not_price` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'Lam co de day cac SP co gia = 0 xuong duoi cung',
  `condition` tinyint(4) DEFAULT '0' COMMENT 'Tinh trang san pham la cu, moi',
  `sell_zone_bk` int(10) unsigned DEFAULT '0' COMMENT 'Ma khu vuc tinh thanh cua nguoi dang ban',
  `sell_zone` int(11) DEFAULT '0' COMMENT 'Ma khu vuc tinh thanh cua nguoi dang ban',
  `type` tinyint(4) DEFAULT '0' COMMENT 'Kieu san pham dau gia / Mua ngay / Gian hang/ mac ca',
  `is_promotional` tinyint(4) DEFAULT '0' COMMENT 'Co khuyen mai',
  `estore_id` int(10) unsigned DEFAULT '0' COMMENT 'Ma GHCN',
  `shop_level` tinyint(4) DEFAULT '0' COMMENT 'cap do nguoi ban: 0 Thuong, >= 5 la ShopCN, < 5 la shop co ban',
  `time_for_return` tinyint(3) unsigned DEFAULT '0' COMMENT 'Cho phep tra lai hang trong x ngay',
  `warrenty` smallint(5) unsigned DEFAULT '0' COMMENT 'Thong tin bao hanh, nhap theo (theo so ngay bao hanh)',
  `payment_method` tinyint(4) DEFAULT '0' COMMENT 'Kieu chap nhan thanh toan( ngan luong/ truoc tiep)',
  `shipping_type` tinyint(4) DEFAULT '0' COMMENT 'Hinh thuc van chuyen: mien phi, co dinh, hang',
  `status` smallint(4) unsigned NOT NULL DEFAULT '1' COMMENT 'Tinh trang duyet san pham',
  `seller_score` smallint(6) DEFAULT '0' COMMENT 'Dien chi so cua seller, dung de tiinh item_score',
  `time_start` int(10) unsigned DEFAULT '0' COMMENT 'Thoi gian bat dau len san cua 1 san pham',
  `time_end` int(11) DEFAULT '0' COMMENT 'Thoi gian het han/ket thuc cua 1 san pham',
  `time_post` int(10) unsigned DEFAULT '0' COMMENT 'Thoi gian dang san pham',
  `time_update` int(10) unsigned DEFAULT '0' COMMENT 'Thoi gian cap nhat san pham',
  `item_score` int(5) unsigned NOT NULL COMMENT 'Diem chat luong san pham = SUM( is_nganluong+ free_shiping+return+warrenty+promotion+seller_score+seller_level+user_like)',
  `currency_id` tinyint(4) DEFAULT '1' COMMENT 'Ma don vi tien te(1- VND hoac 3 - USD)',
  `image_name` varchar(200) COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'Ten anh cua SP',
  `hit_count` smallint(5) unsigned NOT NULL COMMENT 'tong so luot xem',
  `total_like` smallint(5) unsigned NOT NULL COMMENT 'tong so luot uu thich',
  `total_track` smallint(5) unsigned NOT NULL COMMENT 'Tong so luot theo doi',
  `total_bid_bargain` mediumint(8) unsigned NOT NULL COMMENT 'tong so luot dat gia hoac mac ca',
  `total_order` smallint(5) unsigned NOT NULL COMMENT 'Tong so hoa don phat sinh',
  `total_comment` smallint(5) unsigned NOT NULL COMMENT 'Tong so comment cua 1 san pham',
  `site_product_id` varchar(255) COLLATE utf8_unicode_ci NOT NULL,
  `total_item` mediumint(8) unsigned NOT NULL COMMENT 'Tong so san pham',
