--
-- Table structure for table `order`
--

CREATE TABLE IF NOT EXISTS `order` (
  `id` int(11) unsigned NOT NULL AUTO_INCREMENT COMMENT 'id tự tăng',
  `order_type` tinyint(1) DEFAULT NULL COMMENT 'Loai hoa don. 1: Mua ngay, 2: YCĐB, 3: Đau gia, 4: MuaDB',
  `code` varchar(20) CHARACTER SET utf8 COLLATE utf8_unicode_ci DEFAULT NULL COMMENT 'Ma hoa don',
  `payment_status` tinyint(1) NOT NULL DEFAULT '0' COMMENT 'trang thái thanh toán. 1: Chua thanh toan, 2:Da thanh toan, 3:Da tra lai tien',
  `order_status` int(2) DEFAULT NULL COMMENT '-1 => ''Tất cả'',     0 => ''Không có vấn đề'',      1 => ''Vấn đề mua hàng - chưa được giải quyết'',     2 => ''Vấn đề hàng về - chưa được giải quyết'',     3 => ''Vấn đề đã được giải quyết''',
  `order_history` text COMMENT 'ghi chu van de',
  `cost` double DEFAULT NULL COMMENT 'Tong giá tri thuan hóa don. chua bao gom cac khoan phi',
  `vnd_total_cost` double DEFAULT NULL COMMENT 'Tong gia tri don hang theo Viet Nam Dong',
  `usd_total_cost` double DEFAULT NULL COMMENT 'Tong gia tri don hang theo Do La My',
  `total_checkout` double DEFAULT NULL COMMENT 'Tong gia tri thanh toan don hang. Sau khi bao gom ca cac khoan phi',
  `total_discount` double DEFAULT NULL COMMENT 'Tong gia tri duoc giam gia cua don hang',
  `exchange_rate` double DEFAULT NULL COMMENT 'Ty gia',
  `time_checkout` int(11) unsigned DEFAULT NULL COMMENT 'Thoi gian thanh toan',
  `time_created` int(11) unsigned DEFAULT NULL COMMENT 'Thoi gian tao hoa don',
  `time_pay_expire` int(11) unsigned DEFAULT NULL COMMENT 'Han thanh toán tinh theo ngay thang, gio, phut, giay',
  `token_nganluong` varchar(100) DEFAULT NULL COMMENT 'Mã token ngân lượng',
  `nganluong_id` int(11) unsigned DEFAULT NULL COMMENT 'Ma ngan luong ap dung cho hoa don',
  `refund_nganluong_id` int(11) DEFAULT NULL,
  `nganluong_type` tinyint(1) DEFAULT NULL COMMENT 'Kieu thanh toan tai ngan luong',
  `method_payment` int(2) DEFAULT NULL COMMENT 'Phương thức thanh toán: 1-Số dư ví NL; 2-Paypal; 3-ATM online; 4-ATM offline; 5-TT tại nhà; 6- TTVP; 7-Visa; 8-NH_OFFLINE',
  `payment_info` varchar(1000) DEFAULT NULL COMMENT 'Thông tin thanh toán',
  `deleted` tinyint(1) NOT NULL DEFAULT '0' COMMENT 'trang thai xoa. 0 khong xoa, 1 bi xoa',
  `ebay_item_status` tinyint(1) NOT NULL DEFAULT '1' COMMENT 'Trạng thai mua hàng ở ebay',
  `ebay_item_activity_status` tinyint(1) NOT NULL DEFAULT '1' COMMENT '1: đang năm ở, 2: đang di chuyển',
  `ebay_item_location_status` tinyint(1) NOT NULL DEFAULT '1' COMMENT '1:Người bán,2:Proxy3:Hải quan,4:VP ChợĐT,5:Người mua',
  `location_store` varchar(255) DEFAULT NULL,
  `buyer_username` varchar(200) DEFAULT NULL COMMENT 'Username người mua',
  `ga` tinyint(1) NOT NULL DEFAULT '0' COMMENT 'Cờ cập nhật mã GA',
  PRIMARY KEY (`id`),
  KEY `id` (`id`),
  KEY `order_type` (`order_type`,`time_created`,`time_pay_expire`),
  KEY `payment_status` (`payment_status`)
) ENGINE=MyISAM  DEFAULT CHARSET=latin1 AUTO_INCREMENT=45651 ;

-- --------------------------------------------------------

--
-- Table structure for table `order_shipment`
--

CREATE TABLE IF NOT EXISTS `order_shipment` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `order_id` varchar(100) NOT NULL COMMENT ' Mã hóa đơn ',
  `shipping_detination` tinyint(1) NOT NULL DEFAULT '3' COMMENT ' Hải quan',
  `custom_date` int(11) DEFAULT NULL COMMENT ' ngày làm thủ tục hải quan ',
  `item_num_hq` int(3) DEFAULT '1' COMMENT 'So luong lam thu tuc hai quan',
  `shipping_fee_vnvn` varchar(255) DEFAULT NULL COMMENT 'Phí vận chuyển nội địa VN-VN',
  `shipping_vnn_code` varchar(100) NOT NULL COMMENT 'Mã vận đơn vận chuyển nội địa VN - VN',
  `custom_price_usd` double DEFAULT NULL COMMENT ' giá trị sản phẩm (bằng USD) mà trên thực tế HQ áp cho SP này ',
  `custom_exch_rate` double DEFAULT NULL COMMENT ' tỷ giá tiền tệ mà trên thực tế HQ sử dụng để quy đổi từ USD sang VNĐ vào ngày làm thủ tục HQ ',
  `custom_tax_import` double DEFAULT NULL COMMENT ' thuế nhập khẩu mà trên thực tế HQ áp cho SP này ',
  `custom_tax_import_vnd` double NOT NULL COMMENT 'Thuế hải quan thực tiền VNĐ',
  `tracking_nummber` varchar(200) DEFAULT NULL COMMENT 'mã tracking kiện hàng tại Proxy',
  `custom_tax_vat` float DEFAULT NULL COMMENT '% thuế VAT mà trên thực tế HQ áp cho SP này ',
  `custom_tax_vat_vnd` double NOT NULL COMMENT 'Thuế Vat hải quan áp thực tế tiền VNĐ',
  `custom_tax_other` double DEFAULT NULL COMMENT ' thuế khác mà trên thực tế HQ áp cho SP này ',
  `custom_tax_paid` double DEFAULT NULL COMMENT ' Tổng thuế hải quan áp = (custom_tax_import cộng custom_tax_other) * custom_price_usd * custom_exch_rate * custom_tax_vat ',
  `custom_info` varchar(255) DEFAULT NULL COMMENT ' thông tin thực tế v/v dealing với HQ về SP này ',
  `waybill_number` varchar(200) DEFAULT NULL COMMENT 'Mã kiện hàng',
  `position` int(3) DEFAULT NULL COMMENT 'Vị trí của hóa đơn trong kiện',
  `custom_status` tinyint(1) DEFAULT NULL COMMENT ' giá trị thể hiện hải quan ko cho nhập, fải tái xuất. 1 Cho nhập, 2 Tái xuất ',
  `user_carrier` varchar(255) DEFAULT NULL COMMENT ' HÌnh thức vận chuyển tại Proxy ',
  `date_shipped` int(11) DEFAULT NULL COMMENT ' Ngày gói hàng tại Proxy ',
  `expected_receive_date` int(11) DEFAULT NULL COMMENT ' Ngày dự kiến hàng về',
  `item_activity_history` text NOT NULL,
  `cskh_status` int(1) NOT NULL COMMENT '0: Chưa chăm sóc; 1: Đã đc chăm sóc',
  `cskh_history` text COMMENT 'Ghi chú chăm sóc',
  `time_tele_sales` int(11) DEFAULT NULL COMMENT 'Thời gian Tele-Sales',
  `history_tele_sales` text COMMENT 'Lịch sử Tele-Sales',
  `username_tele_sales` varchar(25) DEFAULT NULL COMMENT 'User Tele-sales',
  PRIMARY KEY (`id`)
) ENGINE=MyISAM  DEFAULT CHARSET=latin1 AUTO_INCREMENT=28265 ;

-- --------------------------------------------------------

--
-- Table structure for table `order_transaction`
--

CREATE TABLE IF NOT EXISTS `order_transaction` (
  `id` bigint(20) NOT NULL AUTO_INCREMENT,
  `order_id` varchar(100) NOT NULL COMMENT ' Mã hóa đơn ',
  `item_id` varchar(200) DEFAULT NULL COMMENT ' Mã sản phẩm ',
  `item_name` varchar(255) DEFAULT NULL COMMENT ' Tên sản phẩm ',
  `item_name_vn` varchar(255) DEFAULT NULL,
  `item_category` int(11) DEFAULT NULL COMMENT ' Danh mục sản phẩm ',
  `item_category_name` varchar(255) DEFAULT NULL COMMENT ' Tên danh mục của sản phẩm ',
  `hs_code` varchar(100) DEFAULT NULL COMMENT 'Mã HS danh mục',
  `item_image` varchar(255) DEFAULT NULL COMMENT ' Ảnh sản phẩm ',
  `item_num` int(11) NOT NULL DEFAULT '1' COMMENT ' Số lượng sản phẩm ',
  `item_condition` varchar(255) DEFAULT NULL COMMENT 'tình trạng SP: Cũ mới',
  `item_weight` float DEFAULT NULL COMMENT ' Khối lượng sản phẩm (Lbs) ',
  `item_size` varchar(255) DEFAULT NULL COMMENT 'Kích cỡ',
  `item_color` varchar(255) DEFAULT NULL COMMENT 'Màu sắc',
  `item_shade` varchar(200) DEFAULT NULL,
  `item_sku` varchar(200) DEFAULT NULL,
  `paypal_payment_status` tinyint(4) NOT NULL DEFAULT '1' COMMENT ' Trạng thái thanh toán cho PayPal',
  `paypal_id` varchar(200) DEFAULT NULL COMMENT ' Mã Transaction của PayPal ',
  `ebay_transaction_id` varchar(200) DEFAULT NULL COMMENT ' Transaction của sản phẩm tại ebaycom ',
  `buyer_percentage_fee` float DEFAULT '0' COMMENT ' % Phí Người mua phải trả ',
  `buyer_fees` float DEFAULT '0' COMMENT ' Tổng phí người mua phải trả ',
  `shipping_fee_usus` float DEFAULT '0' COMMENT ' phí chuyển hàng trong từ người Bán đến Proxy (bên Mỹ) ',
  `shipping_fee_usus_type` varchar(100) DEFAULT NULL COMMENT ' Hình thức vận chuyển hàng US - US ',
  `shipping_fee_usvn` float DEFAULT NULL,
  `us_statustax` float DEFAULT NULL COMMENT ' % Thuế bang US - US ',
  `us_statustax_usd` float DEFAULT NULL COMMENT ' Số tiền thuế bang US - US ',
  `auction_fee` float DEFAULT NULL COMMENT ' Phí đấu giá ',
  `buyer_tax_import` float DEFAULT NULL COMMENT ' Thuế nhập khẩu người mua trả cho CĐT ',
  `buyer_tax_vat` float NOT NULL COMMENT ' thuế VAT người Mua trả cho CĐT ',
  `buyer_tax_other` float NOT NULL COMMENT ' thuế khác người Mua trả cho CĐT ',
  `buyer_tax_paid` float NOT NULL COMMENT ' Tổng số tiền thuế tạm tính mà người mua phải trả',
  `buyer_fee_other` float DEFAULT NULL COMMENT ' Phí thu thêm của người mua. Trường hợp hàng về nhưng phí quá cao phải thu thêm khách hàng ',
  `pay_plus` tinyint(1) NOT NULL DEFAULT '0' COMMENT 'Thu thêm: 1- Phải thu thêm; 0- Ko phải thu thêm',
  `token_nganluong_other` varchar(100) DEFAULT NULL COMMENT 'Mã token ngân lượng khi thu thêm',
  `buyer_fee_other_nganluongid` int(11) DEFAULT NULL COMMENT ' Mã NgânLượng thu thêm ',
  `buyer_fee_other_nganluongid_refund` int(11) DEFAULT NULL COMMENT ' Mã NgânLượng thu thêm nhưng trả lại tiền ',
  `buyer_fee_other_status` tinyint(4) NOT NULL DEFAULT '1' COMMENT ' Trạng thái phí thu thêm. 1 Chưa thu, 2 Đã thu, 3 Trả lại tiền ',
  `buyer_fee_other_reason` varchar(255) DEFAULT NULL COMMENT ' Lý do thu thêm ',
  `buyer_fee_other_history` text COMMENT ' Lịch sử lý do thu thêm ',
  `total_cost_usd_paypal` double DEFAULT NULL COMMENT 'Giá trị sản phẩm CĐT thanh toán cho PayPal',
  `buyer_fees_paypal` decimal(10,0) DEFAULT NULL COMMENT 'Phí Vận chuyển US - US CĐT thanh toán cho PayPal',
  `buyer_fee_usvn_paypal` double DEFAULT NULL COMMENT 'Phí vận chuyển từ Mỹ về VN mà CĐT phải trả',
  `exchange_paypal` float NOT NULL COMMENT 'Tỷ giá Paypal áp',
  `promotion_type` tinyint(4) DEFAULT NULL COMMENT ' 1 SundayOff 2 SpecialDay 3 LuckyDay, 4 Coupon ',
  `promotion_number` varchar(100) DEFAULT NULL COMMENT ' Mã coupon nếu chiến dịch KM là coupon, hoặc mã Chiến dịch ',
  `promotion_money` float DEFAULT NULL COMMENT ' Số tiền KM theo chiến dịch',
  `nl_payment_history` text CHARACTER SET utf8 NOT NULL COMMENT 'Lịch sử thanh toán NL',
  `ebay_buy_history` text CHARACTER SET utf8 NOT NULL COMMENT 'Lịch sử mua hàng ebay.com',
  `paypal_payment_history` text CHARACTER SET utf8 NOT NULL COMMENT 'Lịch sử thanh toán paypal',
  `method_payment_other` int(2) DEFAULT NULL COMMENT 'Phương thức thanh toán: 1-Số dư ví NL; 2-Paypal; 3-ATM online; 4-ATM offline; 5-TT tại nhà; 6- TTVP; 7-Visa; 8-NH_OFFLINE',
  `payment_info_other` varchar(1000) DEFAULT NULL COMMENT 'Thông tin thanh toán thu thêm (ngân hàng, địa chỉ.....)',
  `buyer_other_code` varchar(20) DEFAULT NULL COMMENT 'Mã phí thu thêm',
  `buyer_other_time_checkout` int(11) DEFAULT NULL,
  `buyer_email_paypal` varchar(255) DEFAULT NULL,
  PRIMARY KEY (`id`)
) ENGINE=MyISAM  DEFAULT CHARSET=latin1 AUTO_INCREMENT=40106 ;

