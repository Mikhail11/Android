  `allow_reply` tinyint(1) NOT NULL DEFAULT '1' COMMENT 'Cho phép reply hay không',
  `delete_status` tinyint(4) NOT NULL COMMENT '1: Xóa bởi người gửi, 2: Xóa bởi người nhận, 3: Xóa bởi cả gửi lẫn nhận',
  PRIMARY KEY (`id`),
