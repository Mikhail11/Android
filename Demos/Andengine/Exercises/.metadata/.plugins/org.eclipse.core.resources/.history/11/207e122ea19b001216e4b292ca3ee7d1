-- --------------------------------------------------------

--
-- Table structure for table `category_history_post`
--

CREATE TABLE IF NOT EXISTS `category_history_post` (
  `id` int(10) unsigned NOT NULL AUTO_INCREMENT,
  `user_id` int(10) unsigned DEFAULT NULL COMMENT 'Ma di cua seller',
  `category_id` int(11) DEFAULT NULL COMMENT 'Ma danh muc cap la da duoc dang',
  `time` int(10) unsigned DEFAULT NULL COMMENT 'Thoi gian dang',
  PRIMARY KEY (`id`),
  KEY `user_id` (`user_id`)
) ENGINE=MyISAM  DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci COMMENT='Bang luu lich su cac danh muc da dang cua 1 seller\r\n' AUTO_INCREMENT=44397 ;

